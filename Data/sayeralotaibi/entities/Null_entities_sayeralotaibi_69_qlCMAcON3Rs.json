"  \"Entities\": [    {      \"name\": \"نواف\",      \"original_text\": \"Nuwaf\",      \"type\": \"PERSON\",      \"description\": \"ابن خليل الرواف الذي فُقد في أمريكا، عاش حياة مليئة بالتغيرات، بدءاً من تغيير اسمه إلى المشاركة في الحرب الفيتنامية. \"    },    {      \"name\": \"خليل الرواف\",      \"original_text\": \"Khalil Al-Rawaf\",      \"type\": \"PERSON\",      \"description\": \"رجل سعودي من القصيم، سافر إلى أمريكا وفقد ابنه نواف هناك، قضى سنوات طويلة في البحث عنه حتى جمعهم القدر أخيراً.\"    },    {      \"name\": \"أمريكا\",      \"original_text\": \"America\",      \"type\": \"LOCATION\",      \"description\": \"الدولة التي سافر إليها خليل الرواف وفقد ابنه فيها، وأيضاً المكان الذي عاش فيه نواف بعد انفصال والديه.\"    },    {      \"name\": \"الحرب الفيتنامية\",      \"original_text\": \"Vietnam War\",      \"type\": \"EVENT\",      \"description\": \"حرب شارك فيها نواف (كلايف) بعد تجنيده في الجيش الأمريكي، وهي حرب معروفة بقسوتها وعدد الضحايا الكبير.\"    },    {      \"name\": \"مصر\",      \"original_text\": \"Egypt\",      \"type\": \"LOCATION\",      \"description\": \"الدولة التي سافر إليها خليل الرواف بعد انفصاله عن زوجته، وهناك التقى بزوجته الثانية.\"    },    {      \"name\": \"فندق سميراميس\",      \"original_text\": \"Semiramis Hotel\",      \"type\": \"LOCATION\",      \"description\": \"فندق في مصر أقام فيه خليل الرواف، وهناك التقى بزوجته الثانية.\"    },    {      \"name\": \"الرياض\",      \"original_text\": \"Riyadh\",      \"type\": \"LOCATION\",      \"description\": \"مدينة في السعودية، عاش فيها خليل الرواف بعد عودته من أمريكا.\"    },    {      \"name\": \"الأمير طلال بن عبد العزيز\",      \"original_text\": \"Prince Talal bin Abdulaziz\",      \"type\": \"PERSON\",      \"description\": \"أمير سعودي، سافر خليل الرواف معه إلى أمريكا.\"    },    {      \"name\": \"كونستانس\",      \"original_text\": \"Constance\",      \"type\": \"PERSON\",      \"description\": \"زوجة خليل الرواف الأولى، وأم نواف، انفصلت عن خليل وتزوجت رجلين آخرين.\"