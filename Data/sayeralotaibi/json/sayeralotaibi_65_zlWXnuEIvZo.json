        },
        {
            "text": "شفنا مقهى كانوا جالسين فيه ضباط اتراك",
            "start": 441.96,
            "duration": 6.84
        },
        {
            "text": "فابلغ ان قطاع الطرق قتلوا زميلهم لكن",
            "start": 445.02,
            "duration": 6.06
        },
        {
            "text": "كانت رده فعلهم ان جمعوا البيانات",
            "start": 448.8,
            "duration": 5.82
        },
        {
            "text": "والشكاوي فقط وكانها ظاهره عاديه تحصل كل",
            "start": 451.08,
            "duration": 6.54
        },
        {
            "text": "يوم فيقول عبد العزيز رغم ان حادثه القتل",
            "start": 454.62,
            "duration": 6.12
        },
        {
            "text": "كانت قريبه منهم الا انه كان دورهم انه",
            "start": 457.62,
            "duration": 6.079
        },
        {
            "text": "تاسفوا على القتلى والمسلوبين وبالمقابل",
            "start": 460.74,
            "duration": 6.899
        },
        {
            "text": "شتموا قطاع الطرق وكان سلوكهم انه تجادلوا",
            "start": 463.699,
            "duration": 7.301
        },
        {
            "text": "فيما بينهم بمن ياتي بجثه الجنود القتله",
            "start": 467.639,
            "duration": 7.62
        },
        {
            "text": "طبعا عبد العزيز لما وصل مكه ما نسى مهمته",
            "start": 471.0,
            "duration": 6.66
        },
        {
            "text": "اللي هي مراقبه المسلمين الروس في مكه",
            "start": 475.259,
            "duration": 5.401
        },
        {
            "text": "فيقول الروس الحجاج كانوا عباره عن ثلاثه",
            "start": 477.66,
            "duration": 6.3
        },
        {
            "text": "الاول القادمين من القفقاس والثاني",
            "start": 480.66,
            "duration": 6.659
        },
        {
            "text": "القادمين من القرن والثالث البقيه من",
            "start": 483.96,
            "duration": 6.12
        },
        {
            "text": "السوفيت يقول عبد العزيز انصدمت من اقامه",
            "start": 487.319,
            "duration": 6.961
        },
        {
            "text": "حجاجنا في مكه فكانت اقامه مجانيه فتوجد",
            "start": 490.08,
            "duration": 7.5
        },
        {
            "text": "ثمانيه بيوت من الطراز المحلي اشتروها",
            "start": 494.28,
            "duration": 7.08
        },
        {
            "text": "مسلمون وقدموها هديه للاوقاف فكان احد من",
            "start": 497.58,
            "duration": 6.78
        },
        {
            "text": "مواطنين الروس استقر للابد في مكه المكرمه",
            "start": 501.36,
            "duration": 6.66
        },
        {
            "text": "وكان دوره ان يشرف على هذه البيوت يقول",
            "start": 504.36,
            "duration": 6.36
        },
        {
            "text": "عبد العزيز التقيت بالحجاج الروس اللي",
            "start": 508.02,
            "duration": 5.459
        },
        {
            "text": "يعتبرون من الطبقه الغنيه فالرغم امتلاكهم",
            "start": 510.72,
            "duration": 5.939
        },
        {
            "text": "مبالغ كبيره من الاموال الا انهم يصرون",
            "start": 513.479,
            "duration": 6.42
        },
        {
            "text": "على العيش بابسط وسائل الراحه والرفاهيه",
            "start": 516.659,
            "duration": 6.541
        },
        {
            "text": "فكانوا يؤمنون ان تجربه الحج لازم تكون",
            "start": 519.899,
            "duration": 6.721
        },
        {
            "text": "بالشكل المتواضع قبل لا اتكلم لكم عن رحله",
            "start": 523.2,
            "duration": 5.46
        },
        {
            "text": "الحج العجيبه اللي خاضها عبد العزيز",
            "start": 526.62,
            "duration": 5.88
        },
        {
            "text": "دولشين مع الحجاج الروس ودي اتكلم لكم عن",
            "start": 528.66,
            "duration": 6.119
        },
        {
            "text": "الظواهر الغريبه اللي شافها عبد العزيز",
            "start": 532.5,
            "duration": 5.519
        },
        {
            "text": "اثناء تواجدهم في المدينه المنوره فيقول",
            "start": 534.779,
            "duration": 5.24
        },
        {
            "text": "عبد العزيز واحنا في المدينه المنوره",
            "start": 538.019,
            "duration": 5.701
        },
        {
            "text": "شاهدت المسلمين يسجدون امام قبر النبي",
            "start": 540.019,
            "duration": 6.88
        },
        {
            "text": "محمد صلى الله عليه وسلم فيقول كنت اسمع",
            "start": 543.72,
            "duration": 6.54
        },
        {
            "text": "من المسلمين ان قبر النبي محمد يعتبر من",
            "start": 546.899,
            "duration": 6.541
        },
        {
            "text": "اهم واكبر الاثار عند المسلمين وبنفس",
            "start": 550.26,
            "duration": 5.699
        },
        {
            "text": "الوقت كنت اسمع من المسلمين الاخرين ان",
            "start": 553.44,
            "duration": 6.12
        },
        {
            "text": "السجود ليس فريضه على المسلمين ولا يمت",
            "start": 555.959,
            "duration": 6.601
        },
        {
            "text": "بالحج بصله لكن كان هناك عدد كبير من",
            "start": 559.56,
            "duration": 6.719
        },
        {
            "text": "الحجاج القادمين من الحجاز يسجدون من قبر",
            "start": 562.56,
            "duration": 6.66
        },
        {
            "text": "النبي فيقول شاهد ثلاث قبور قبر النبي",
            "start": 566.279,
            "duration": 5.821
        },
        {
            "text": "محمد صلى الله عليه وسلم والخليفتين ابو",
            "start": 569.22,
            "duration": 6.36
        },
        {
            "text": "بكر وعمر فالقبور كانت مغطاه بغطاء حريري",
            "start": 572.1,
            "duration": 7.5
        },
        {
            "text": "اخضر اللون مطرز بالاحجار الكريمه والخيوط",
            "start": 575.58,
            "duration": 7.14
        },
        {
            "text": "الذهبيه من الجانب الشمالي يلتصق بالحجره",
            "start": 579.6,
            "duration": 6.72
        },
        {
            "text": "الشريفه قبر ابنه النبي فاطمه وفي ساحه",
            "start": 582.72,
            "duration": 7.2
        },
        {
            "text": "صغيره وسط الحرم توجد روضه صغيره وفيها",
            "start": 586.32,
            "duration": 6.6
        },
        {
            "text": "بار فاطمه على العموم نرجع الى رحله الحج",
            "start": 589.92,
            "duration": 7.08
        },
        {
            "text": "يقول عبد العزيز في عام 1898 كانت الرحله",
            "start": 592.92,
            "duration": 7.5
        },
        {
            "text": "الى عرفات تعتبر من ارهب الرحلات بالنسبه",
            "start": 597.0,
            "duration": 6.839
        },
        {
            "text": "للحجاج فالذكريات والاقاسيس اللي سمعوها",
            "start": 600.42,
            "duration": 6.0
        },
        {
            "text": "من الحجاج السابقين عن او بيئه الكوليرا",
            "start": 603.839,
            "duration": 6.0
        },
        {
            "text": "الرهيبه تثير الرعب في قلوبهم فكانوا",
            "start": 606.42,
            "duration": 6.12
        },
        {
            "text": "يدعون ربهم انه يعودون السالمين من عرفات",
            "start": 609.839,
            "duration": 5.461
        },
        {
            "text": "الى درجه ان اسمعوا المسلمين من حولي",
            "start": 612.54,
            "duration": 6.44
        },
        {
            "text": "اثناء توجهنا الى عرفات يوصلون بعضهم ببعض",
            "start": 615.3,
            "duration": 7.26
        },
        {
            "text": "بكيفيه التصرف بالعمال اثناء حاله الوفاه",
            "start": 618.98,
            "duration": 7.18
        },
        {
            "text": "اثناء الطريق الى عرفات ترى الحجاج مره",
            "start": 622.56,
            "duration": 7.5
        },
        {
            "text": "كنت كتله بشريه واحده لابسين الاحرام بلون",
            "start": 626.16,
            "duration": 7.32
        },
        {
            "text": "الابيض فترى المنظر الرهيب الحجاج يركبون",
            "start": 630.06,
            "duration": 6.6
        },
        {
            "text": "على القوافل او على الحمير او يسيرون مشيا",
            "start": 633.48,
            "duration": 5.88
        },
        {
            "text": "على الاقدام وترى العساكر المرابطين في",
            "start": 636.66,
            "duration": 6.78
        },
        {
            "text": "مكه ومعاهم الوالي وكذلك الشريف وايضا",
            "start": 639.36,
            "duration": 5.78
        },
        {
            "text": "جميع سكان المدينه",
            "start": 643.44,
            "duration": 5.399
        },
        {
            "text": "يتركونها ويغادرون الى عرفات هذا النزوح",
            "start": 645.14,
            "duration": 7.0
        },
        {
            "text": "كان عباره عن مشهد عظيم لم اراه في حياتي",
            "start": 648.839,
            "duration": 6.301
        },
        {
            "text": "الحجاج كان اغلبهم يجيبون معاهم من مكه",
            "start": 652.14,
            "duration": 6.0
        },
        {
            "text": "خيام خاصه فيهم والبعض الحجاج الاخرين",
            "start": 655.14,
            "duration": 6.48
        },
        {
            "text": "يصنعون لانفسهم ظل فكانوا يغرسون العصا في",
            "start": 658.14,
            "duration": 6.24
        },
        {
            "text": "الارض ويمدون قطعه من الخيش بوك والفئه",
            "start": 661.62,
            "duration": 5.459
        },
        {
            "text": "المعدمه كانت تاوي قرب المسجد فكانوا",
            "start": 664.38,
            "duration": 5.34
        },
        {
            "text": "يحتمون من الشمس من خلال الجلوس تحت",
            "start": 667.079,
            "duration": 5.641
        },
        {
            "text": "الصخور والاشجار يقول عبد العزيز بعد ما",
            "start": 669.72,
            "duration": 6.239
        },
        {
            "text": "اداوي الحجاج من مزدلفه ومنى وعادوا الى",
            "start": 672.72,
            "duration": 5.88
        },
        {
            "text": "مكه على طول يصرعون الحجاج الى اعلام",
            "start": 675.959,
            "duration": 5.761
        },
        {
            "text": "اقاربهم في الوطن بسلامه الرحله الى عرفات",
            "start": 678.6,
            "duration": 7.56
        },
        {
            "text": "والان يستعدون الحجاج للرحيل الهاكر رح في",
            "start": 681.72,
            "duration": 7.799
        },
        {
            "text": "العوده الى الوطن تختلف عن عوده رحله حجاج",
            "start": 686.16,
            "duration": 5.94
        },
        {
            "text": "اليوم الحجاج في ذاك الوقت لما يستعدون",
            "start": 689.519,
            "duration": 5.341
        },
        {
            "text": "للرحيل كانوا مثل اليوم يجهزون بعض",
            "start": 692.1,
            "duration": 5.88
        },
        {
            "text": "الهدايا لاقاربهم بسبب احوالهم الماليه",
            "start": 694.86,
            "duration": 6.84
        },
        {
            "text": "كانوا يجيبون كميات متفاوته من ماء زمزم",
            "start": 697.98,
            "duration": 6.479
        },
        {
            "text": "وكذلك التمر المحلي وايضا كانوا يشترون",
            "start": 701.7,
            "duration": 5.639
        },
        {
            "text": "مسابيح من الصنع المحلي او من صنع",
            "start": 704.459,
            "duration": 6.901
        },
        {
            "text": "القسطنطينيه وخواتم فضيه مصنوعه من بقايا",
            "start": 707.339,
            "duration": 6.961
        },
        {
            "text": "الزخارف اللي تتغير كل سنه في مقام",
            "start": 711.36,
            "duration": 5.219
        },
        {
            "text": "ابراهيم لكن كانت اغرب الهدايا في تلك",
            "start": 714.3,
            "duration": 5.64
        },
        {
            "text": "الفتره انهم كانوا يشترون قطعه من الخيش",
            "start": 716.579,
            "duration": 7.921
        },
        {
            "text": "مبلله بماء زمزم عشان يستخدمونها للاكفان",
            "start": 719.94,
            "duration": 7.32
        },
        {
            "text": "كانوا يجمعون هذه الاشياء ويدسونها في",
            "start": 724.5,
            "duration": 6.0
        },
        {
            "text": "الصناديق ويسلمونها لسه ماسره عشان توصل",
            "start": 727.26,
            "duration": 6.06
        },
        {
            "text": "لهم الى القسطنطينيه وبالنسبه لرحله",
            "start": 730.5,
            "duration": 5.76
        },
        {
            "text": "العوده الى روسيا كانت رحله جدا عجيبه",
            "start": 733.32,
            "duration": 6.0
        },
        {
            "text": "فكانوا يمرون الحجاج الروس الى بيروت",
            "start": 736.26,
            "duration": 6.36
        },
        {
            "text": "وبعدها يتوجهون الى القدس ومن القدس كانوا",
            "start": 739.32,
            "duration": 7.56
        },
        {
            "text": "يروحون الى دمشق ويزورون مدفن يحيى اللي",
            "start": 742.62,
            "duration": 6.959
        },
        {
            "text": "موجود في الجامع الاموي وبعدها يزورون",
            "start": 746.88,
            "duration": 6.36
        },
        {
            "text": "القاهره وراسا يروحون الى مدفن حفيد النبي",
            "start": 749.579,
            "duration": 6.601
        },
        {
            "text": "صلى الله عليه وسلم اللي هو الحسين رضي",
            "start": 753.24,
            "duration": 5.58
        },
        {
            "text": "الله عنه وبعده يزورون مدفن الائمه",
            "start": 756.18,
            "duration": 6.0
        },
        {
            "text": "الاربعه ومنهم الشافعي وبعدها في طريق",
            "start": 758.82,
            "duration": 6.84
        },
        {
            "text": "العوده يوصلون الحجاج الى القسطنطينيه وهي",
            "start": 762.18,
            "duration": 6.839
        },
        {
            "text": "تعتبر النقطه الاخيره من الرحله فمن هناك",
            "start": 765.66,
            "duration": 6.78
        },
        {
            "text": "يشترون تذاكر السفر اللي تدخلهم الى روسيا",
            "start": 769.019,
            "duration": 6.481
        },
        {
            "text": "طبعا الحجاج الروسي العائدين الى اقربائهم",
            "start": 772.44,
            "duration": 6.78
        },
        {
            "text": "يكون لهم احتفال مهيب فيتوافدون جميع",
            "start": 775.5,
            "duration": 6.72
        },
        {
            "text": "السكان في القريه لرؤيه الحاج وطبعا",
            "start": 779.22,
            "duration": 6.179
        },
        {
            "text": "المطلوب من الحاج هو الحديث بالتفصيل عن",
            "start": 782.22,
            "duration": 6.66
        },
        {
            "text": "من راه وسمعه اثناء هذه الرحله الطويله في",
            "start": 785.399,
            "duration": 8.221
        },
        {
            "text": "الحج تذوب الطبقيه وتتلاشى التفرقه وتتجسد",
            "start": 788.88,
            "duration": 8.579
        },
        {
            "text": "المساواه الحق المساواه الصادقه لان فيها",
            "start": 793.62,
            "duration": 7.2
        },
        {
            "text": "الانسان انسلخ من الماديه الى عالم",
            "start": 797.459,
            "duration": 6.141
        },
        {
            "text": "المعنويات",
            "start": 800.82,
            "duration": 2.78
        },
        {
            "text": "[موسيقى]",
            "start": 804.73,
            "duration": 3.689
        },
        {
            "text": "[موسيقى]",
            "start": 811.72,
            "duration": 9.809
        }
    ]
}