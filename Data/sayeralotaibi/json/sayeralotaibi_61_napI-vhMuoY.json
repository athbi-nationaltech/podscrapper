        },
        {
            "text": "يجمعون اكثر من 100 الف عالم من مختلف",
            "start": 386.699,
            "duration": 7.021
        },
        {
            "text": "التخصصات والجامعات والدول وضعوهم جميعا",
            "start": 390.419,
            "duration": 6.541
        },
        {
            "text": "في جبهه واحده عشان يصنعون القنبله قبل",
            "start": 393.72,
            "duration": 6.419
        },
        {
            "text": "الالمان رغم المشروع مرتبط في منهات انفي",
            "start": 396.96,
            "duration": 6.84
        },
        {
            "text": "نيويورك لكن كان المكان بعيد عن نيويورك",
            "start": 400.139,
            "duration": 7.981
        },
        {
            "text": "ومتواجد في صحراء فوضع المشروع في مكان",
            "start": 403.8,
            "duration": 7.619
        },
        {
            "text": "بعيد ومعزول اولا لعمل اختبارات نوويه",
            "start": 408.12,
            "duration": 7.68
        },
        {
            "text": "سليمه وثانيا عشان المشروع يكون سري وطبعا",
            "start": 411.419,
            "duration": 7.5
        },
        {
            "text": "الشخص اللي اختاروه انه يكون قائد لتحالف",
            "start": 415.8,
            "duration": 5.66
        },
        {
            "text": "العلماء اللي هو روبرت اوبن هايبر",
            "start": 418.919,
            "duration": 5.28
        },
        {
            "text": "وللمعلوميه اوبن هايبر اختاروه لانه في",
            "start": 421.46,
            "duration": 5.679
        },
        {
            "text": "الفتره الاخيره عمل اختراعات كبيره في علم",
            "start": 424.199,
            "duration": 5.461
        },
        {
            "text": "الفيزياء فكانوا متوقعين منه انه يكون",
            "start": 427.139,
            "duration": 5.881
        },
        {
            "text": "البرت اينشتاين المستقبل اول مشكله واجهت",
            "start": 429.66,
            "duration": 5.879
        },
        {
            "text": "الحكومه الامريكيه في هذا المشروع وجدت",
            "start": 433.02,
            "duration": 5.519
        },
        {
            "text": "نفسها مطالبه امنيا باحتواء اكثر من مئه",
            "start": 435.539,
            "duration": 6.841
        },
        {
            "text": "الف عالم باسرهم واولادهم كل هذا واشتغلون",
            "start": 438.539,
            "duration": 7.56
        },
        {
            "text": "بسريه تامه وتحت حراسه مشدده من غير ما",
            "start": 442.38,
            "duration": 6.599
        },
        {
            "text": "تخرج اي معلومه او تسريب فتخيلوا معي",
            "start": 446.099,
            "duration": 6.241
        },
        {
            "text": "تحتاج ان السر يكون بين 100 الف شخص وهو",
            "start": 448.979,
            "duration": 6.141
        },
        {
            "text": "سر خطير لانه عن وجود القنبله الذريه",
            "start": 452.34,
            "duration": 5.82
        },
        {
            "text": "فاضطرت استخبارات الامريكيه انها تلجا",
            "start": 455.12,
            "duration": 6.699
        },
        {
            "text": "الاجراءات امنيه مهلكه فبنوا مدينه كامله",
            "start": 458.16,
            "duration": 7.74
        },
        {
            "text": "فيها كنيسه مدرسه ومنازل وايضا مستشفيات",
            "start": 461.819,
            "duration": 7.861
        },
        {
            "text": "ومختبرات ومعامل رئيسيه للمشروع كلها في",
            "start": 465.9,
            "duration": 6.9
        },
        {
            "text": "مكان سري اسمه لوس الموس في ولايه",
            "start": 469.68,
            "duration": 6.6
        },
        {
            "text": "نيومكسيكو وطبعا كل العلماء لازم يتعاملون",
            "start": 472.8,
            "duration": 6.899
        },
        {
            "text": "مع بعض باسماء مستعاره علشان اذا احد قرر",
            "start": 476.28,
            "duration": 6.479
        },
        {
            "text": "انه يسرب المعلومات ما يعرف اسم اي واحد",
            "start": 479.699,
            "duration": 6.361
        },
        {
            "text": "فيهم وتخيلوا كل اطفال العلماء الموجودين",
            "start": 482.759,
            "duration": 5.88
        },
        {
            "text": "في المنطقه السريه ابائهم يسجلون وكان",
            "start": 486.06,
            "duration": 6.3
        },
        {
            "text": "ولادتهم في شهاده الميلاد باسم بدل اسم",
            "start": 488.639,
            "duration": 6.84
        },
        {
            "text": "لوس الماس عشان ما يتم الكشف عن موقع",
            "start": 492.36,
            "duration": 5.58
        },
        {
            "text": "العلماء رغم وجود الاجراءات الامنيه",
            "start": 495.479,
            "duration": 6.181
        },
        {
            "text": "المشدده الا انه اوبن هايبر قرر انه ما",
            "start": 497.94,
            "duration": 7.259
        },
        {
            "text": "يستبعد اي عالم له ميول يساريه او له اصول",
            "start": 501.66,
            "duration": 6.12
        },
        {
            "text": "المانيه لان جزء كبير من العلماء اللي",
            "start": 505.199,
            "duration": 5.34
        },
        {
            "text": "يشتغلون في مشروع مان هاتن هم مهاجرين",
            "start": 507.78,
            "duration": 6.119
        },
        {
            "text": "بالاساس هربا من هتلر بعد جهد اوبن هايمر",
            "start": 510.539,
            "duration": 6.36
        },
        {
            "text": "والعلماء اللي معاه بعد فتره طويله قبلها",
            "start": 513.899,
            "duration": 7.14
        },
        {
            "text": "في الصراعات الامريكيه بالاخير استثمرت",
            "start": 516.899,
            "duration": 5.361
        },
        {
            "text": "جهودهم",
            "start": 521.039,
            "duration": 6.48
        },
        {
            "text": "او الاختبار النووي ففي 16 يوليو عام 1945",
            "start": 522.26,
            "duration": 8.8
        },
        {
            "text": "هذا اليوم سيشهد فيه اوبن هايبر اول",
            "start": 527.519,
            "duration": 7.141
        },
        {
            "text": "اختبار نووي للقنبله الذريه وكان الاختبار",
            "start": 531.06,
            "duration": 6.48
        },
        {
            "text": "يعتمد على انشطار البلوتونيوم فاطلقه",
            "start": 534.66,
            "duration": 6.239
        },
        {
            "text": "القنبله ولما اوبن هايمر شاف الانفجار في",
            "start": 537.54,
            "duration": 6.6
        },
        {
            "text": "الاختبار التجريبي واثرها التدميري قال",
            "start": 540.899,
            "duration": 7.44
        },
        {
            "text": "مقولته المشهوره الان انا الموت مدمر",
            "start": 544.14,
            "duration": 6.78
        },
        {
            "text": "العوالم وبالنسبه لرده فعل العلماء",
            "start": 548.339,
            "duration": 7.141
        },
        {
            "text": "الاخرين البعض بكى لكن الاغلب كان صامت",
            "start": 550.92,
            "duration": 8.46
        },
        {
            "text": "ومصدوم فهذه كانت اخر لحظه في العلماء",
            "start": 555.48,
            "duration": 8.34
        },
        {
            "text": "يوقفون مع بعض ولهدف واحد فبعدها حصل",
            "start": 559.38,
            "duration": 7.44
        },
        {
            "text": "الاختلاف ما بينهم فعلى حسب احد العلماء",
            "start": 563.82,
            "duration": 4.1
        },
        {
            "text": "اللي اسمه",
            "start": 566.82,
            "duration": 5.28
        },
        {
            "text": "يقول بعض العلماء بعدها ارفضوا بشكل قطعي",
            "start": 567.92,
            "duration": 7.3
        },
        {
            "text": "انه يتم استخدام السلاح اللي صنعوه في هذه",
            "start": 572.1,
            "duration": 5.94
        },
        {
            "text": "الحرب والبعض الاخر راسل البيت الا",
            "start": 575.22,
            "duration": 6.239
        },
        {
            "text": "بالتحديد الرئيس الامريكي وطالب انه ما",
            "start": 578.04,
            "duration": 6.0
        },
        {
            "text": "يتم استخدام القنبله في الاماكن اللي فيها",
            "start": 581.459,
            "duration": 5.641
        },
        {
            "text": "سكان وبعض العلماء تمنوا لو كانوا من",
            "start": 584.04,
            "duration": 5.4
        },
        {
            "text": "سحبين من مشروع منهاتهم ومنهم العالم",
            "start": 587.1,
            "duration": 6.0
        },
        {
            "text": "البولندي جوزيف اللي سحب نفسه من مشروع",
            "start": 589.44,
            "duration": 7.44
        },
        {
            "text": "مان هاتهم في سنه 1943 بعد ما عرف ان",
            "start": 593.1,
            "duration": 6.0
        },
        {
            "text": "العلماء الالمان ما عندهم الوقت او",
            "start": 596.88,
            "duration": 4.92
        },
        {
            "text": "الامكانيات الكافيه انه يعملون القنبله",
            "start": 599.1,
            "duration": 6.9
        },
        {
            "text": "فكرس حياته كناشط ضد الاسلحه النوويه وحصل",
            "start": 601.8,
            "duration": 8.34
        },
        {
            "text": "على جائزه نوبل للسلام في سنه 1995 بعد",
            "start": 606.0,
            "duration": 7.62
        },
        {
            "text": "الاختبار النووي وصدمه العلماء قام اوبن",
            "start": 610.14,
            "duration": 6.66
        },
        {
            "text": "هايبر وعمل مؤتمر صحفي وقال كلمته",
            "start": 613.62,
            "duration": 6.839
        },
        {
            "text": "المشهوره اليوم انتم ابطال لكن في وقت",
            "start": 616.8,
            "duration": 8.52
        },
        {
            "text": "قريب الناس ستنتقدكم لفتره طويله رغم شعور",
            "start": 620.459,
            "duration": 8.101
        },
        {
            "text": "العلماء بالذنب ومطالباتهم بوقف استخدام",
            "start": 625.32,
            "duration": 6.9
        },
        {
            "text": "السلاح النووي ورغم نتائج الحرب اللي تؤكد",
            "start": 628.56,
            "duration": 6.36
        },
        {
            "text": "هزيمه هتلر الا ان الولايات المتحده",
            "start": 632.22,
            "duration": 5.76
        },
        {
            "text": "الامريكيه في 6 اغسطس عام",
            "start": 634.92,
            "duration": 7.979
        },
        {
            "text": "145 قصفت مدينه هيروشيما اليابانيه بقنبله",
            "start": 637.98,
            "duration": 7.859
        },
        {
            "text": "سموها ليتل بوي معموله من اليورانيوم",
            "start": 642.899,
            "duration": 8.221
        },
        {
            "text": "واللي راح ضحيتها 236 الف انسان وبعد",
            "start": 645.839,
            "duration": 8.701
        },
        {
            "text": "ثلاثه ايام رموا قنبله البلوتونيوم اللي",
            "start": 651.12,
            "duration": 7.2
        },
        {
            "text": "اسموها زفاتمان على مدينه نجازاكي واللي",
            "start": 654.54,
            "duration": 8.88
        },
        {
            "text": "كذلك راح ضحيتها اكثر من 45 الف انسان ومن",
            "start": 658.32,
            "duration": 9.18
        },
        {
            "text": "بعدها استسلمت اليابان وانتهت الحرب للاسف",
            "start": 663.42,
            "duration": 6.599
        },
        {
            "text": "البعض ياكد انها القنبله هي اللي انهت",
            "start": 667.5,
            "duration": 5.1
        },
        {
            "text": "الحرب فمثلا وزير الحرب الامريكي هنريس",
            "start": 670.019,
            "duration": 7.32
        },
        {
            "text": "سمسم في سنه 1947 يقول ان اليابان ما راح",
            "start": 672.6,
            "duration": 8.52
        },
        {
            "text": "تستسلم ابدا الا بغزو بري فيقول انها",
            "start": 677.339,
            "duration": 6.74
        },
        {
            "text": "الغزو راح يموت اكثر من مليون جندي امريكي",
            "start": 681.12,
            "duration": 5.7
        },
        {
            "text": "وضعف هالعدد راح يموتون من الجنود",
            "start": 684.079,
            "duration": 6.281
        },
        {
            "text": "اليابانيين فعندما تقتن 320 الف في سبيل",
            "start": 686.82,
            "duration": 6.06
        },
        {
            "text": "ان ملايين الناس يعيشون فيقول وزير الحرب",
            "start": 690.36,
            "duration": 6.3
        },
        {
            "text": "الامريكي بحسابات لغه الارقام القنبله كان",
            "start": 692.88,
            "duration": 5.04
        },
        {
            "text": "حل رحيم",
            "start": 696.66,
            "duration": 3.66
        },
        {
            "text": "من الناحيه الثانيه الكثير من المؤرخين",
            "start": 697.92,
            "duration": 6.0
        },
        {
            "text": "يؤكدون ان المعطيات تؤكد ان اليابان راح",
            "start": 700.32,
            "duration": 7.019
        },
        {
            "text": "تستسلم قبل اسقاط القنبلتين وقالوا ان",
            "start": 703.92,
            "duration": 6.24
        },
        {
            "text": "القنبله ممكن انه يرمونها على طوكيو باي",
            "start": 707.339,
            "duration": 5.821
        },
        {
            "text": "او ميناء طوكيو وفيها حفاظ على البشر",
            "start": 710.16,
            "duration": 6.48
        },
        {
            "text": "وكذلك رساله تهديد تنهي الحرب من غير",
            "start": 713.16,
            "duration": 7.5
        },
        {
            "text": "ضحايا بالنسبه لاوبن هايمر اكد انه هو ادى",
            "start": 716.64,
            "duration": 6.72
        },
        {
            "text": "واجبه للاخلاقي وكذلك الوطني وان",
            "start": 720.66,
            "duration": 5.88
        },
        {
            "text": "المسؤوليه كلها تقع على الحكومات وليس على",
            "start": 723.36,
            "duration": 6.36
        },
        {
            "text": "العلماء وقال ان مشروع منهاتن مثل ما اطلق",
            "start": 726.54,
            "duration": 6.12
        },
        {
            "text": "القنبله ما ننسى الفضل بسبب هذا المشروع",
            "start": 729.72,
            "duration": 6.96
        },
        {
            "text": "تم اختراع الطاقات نوويه سليمه فالطاقات",
            "start": 732.66,
            "duration": 6.66
        },
        {
            "text": "النوويه اللي اخترعوها كانت مهمه بتوليد",
            "start": 736.68,
            "duration": 5.339
        },
        {
            "text": "الكهرباء اللي ساهمت باختراع اجهزه",
            "start": 739.32,
            "duration": 5.699
        },
        {
            "text": "التصوير ام ار اي وهي الاشعه المقطعيه",
            "start": 742.019,
            "duration": 5.581
        },
        {
            "text": "للتصوير بالرنين المغناطيسي وكذلك",
            "start": 745.019,
            "duration": 5.82
        },
        {
            "text": "اختراعاتهم ساهمت للعلاج الاشعاعي للسرطان",
            "start": 747.6,
            "duration": 6.479
        },
        {
            "text": "فيقول اوبن هايبر مشروع مان هاتن ادى",
            "start": 750.839,
            "duration": 6.901
        },
        {
            "text": "لاكتشافات جديده افادت البشريه قصه اوبن",
            "start": 754.079,
            "duration": 7.141
        },
        {
            "text": "هاي والقنبله ما انتهت بهذه اللحظه ففي",
            "start": 757.74,
            "duration": 8.52
        },
        {
            "text": "سبتمبر عام 1949 نجح الاتحاد السوفيتي في",
            "start": 761.22,
            "duration": 8.88
        },
        {
            "text": "صنع القنبله الذريه الاولى لهم فتم تسريب",
            "start": 766.26,
            "duration": 7.259
        },
        {
            "text": "صنع القنبله الذريه من مصنع منهاتن الى",
            "start": 770.1,
            "duration": 6.299
        },
        {
            "text": "الاتحاد السوفيتي فاصبح هناك عدو جديد",
            "start": 773.519,
            "duration": 5.581
        },
        {
            "text": "لامريكا من بعد العدو الاول هتلر",
            "start": 776.399,
            "duration": 6.541
        },
        {
            "text": "وبهاللحظه بدات الحرب البارده بينهم فقررت",
            "start": 779.1,
            "duration": 6.0
        },
        {
            "text": "الولايات المتحده الامريكيه انها تصنع",
            "start": 782.94,
            "duration": 5.639
        },
        {
            "text": "سلاح اقوى من السلاح السابق وطلبت للمره",
            "start": 785.1,
            "duration": 6.179
        },
        {
            "text": "الثانيه من اوبن هايبر انه يشرف على",
            "start": 788.579,
            "duration": 5.221
        },
        {
            "text": "المشروع الجديد والمشروع باختصار صناعه",
            "start": 791.279,
            "duration": 6.481
        },
        {
            "text": "القنبله الهيدروجينيه وهذه القنبله اقوى ب",
            "start": 793.8,
            "duration": 6.9
        },
        {
            "text": "700 مره من القنبله اللي سموها ليتل بوي",
            "start": 797.76,
            "duration": 7.079
        },
        {
            "text": "اللي اختلت 237 الف انسان فالقنبله",
            "start": 800.7,
            "duration": 7.379
        },
        {
            "text": "الهيدروجينيه تعادل قوتها عشره مليون طن",
            "start": 804.839,
            "duration": 6.06
        },
        {
            "text": "تي ان تي قرروا اوبن هايبر انه ينسحب من",
            "start": 808.079,
            "duration": 5.76
        },
        {
            "text": "هذا المشروع وقال لهم انا وافقت اني اعمل",
            "start": 810.899,
            "duration": 6.961
        },
        {
            "text": "سلاح رادع يكسبكم الحرب ضد هتلر لكن انا",
            "start": 813.839,
            "duration": 7.021
        },
        {
            "text": "سلاح يبيد البشريه فاستمرت الجهود",
            "start": 817.86,
            "duration": 5.4
        },
        {
            "text": "الامريكيه بالاعتماد على علماء ثانيين",
            "start": 820.86,
            "duration": 5.64
        },
        {
            "text": "غيره وبالفعل انتج القنبله هذه في نوفمبر",
            "start": 823.26,
            "duration": 7.139
        },
        {
            "text": "عام 1952 اما بالنسبه للعالم اوبن هايمر",
            "start": 826.5,
            "duration": 6.2
        },
        {
            "text": "العالم اللي انهى الحرب العالميه الثانيه",
            "start": 830.399,
            "duration": 5.821
        },
        {
            "text": "فامريكا بدات تعتبر جاسوس ومتعاطف مع",
            "start": 832.7,
            "duration": 6.759
        },
        {
            "text": "الاتحاد السوفيتي لمجرد رفضه بالاستمرار",
            "start": 836.22,
            "duration": 5.7
        },
        {
            "text": "في المجهود الحربي فبدات الاجهزه الامنيه",
            "start": 839.459,
            "duration": 6.661
        },
        {
            "text": "اتطلع انه معلومات قديمه ومنها علاقه حب",
            "start": 841.92,
            "duration": 8.34
        },
        {
            "text": "قديمه مع فتاه اسمها جين تتلوك كان يحبها",
            "start": 846.12,
            "duration": 7.56
        },
        {
            "text": "في فتره الثلاثينات وهذه الفتاه كانت عضو",
            "start": 850.26,
            "duration": 7.079
        },
        {
            "text": "في الحزب الشيوعي وطبعا قبل هايمر يقبل ان",
            "start": 853.68,
            "duration": 6.599
        },
        {
            "text": "يكون مشرف على مشروع منهاتن كانت الاجهزه",
            "start": 857.339,
            "duration": 5.821
        },
        {
            "text": "الامنيه عندها المعلومات كلها عن اوبن",
            "start": 860.279,
            "duration": 5.641
        },
        {
            "text": "هايبر بس لما رفض اوبن هايبر انه يشرف على",
            "start": 863.16,
            "duration": 5.4
        },
        {
            "text": "القنبله هيدروجينيه اخرجوا هذه المعلومات",
            "start": 865.92,
            "duration": 5.7
        },
        {
            "text": "ضده والرئيس الامريكي دوايه ايزنهاور اصدر",
            "start": 868.56,
            "duration": 7.56
        },
        {
            "text": "قرار في سنه 1953 بمن اي معلومه توصل",
            "start": 871.62,
            "duration": 7.32
        },
        {
            "text": "لاوبن هايبر على المجه النووي وبعد",
            "start": 876.12,
            "duration": 5.94
        },
        {
            "text": "استجوابه قررت لجنه من الكونجرس سحب",
            "start": 878.94,
            "duration": 6.3
        },
        {
            "text": "ترخيصه الامني واتخذوا هذا القرار رغم",
            "start": 882.06,
            "duration": 5.94
        },
        {
            "text": "معارضه العالم البرت اينشتاين والكثير من",
            "start": 885.24,
            "duration": 6.0
        },
        {
            "text": "العلماء في امريكا وبالنهايه دفع اوبن",
            "start": 888.0,
            "duration": 7.019
        },
        {
            "text": "هايمر ثمن اختياره وتحول من عالم وطني الى",
            "start": 891.24,
            "duration": 7.62
        },
        {
            "text": "جاسوس وبالاخير ان العلم من غير الاخلاق",
            "start": 895.019,
            "duration": 8.401
        },
        {
            "text": "والضمير ستقدم لنا اجابات ناقصه اجابات",
            "start": 898.86,
            "duration": 10.44
        },
        {
            "text": "مثل قنبله مرعبه تقتل وتبيد البشريه ولنا",
            "start": 903.42,
            "duration": 9.599
        },
        {
            "text": "عبره في الذكاء الاصطناعي الذي سيصنع مجد",
            "start": 909.3,
            "duration": 8.64
        },
        {
            "text": "لعالمنا بوجود الضمائر ويبيد البشريه من",
