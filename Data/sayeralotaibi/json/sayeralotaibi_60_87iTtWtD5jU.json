        },
        {
            "text": "ووزير الخارجيه الكسندر ميلان اللي اصبح",
            "start": 369.6,
            "duration": 7.5
        },
        {
            "text": "رئيس فرنسا فيما بعد بتقرير من 23 صفحه عن",
            "start": 373.199,
            "duration": 7.201
        },
        {
            "text": "مواقف المسلمين من شمال افريقيا والمشرق",
            "start": 377.1,
            "duration": 6.539
        },
        {
            "text": "العربي اتجاه فرنسا وكان بالمقابل انه",
            "start": 380.4,
            "duration": 5.519
        },
        {
            "text": "دعاهم الى مزيد من التحكم الذاتي",
            "start": 383.639,
            "duration": 5.461
        },
        {
            "text": "بالمستعمرات الفرنسيه والامر الخطير ان",
            "start": 385.919,
            "duration": 6.601
        },
        {
            "text": "اسس معهد باريس الاسلامي اللي كان له دور",
            "start": 389.1,
            "duration": 7.319
        },
        {
            "text": "كبير كمصدر حيوي للاستخبارات الفرنسيه على",
            "start": 392.52,
            "duration": 6.899
        },
        {
            "text": "المسلمين في فرنسا كان عبد القادر يحاول",
            "start": 396.419,
            "duration": 6.541
        },
        {
            "text": "يقدم طلب للحصول على اذن لبناء مسجد في",
            "start": 399.419,
            "duration": 6.961
        },
        {
            "text": "باريس عشان يكون له هيكل يرمز الى الصداقه",
            "start": 402.96,
            "duration": 6.48
        },
        {
            "text": "الابديه ما بين فرنسا والاسلام ويقول عبد",
            "start": 406.38,
            "duration": 5.939
        },
        {
            "text": "القادر عشان يحيي ذكرى تضحيات عشرات",
            "start": 409.44,
            "duration": 5.819
        },
        {
            "text": "الالاف من الجنود المسلمين اللي قاتلوا",
            "start": 412.319,
            "duration": 5.281
        },
        {
            "text": "لدعم فرنسا خلال الحرب العالميه الاولى",
            "start": 415.259,
            "duration": 6.06
        },
        {
            "text": "بما في ذلك معركه فردان اللي دارت ريحاها",
            "start": 417.6,
            "duration": 7.14
        },
        {
            "text": "في الجبهه الغربيه بفرنسا وتعتبر من اطول",
            "start": 421.319,
            "duration": 6.361
        },
        {
            "text": "المعارك في الحرب العالميه الاولى انتصار",
            "start": 424.74,
            "duration": 6.239
        },
        {
            "text": "الفرنسيين في فردان لا يمكن انه يحدث لولا",
            "start": 427.68,
            "duration": 5.7
        },
        {
            "text": "تضحيات المسلمين فكانوا المسلمين في فرنسا",
            "start": 430.979,
            "duration": 5.28
        },
        {
            "text": "لهم دور كبير في مواجهه الالمان على",
            "start": 433.38,
            "duration": 5.099
        },
        {
            "text": "العموم الحكومه الفرنسيه وافقت على بناء",
            "start": 436.259,
            "duration": 5.601
        },
        {
            "text": "المسجد اللي استمر بنيانه من عام 1922",
            "start": 438.479,
            "duration": 7.321
        },
        {
            "text": "وحتى عام 1926 بعد الحرب العالميه الاولى",
            "start": 441.86,
            "duration": 7.0
        },
        {
            "text": "انتقل عدد كبير من الجزائريين الى فرنسا",
            "start": 445.8,
            "duration": 6.0
        },
        {
            "text": "وغالبا كانوا يعملون في المصانع وظائف",
            "start": 448.86,
            "duration": 5.279
        },
        {
            "text": "البناء واغلبهم من القبائل المعروفه باسم",
            "start": 451.8,
            "duration": 4.619
        },
        {
            "text": "القبايل واللي موجودين في جبال الاطلس",
            "start": 454.139,
            "duration": 4.861
        },
        {
            "text": "والقرى الفقيره في الجزائر كتاب عنهم",
            "start": 456.419,
            "duration": 4.641
        },
        {
            "text": "الفيلسوف الفرنسي",
            "start": 459.0,
            "duration": 5.46
        },
        {
            "text": "ووصفهم السكان المسلمين المهيمنين في",
            "start": 461.06,
            "duration": 6.22
        },
        {
            "text": "باريس الكثير منهم عاش في احياء فقيره في",
            "start": 464.46,
            "duration": 6.359
        },
        {
            "text": "بلفيل المتواجد في شمال شرق باريس وهناك",
            "start": 467.28,
            "duration": 6.24
        },
        {
            "text": "شكلوا روابط مع جيرانهم المهاجرين او",
            "start": 470.819,
            "duration": 4.681
        },
        {
            "text": "زملائهم في العمل وكانوا خليط من",
            "start": 473.52,
            "duration": 4.38
        },
        {
            "text": "التونسيون والمغاربه واليهود من شمال",
            "start": 475.5,
            "duration": 5.28
        },
        {
            "text": "افريقيا وروسيا على العموم نجح عبد القادر",
            "start": 477.9,
            "duration": 6.12
        },
        {
            "text": "بتاسيس الجامع الكبير بباريس وانتهى",
            "start": 480.78,
            "duration": 6.78
        },
        {
            "text": "البنيان في عام 1926 وفي افتتاح الجامع",
            "start": 484.02,
            "duration": 6.5
        },
        {
            "text": "الكبير اشاد عبد القادر في خطبته الاولى",
            "start": 487.56,
            "duration": 5.94
        },
        {
            "text": "بالاتحاد الابدي ما بين الاسلام وفرنسا",
            "start": 490.52,
            "duration": 5.38
        },
        {
            "text": "وكانت الخطبه باللغه العربيه والفرنسيه",
            "start": 493.5,
            "duration": 5.699
        },
        {
            "text": "لان اول صلاه في هذا المسجد كان بحضور",
            "start": 495.9,
            "duration": 6.66
        },
        {
            "text": "رئيس فرنسا بعد هذه الاحداث والمواقف ما",
            "start": 499.199,
            "duration": 6.18
        },
        {
            "text": "بين عبد القادر والحكومه الفرنسيه بدا",
            "start": 502.56,
            "duration": 5.579
        },
        {
            "text": "يتردد على الصالونات الباريسيه واللي فيها",
            "start": 505.379,
            "duration": 5.04
        },
        {
            "text": "يتناقشون بالامور السياسيه على مستوى",
            "start": 508.139,
            "duration": 5.481
        },
        {
            "text": "اصحاب النفوذ في فرنسا فلقبوا عبد القادر",
            "start": 510.419,
            "duration": 7.081
        },
        {
            "text": "كاكثر المسلمين الباريسيين في عام 1940",
            "start": 513.62,
            "duration": 7.419
        },
        {
            "text": "غاز النازيون فرنسا واسقطوها خلال الحرب",
            "start": 517.5,
            "duration": 5.82
        },
        {
            "text": "العالميه الثانيه فانضمت العديد من",
            "start": 521.039,
            "duration": 4.621
        },
        {
            "text": "القبائل المسلمه الى صفوف المقاومه",
            "start": 523.32,
            "duration": 5.76
        },
        {
            "text": "الفرنسيه وتحققت نجاحات كبيره للقبائل",
            "start": 525.66,
            "duration": 7.2
        },
        {
            "text": "بالعمليات السريه ضد الفاشيه بسبب تنظيمهم",
            "start": 529.08,
            "duration": 6.78
        },
        {
            "text": "السري وتواجدهم في المسجد الكبير ورئيس",
            "start": 532.86,
            "duration": 5.34
        },
        {
            "text": "الجامع اللي هو عبد القادر لما عرف ان",
            "start": 535.86,
            "duration": 4.979
        },
        {
            "text": "الجستابو اللي هي قوه الشرطه السياسيه",
            "start": 538.2,
            "duration": 6.42
        },
        {
            "text": "للدوله النازيه ترتكب مذابح في يهود باريس",
            "start": 540.839,
            "duration": 8.221
        },
        {
            "text": "ابتكر عمليه انقاذ ثلاثيه اولا قدم لليهود",
            "start": 544.62,
            "duration": 7.8
        },
        {
            "text": "الاوروبيين والجزائريين ماوى في نفس الشقق",
            "start": 549.06,
            "duration": 6.18
        },
        {
            "text": "اللي تسكنها العائلات المسلمه ثانيا",
            "start": 552.42,
            "duration": 6.599
        },
        {
            "text": "اعطاهم شهادات هويه مزوره لاثبات بانهم",
            "start": 555.24,
            "duration": 6.96
        },
        {
            "text": "مسلمون وليسوا يهودا ثالثا واخيرا بدا في",
            "start": 559.019,
            "duration": 5.76
        },
        {
            "text": "استخدام الانفاق الموجوده اسفل المسجد",
            "start": 562.2,
            "duration": 5.759
        },
        {
            "text": "كمسارات للهروب فزحفوا اليهود المختبئين",
            "start": 564.779,
            "duration": 6.301
        },
        {
            "text": "اسفل المسجد وشقوا طريقهم عبر المج",
            "start": 567.959,
            "duration": 6.901
        },
        {
            "text": "والانفاق ومنها الى ضفاف نهر السين وكانت",
            "start": 571.08,
            "duration": 6.84
        },
        {
            "text": "بانتظارهم قوارب النبيذ الفارغه وكان اللي",
            "start": 574.86,
            "duration": 5.64
        },
        {
            "text": "يديرها القبايل وامه مجموعه امازيغيه",
            "start": 577.92,
            "duration": 5.52
        },
        {
            "text": "فكانوا بانتظارهم عشان يهربونهم من باريس",
            "start": 580.5,
            "duration": 5.1
        },
        {
            "text": "المحتله تم القبض على عبد القادر",
            "start": 583.44,
            "duration": 5.519
        },
        {
            "text": "واستجوابه من قبل الجستابو عده مرات بعد",
            "start": 585.6,
            "duration": 6.419
        },
        {
            "text": "ما انتشرت الشائعات عن مقاومه المسجد ومع",
            "start": 588.959,
            "duration": 5.761
        },
        {
            "text": "ذلك امرت القياده الالمانيه العليا في",
            "start": 592.019,
            "duration": 6.061
        },
        {
            "text": "اطلاق سراحه كل مره لان المانيا كانت تخاف",
            "start": 594.72,
            "duration": 6.059
        },
        {
            "text": "من وجود اعمال شغب من الجزائريين في شمال",
            "start": 598.08,
            "duration": 5.04
        },
        {
            "text": "افريقيا او باريس فكانوا خايفين من",
            "start": 600.779,
            "duration": 5.101
        },
        {
            "text": "المسلمين على الجبهتين من اليهود اللي تم",
            "start": 603.12,
            "duration": 6.0
        },
        {
            "text": "تعريبهم اسفل المسجد مغني يهودي شعبي",
            "start": 605.88,
            "duration": 6.36
        },
        {
            "text": "معروف اسمه سليم هلالي وهو يهودي من",
            "start": 609.12,
            "duration": 6.12
        },
        {
            "text": "البربر في الجزائر ويعتبر صديق عبد القادر",
            "start": 612.24,
            "duration": 6.02
        },
        {
            "text": "اعطاه عبد القادر شهاده اعتناق الاسلام",
            "start": 615.24,
            "duration": 7.02
        },
        {
            "text": "ليثبت زورا ان جد سليم اعتنق الاسلام وفي",
            "start": 618.26,
            "duration": 6.04
        },
        {
            "text": "المقبره الاسلاميه في مدينه بوبين",
            "start": 622.26,
            "duration": 5.639
        },
        {
            "text": "الفرنسيه كان هناك شاهد قبر منقوش باسم",
            "start": 624.3,
            "duration": 6.719
        },
        {
            "text": "عائله جد هلالي بعد ما فحصوه النازيون",
            "start": 627.899,
            "duration": 6.541
        },
        {
            "text": "تركوا هلالي وشانه فعاش طوال الحرب في",
            "start": 631.019,
            "duration": 6.301
        },
        {
            "text": "المسجد الكبير في باريس قبل ان يهرب بعد",
            "start": 634.44,
            "duration": 5.66
        },
        {
            "text": "ما وصل للنازيين عن وجود مخبئ تحت المسجد",
            "start": 637.32,
            "duration": 5.88
        },
        {
            "text": "وقيامهم بالاقتحام بانتظام في مهمه بحث",
            "start": 640.1,
            "duration": 6.94
        },
        {
            "text": "وترحيل كان عند عبد القادر جرس تحذير مخبا",
            "start": 643.2,
            "duration": 7.02
        },
        {
            "text": "في الارضيه تحت مكتبه ينبه الجميع عن وجود",
            "start": 647.04,
            "duration": 6.12
        },
        {
            "text": "غاره نازيه في اي لحظه بعد الحرب اصبح",
            "start": 650.22,
            "duration": 6.78
        },
        {
            "text": "هلالي اشهر مطرب شرقي في اوروبا ظل هو عبد",
            "start": 653.16,
            "duration": 6.78
        },
        {
            "text": "القادر اصدقائه الى وفاتهم وفي يهودي اخر",
            "start": 657.0,
            "duration": 5.399
        },
        {
            "text": "اسمه البرت اسولين وهو يهودي من شمال",
            "start": 659.94,
            "duration": 5.339
        },
        {
            "text": "افريقيا هرب مع صديقه المسلم من معسكر",
            "start": 662.399,
            "duration": 5.581
        },
        {
            "text": "اسرى حرب في المانيا فوصل الى باريس من",
            "start": 665.279,
            "duration": 6.18
        },
        {
            "text": "غير اوراق وهويه فرحب عبد القادر فيه الى",
            "start": 667.98,
            "duration": 6.14
        },
        {
            "text": "المسجد فيقول البرت انا وصديقي المسلم",
            "start": 671.459,
            "duration": 5.82
        },
        {
            "text": "اختبانا في الطابق السفلي في المسجد فرايت",
            "start": 674.12,
            "duration": 5.92
        },
        {
            "text": "العديد من اليهود موجودين في هذا المخبا",
            "start": 677.279,
            "duration": 4.921
        },
        {
            "text": "فيقول الاطفال كانوا موجودين في الطابق",
            "start": 680.04,
            "duration": 4.799
        },
        {
            "text": "العلوي مع العائلات المسلمه والطابق",
            "start": 682.2,
            "duration": 4.68
        },
        {
            "text": "السفلي كانوا موجودين فيه البالغين من",
            "start": 684.839,
            "duration": 6.381
        },
        {
            "text": "اليهود بعد الحرب ادلى اسلين بشهادته بان",
            "start": 686.88,
            "duration": 7.8
        },
        {
            "text": "1600 يهودي يمرون في الطوابق السفليه",
            "start": 691.22,
            "duration": 6.64
        },
        {
            "text": "للمسجد ومنها ينزلون الى انفاق للمتاهه",
            "start": 694.68,
            "duration": 6.599
        },
        {
            "text": "المظلمه واخيرا جعلونا على قوارب المنتظره",
            "start": 697.86,
            "duration": 6.659
        },
        {
            "text": "على نهر السين وبعدها نقلونا الى بر",
            "start": 701.279,
            "duration": 6.06
        },
        {
            "text": "الامان في المغرب العربي واسبانيا بعد",
            "start": 704.519,
            "duration": 5.841
        },
        {
            "text": "الحرب وشهادات اليهود الحكومه الفرنسيه",
            "start": 707.339,
            "duration": 7.141
        },
        {
            "text": "منحت عبد القادر وسام الشرف الكبير وفي",
            "start": 710.36,
            "duration": 6.599
        },
        {
            "text": "المراحل الاولى من حرب استقلال الجزائر",
            "start": 714.48,
            "duration": 7.5
        },
        {
            "text": "توفى عبد القادر في عام 1954 ودفن في",
            "start": 716.959,
            "duration": 7.921
        },
        {
            "text": "المسجد باتجاه مكه كجميع حال المسلمين",
            "start": 721.98,
            "duration": 7.02
        },
        {
            "text": "ستنتهي الحرب ويتصافح القاده والخاسر",
            "start": 724.88,
            "duration": 8.68
        },
        {
            "text": "الاكبر من يتخلى عن وطنه واراد ان يكون في",
            "start": 729.0,
            "duration": 9.72
        },
        {
            "text": "صف القوي ليزداد قوه فيق ضد الضعيف حتى لو",
            "start": 733.56,
            "duration": 9.2
        },
        {
            "text": "كان هذا الضعيف هو وطنه",
            "start": 738.72,
            "duration": 4.04
        },
        {
            "text": "[موسيقى]",
            "start": 748.36,
            "duration": 8.169
        }
    ]
}