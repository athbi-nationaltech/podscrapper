        {
            "text": "الى امريكا فبدات الثوره الجنسيه في عام",
            "start": 382.24,
            "duration": 8.04
        },
        {
            "text": "1960 في الولايات المتحده الامريكيه وصار",
            "start": 386.68,
            "duration": 7.079
        },
        {
            "text": "الامريكان يقولون ان كل فرد له الحق",
            "start": 390.28,
            "duration": 8.28
        },
        {
            "text": "بممارسه الجنس مع اي كائن يشاء وان اللبس",
            "start": 393.759,
            "duration": 9.201
        },
        {
            "text": "هو حريه فرديه ولا معنى للاحتشام او الستر",
            "start": 398.56,
            "duration": 8.12
        },
        {
            "text": "لكن البعض منهم كان يعارض ويقول ان هذه",
            "start": 402.96,
            "duration": 7.519
        },
        {
            "text": "الحريه دون قيود راح تنتج عن مشكله وهي",
            "start": 406.68,
            "duration": 7.2
        },
        {
            "text": "الابناء غير الشرعيين لانه ممارسه",
            "start": 410.479,
            "duration": 7.12
        },
        {
            "text": "العلاقات بين الجنسين دون زواج ممكن ينتج",
            "start": 413.88,
            "duration": 7.2
        },
        {
            "text": "عن حمل فشلون عالج هذه المشكله فبدات",
            "start": 417.599,
            "duration": 7.201
        },
        {
            "text": "شركات الادويه في هذه الفتره باصدار حبوب",
            "start": 421.08,
            "duration": 7.48
        },
        {
            "text": "منع الحمل بشكل مكثف وصارت هذه الحبوب",
            "start": 424.8,
            "duration": 5.48
        },
        {
            "text": "مرخصه في عام",
            "start": 428.56,
            "duration": 5.52
        },
        {
            "text": "1961 وبدات تنتشر فكره ان الحمل لو اصبح",
            "start": 430.28,
            "duration": 7.759
        },
        {
            "text": "خطا بالامكان الاجهاض بكل سهوله ودون",
            "start": 434.08,
            "duration": 8.08
        },
        {
            "text": "مسؤوليه فبرزت بعدها فكره النسويه اللي",
            "start": 438.039,
            "duration": 7.641
        },
        {
            "text": "احتفت بظاهره الام العزباء غير المتزوجه",
            "start": 442.16,
            "duration": 7.28
        },
        {
            "text": "وانتشرت افكار المثليه تحت شعار الحب",
            "start": 445.68,
            "duration": 8.88
        },
        {
            "text": "ينتصر او لوف وينز واللي نسمعه بشكل مستمر",
            "start": 449.44,
            "duration": 8.56
        },
        {
            "text": "في شهر 6 من كل عام لكن شنو سبب اختيارهم",
            "start": 454.56,
            "duration": 7.28
        },
        {
            "text": "لهذا الشهر للتفاخر بميولهم حادثه حانه",
            "start": 458.0,
            "duration": 7.44
        },
        {
            "text": "ستون وول في نيويورك هي سبب اختيار شهر 6",
            "start": 461.84,
            "duration": 6.199
        },
        {
            "text": "تحديدا فكان من عاده المثليين انهم",
            "start": 465.44,
            "duration": 5.36
        },
        {
            "text": "يجتمعون في هذه الحانه تحديدا وهذه",
            "start": 468.039,
            "duration": 6.801
        },
        {
            "text": "التجمعات كانت تحصل بشكل سري لان المثليه",
            "start": 470.8,
            "duration": 6.88
        },
        {
            "text": "كانت بذاك الوقت غير مقبوله قانونيا",
            "start": 474.84,
            "duration": 6.88
        },
        {
            "text": "واجتماعيا في امريكا وفي شهر 6 من عام",
            "start": 477.68,
            "duration": 7.72
        },
        {
            "text": "1969 سمعت الشرطه الامريكيه عن وجود جريمه",
            "start": 481.72,
            "duration": 7.199
        },
        {
            "text": "في هذا المكان فاقتحمت الحانه واشتبكت مع",
            "start": 485.4,
            "duration": 6.359
        },
        {
            "text": "المثليين واعتدى المثليين على الشرطه في",
            "start": 488.919,
            "duration": 6.081
        },
        {
            "text": "هذا الاشتباك فكان المكان مليء بالفوضى",
            "start": 491.759,
            "duration": 6.921
        },
        {
            "text": "ومع وصول الاسناد سيطرت الشرطه على المكان",
            "start": 495.0,
            "duration": 7.479
        },
        {
            "text": "ولكن المؤيدين للانحرافات اتوا من كل مكان",
            "start": 498.68,
            "duration": 7.199
        },
        {
            "text": "لمواجهه الشرطه واثاره الغضب واستمرت",
            "start": 502.479,
            "duration": 6.761
        },
        {
            "text": "الفوضى لايام في الشارع اللي فيه الحانه",
            "start": 505.879,
            "duration": 6.561
        },
        {
            "text": "وبسبب هذه الحادثه ما زال المؤيدين للشدو",
            "start": 509.24,
            "duration": 6.719
        },
        {
            "text": "يستذكرون هذا الحدث وخصوصا شهر سته ليكون",
            "start": 512.44,
            "duration": 7.64
        },
        {
            "text": "هو الشهر المخصص للتفاخر بالميول وكذلك",
            "start": 515.959,
            "duration": 8.241
        },
        {
            "text": "لدعم جميع الشواذ حول العالم فتم تسميته",
            "start": 520.08,
            "duration": 8.0
        },
        {
            "text": "بشهر الفخر او برايد م وهي عباره عن",
            "start": 524.2,
            "duration": 7.199
        },
        {
            "text": "مظاهرات كبيره تحصل كل عام يرفعون فيها",
            "start": 528.08,
            "duration": 6.879
        },
        {
            "text": "علم الشواد لكن السؤال هل الكل اقتنع",
            "start": 531.399,
            "duration": 6.88
        },
        {
            "text": "بالثوره الجنسيه وهل الجميع يؤيد الانحراف",
            "start": 534.959,
            "duration": 6.161
        },
        {
            "text": "عن الفطره هذه ثوره ما اقتنع فيها كل",
            "start": 538.279,
            "duration": 6.081
        },
        {
            "text": "الناس ولذلك زاد النشاط الاعلامي اللي",
            "start": 541.12,
            "duration": 7.2
        },
        {
            "text": "يدعو للتحرر فالاعلام بدا تصوير المراه",
            "start": 544.36,
            "duration": 7.599
        },
        {
            "text": "المحتشمه على انها ممله وقديمه وان المراه",
            "start": 548.32,
            "duration": 6.92
        },
        {
            "text": "الناجحه يجب ان تكون فاتنه في مظهرها",
            "start": 551.959,
            "duration": 7.44
        },
        {
            "text": "وملفته للانظار كل هذا في سبيل نزع مفهوم",
            "start": 555.24,
            "duration": 6.88
        },
        {
            "text": "الحشمه عن المراه اما بالنسبه لنشر فكره",
            "start": 559.399,
            "duration": 5.921
        },
        {
            "text": "الشذوذ تحديدا الاعلام بدا يروج ان",
            "start": 562.12,
            "duration": 6.279
        },
        {
            "text": "العلاقات الشاذه اكثر متعه من الزواج",
            "start": 565.32,
            "duration": 6.88
        },
        {
            "text": "الطبيعي لان الزواج مسؤوليه وابناء اما",
            "start": 568.399,
            "duration": 6.721
        },
        {
            "text": "الممارسه مع نفس الجنس فهي لذه من غير",
            "start": 572.2,
            "duration": 6.879
        },
        {
            "text": "مسؤوليه ومع الضخ الاعلامي الضخم والمستمر",
            "start": 575.12,
            "duration": 7.08
        },
        {
            "text": "اقتنعت شريحه واسعه من المجتمع الامريكي",
            "start": 579.079,
            "duration": 6.841
        },
        {
            "text": "وانتشرت فيها الظواهر الجنسيه هذه الظواهر",
            "start": 582.2,
            "duration": 7.879
        },
        {
            "text": "كانت مثل كره الثلج تكبر تدريجيا ويصعب",
            "start": 585.92,
            "duration": 7.479
        },
        {
            "text": "ايقاف وفي اواخر السبعينات ظهر اختراع",
            "start": 590.079,
            "duration": 7.521
        },
        {
            "text": "خطير نتيجه هذه الثوره هذا الاختراع سموه",
            "start": 593.399,
            "duration": 8.081
        },
        {
            "text": "الباحثين اليوم انه هو وباء العصر الحديث",
            "start": 597.6,
            "duration": 8.52
        },
        {
            "text": "الا وهو الاباحيه بعد الثوره برزت شخصيات",
            "start": 601.48,
            "duration": 8.599
        },
        {
            "text": "نسائيه تصدرت الصحف والافلام والاعلانات",
            "start": 606.12,
            "duration": 7.44
        },
        {
            "text": "وكان يتم تصوير النساء بمظهر غير مسبوق",
            "start": 610.079,
            "duration": 8.281
        },
        {
            "text": "بهدف اثاره المتلقي فكانت هذه البوابه",
            "start": 613.56,
            "duration": 9.2
        },
        {
            "text": "لتصنيع الاباحيه وتدريجيا اصبحت الاباحيه",
            "start": 618.36,
            "duration": 8.64
        },
        {
            "text": "تنشر دون خجل وكان انتشارها بين الناس",
            "start": 622.76,
            "duration": 7.199
        },
        {
            "text": "بشكل كبير وعشان ندرك حجم التغيير اللي",
            "start": 627.0,
            "duration": 6.279
        },
        {
            "text": "حدث فلو انك اخبرت رجل عاش قبل الثوره",
            "start": 629.959,
            "duration": 6.641
        },
        {
            "text": "الجنسيه ان اعلانات الافلام الاباحيه تعرض",
            "start": 633.279,
            "duration": 7.0
        },
        {
            "text": "في التايم سكوير مستحيل انه يصدق كلامك",
            "start": 636.6,
            "duration": 7.12
        },
        {
            "text": "ولكن فعلا في السبعينات عرضت اعلانات",
            "start": 640.279,
            "duration": 6.721
        },
        {
            "text": "الافلام الاباحيه في ساحات التايم سكوير",
            "start": 643.72,
            "duration": 6.88
        },
        {
            "text": "في نيويورك واصبحت الافلام تعرض في قاعات",
            "start": 647.0,
            "duration": 7.6
        },
        {
            "text": "السنما وتحقق ايرادات ضخمه بسبب حضور",
            "start": 650.6,
            "duration": 7.08
        },
        {
            "text": "الناس ولكن انقطعت هذه الافلام في فتره",
            "start": 654.6,
            "duration": 6.799
        },
        {
            "text": "الثمانينات لسببه اولا لان الناس بدات",
            "start": 657.68,
            "duration": 7.48
        },
        {
            "text": "تتابعها على اجهزتها الخاصه ثانيا انتشرت",
            "start": 661.399,
            "duration": 7.961
        },
        {
            "text": "دراسات علميه تظهر ضررها الصحي فانقطعت عن",
            "start": 665.16,
            "duration": 8.479
        },
        {
            "text": "السينما لكنها ما زالت الى اليوم تنتج",
            "start": 669.36,
            "duration": 8.2
        },
        {
            "text": "وتنشر رغم ضررها الصحي والشركات اللي تصنع",
            "start": 673.639,
            "duration": 7.521
        },
        {
            "text": "الاباحيه تحقق المليارات من الدولارات كل",
            "start": 677.56,
            "duration": 7.6
        },
        {
            "text": "عام وتجني ارباح اكبر من اغلب شركات",
            "start": 681.16,
            "duration": 7.919
        },
        {
            "text": "العالم الثوره الجنسيه غيرت الاولويات عند",
            "start": 685.16,
            "duration": 8.04
        },
        {
            "text": "الناس فصارت اللذه هي المطلب الاول عندهم",
            "start": 689.079,
            "duration": 7.521
        },
        {
            "text": "كتب عنها رجل مصري عاش في هذه الثوره في",
            "start": 693.2,
            "duration": 7.079
        },
        {
            "text": "امريكا وهو الدكتور والمفكر عبد الوهاب",
            "start": 696.6,
            "duration": 6.6
        },
        {
            "text": "المسيري رحمه الله عليه فيقول ان تفكير",
            "start": 700.279,
            "duration": 6.161
        },
        {
            "text": "الامريكان تغير بهذه المرحله واصبحوا",
            "start": 703.2,
            "duration": 7.12
        },
        {
            "text": "يفضلون اللذه على المسؤوليه والفردان على",
            "start": 706.44,
            "duration": 8.079
        },
        {
            "text": "العائله واصبح الفرد الامريكي لا يفكر الا",
            "start": 710.32,
            "duration": 9.24
        },
        {
            "text": "بشيء واحد الا وهو اللذه يقول الدكتور ان",
            "start": 714.519,
            "duration": 7.88
        },
        {
            "text": "الدفاع الشرس عن الشذوذ الجنسي ليس دعوه",
            "start": 719.56,
            "duration": 7.92
        },
        {
            "text": "للتسامح او تفهم وضع الشواذ جنسيا بل هو",
            "start": 722.399,
            "duration": 8.56
        },
        {
            "text": "هجوم شرس على الطبيعه البشريه بالاضافه",
            "start": 727.48,
            "duration": 6.76
        },
        {
            "text": "لنشر حبوب منع الحمل والحملات الداعمه",
            "start": 730.959,
            "duration": 7.0
        },
        {
            "text": "للاجهاض باسم حقوق المراه فالاعلام تعمد",
            "start": 734.24,
            "duration": 6.64
        },
        {
            "text": "يحرك غريزه الانسان سواء في الاعلانات او",
            "start": 737.959,
            "duration": 6.401
        },
        {
            "text": "الافلام فالاعلان البارزه يتصدرها عارضات",
            "start": 740.88,
            "duration": 6.36
        },
        {
            "text": "الازياء وافلام الهوليوود لا تخلو من",
            "start": 744.36,
            "duration": 6.479
        },
        {
            "text": "مشاهد فيها اثاره للمشاهد والسينما كان",
            "start": 747.24,
            "duration": 5.959
        },
        {
            "text": "لها دور كبير بانها تعود الناس على",
            "start": 750.839,
            "duration": 5.521
        },
        {
            "text": "المثليه فكانوا منتجين الافلام يحرصون على",
            "start": 753.199,
            "duration": 7.0
        },
        {
            "text": "وضع الشخصيات شاذه في العمل لان الكثير من",
            "start": 756.36,
            "duration": 7.0
        },
        {
            "text": "التجار راح يدعمون هذا الفيلم بالمال",
            "start": 760.199,
            "duration": 6.801
        },
        {
            "text": "وكانوا يحرصون ان هذه الشخصيه الشاذه تكون",
            "start": 763.36,
            "duration": 7.64
        },
        {
            "text": "طيبه وظريفه واحيانا مظلومه بحيث تعتاد",
            "start": 767.0,
            "duration": 7.92
        },
        {
            "text": "الناس على قبول هذا النموذج فتدرج اعتاد",
            "start": 771.0,
            "duration": 7.16
        },
        {
            "text": "الناس على الشذوذ وتحول المجتمع الامريكي",
            "start": 774.92,
            "duration": 7.279
        },
        {
            "text": "من مع ض الى مؤيد الى ان اصبحت قانونيه",
            "start": 778.16,
            "duration": 7.08
        },
        {
            "text": "تحت حكم الرئيس الامريكي السابق باراك",
            "start": 782.199,
            "duration": 7.281
        },
        {
            "text": "اوباما اللي اقر في سنه 2008 ان الزواج من",
            "start": 785.24,
            "duration": 7.92
        },
        {
            "text": "نفس الجنس هو قانوني ومباح تقبل هذا",
            "start": 789.48,
            "duration": 6.799
        },
        {
            "text": "الانحراف ما اقتصر على امريكا لان الحضاره",
            "start": 793.16,
            "duration": 6.119
        },
        {
            "text": "الغربيه تحاول تصدير افكارها الى العالم",
            "start": 796.279,
            "duration": 6.441
        },
        {
            "text": "كله فنلاحظ ان لا يوجد فيلم يحصد الجوائز",
            "start": 799.279,
            "duration": 7.761
        },
        {
            "text": "العالميه الا بوجود شخصيات مثليه او افكار",
            "start": 802.72,
            "duration": 7.64
        },
        {
            "text": "منحرفه وحتى الشركات العالميه تحول",
            "start": 807.04,
            "duration": 6.479
        },
        {
            "text": "شعاراتها الى الوان لتكسب تعاطف هذه",
            "start": 810.36,
            "duration": 6.24
        },
        {
            "text": "الشريحه واموالها ولو كانت هذه الشركات",
            "start": 813.519,
            "duration": 6.641
        },
        {
            "text": "حريصه على حقوق الاقليات راينا موقفهم مع",
            "start": 816.6,
            "duration": 7.039
        },
        {
            "text": "اهل غزه على الاقل او غيرهم ممن يتعرضون",
            "start": 820.16,
            "duration": 7.479
        },
        {
            "text": "لابشع انواع العنف وابسط حقوقه يستحق",
            "start": 823.639,
            "duration": 7.361
        },
        {
            "text": "الدعم والتضامن تخيلوا ان هذه المحاوله",
            "start": 827.639,
            "duration": 6.721
        },
        {
            "text": "لنشر الانحرافات ما مست بس البالغين فال",
            "start": 831.0,
            "duration": 7.56
        },
        {
            "text": "المساله تمددت وشملت حتى محتوى الطفل فديز",
            "start": 834.36,
            "duration": 8.2
        },
        {
            "text": "ني نشرت اكثر من عمل فيه ابطال مثليين",
            "start": 838.56,
            "duration": 7.199
        },
        {
            "text": "والمناهج الدراسيه صار فيها دعم صريح",
            "start": 842.56,
            "duration": 7.44
        },
        {
            "text": "للشذوذ وفي اوروبا ما تقدر تقول لابنك انك",
            "start": 845.759,
            "duration": 7.801
        },
        {
            "text": "لا تؤيد الشذوذ لان الشرطه لو عرفت راح",
            "start": 850.0,
            "duration": 7.48
        },
        {
            "text": "تقبض عليك انتهاك فطره الطفل وصلت الى",
            "start": 853.56,
            "duration": 7.959
        },
        {
            "text": "مرحله ابعد ابيكم معاي تشوفون هذا المشهد",
            "start": 857.48,
            "duration": 7.599
        },
        {
            "text": "هذه المظاهره يقولون اصحابها انهم يريدون",
            "start": 861.519,
            "duration": 6.601
        },
        {
            "text": "حريه الممارسه مع الاطفال وانهم يطالبون",
            "start": 865.079,
            "duration": 6.44
        },
        {
            "text": "بحقوق م كما نال الملونين حقوقهم فاذا",
            "start": 868.12,
            "duration": 6.279
        },
        {
            "text": "كانت الحريه دون قيود او ضوابط فلا مانع",
            "start": 871.519,
            "duration": 5.841
        },
        {
            "text": "انه يختار الطفل هذه العلاقه وخصوصا وانه",
            "start": 874.399,
            "duration": 6.161
        },
        {
            "text": "اصبح يختار اذا كان يريد اجراء عمليه",
            "start": 877.36,
            "duration": 6.479
        },
        {
            "text": "تغيير جنسه فتحت المثليه هذا الباب",
            "start": 880.56,
            "duration": 6.639
        },
        {
            "text": "والانحرافات تزداد كل يوم من بيدوفيليا",
            "start": 883.839,
            "duration": 7.041
        },
        {
            "text": "وتحويل الجندر وكذلك وصلت الى الحيوانات",
            "start": 887.199,
            "duration": 6.601
        },
        {
            "text": "التي لم تسلم في هذه الفوضه اللي يعيشها",
            "start": 890.88,
            "duration": 6.36
        },
        {
            "text": "العالم ما هي النظره الاسلاميه المتزنه في",
            "start": 893.8,
            "duration": 7.12
        },
        {
            "text": "القران الكريم قرا نا قصه لوط عليه السلام",
            "start": 897.24,
            "duration": 7.44
        },
        {
            "text": "سيدنا لوط ذهب الى قريه يمارس اصحابها",
            "start": 900.92,
            "duration": 7.159
        },
        {
            "text": "الشذوذ وكانوا ينظرون الى شخص الطبيعي على",
            "start": 904.68,
            "duration": 7.159
        },
        {
            "text": "انه يسير بشكل خاطئ فنصح سيدنا لوط عليه",
            "start": 908.079,
            "duration": 7.601
        },
        {
            "text": "السلام وحاول يرشدهم ويديهم الى الدرب",
            "start": 911.839,
            "duration": 7.841
        },
        {
            "text": "السليم لكن من شده ضلال قوم لوط كانوا",
            "start": 915.68,
            "duration": 8.279
        },
        {
            "text": "يعادون ويهاجمون وكان عذرهم بمهاجمه لوط",
            "start": 919.68,
            "duration": 8.2
        },
        {
            "text": "عليه السلام عباره ذكرت في القران الكريم",
            "start": 923.959,
            "duration": 8.961
        },
        {
            "text": "انهم اناس يتطهرون تخيل معاي انهم يعادونه",
            "start": 927.88,
            "duration": 9.0
        },
        {
            "text": "لانه طاهر وصل الحال بلوط عليه السلام انه",
            "start": 932.92,
            "duration": 7.919
        },
        {
            "text": "كان يقترح عليهم بالزواج ببناته بمقابل",
            "start": 936.88,
            "duration": 7.36
        },
        {
            "text": "انهم يعفون انفسهم ويتركون ممارساتهم",
            "start": 940.839,
            "duration": 8.0
        },
        {
            "text": "لكنهم استمروا بانحرافه وكانوا يعتدون على",
            "start": 944.24,
            "duration": 8.76
        },
        {
            "text": "اي صبي يمر على القريه ويمارسون معه",
            "start": 948.839,
            "duration": 7.601
        },
        {
            "text": "الفاحشه سواء كان يريد او لا يريد فنجى",
            "start": 953.0,
            "duration": 6.48
        },
        {
            "text": "الله سبحانه وتعالى لوط وب ناه من هذا",
            "start": 956.44,
            "duration": 6.639
        },
        {
            "text": "القوم وانزل عليهم العذاب ومن اللي شملهم",
            "start": 959.48,
            "duration": 7.039
        },
        {
            "text": "العذاب امراه لوط اللي كانت متسامحه مع",
            "start": 963.079,
            "duration": 6.281
        },
        {
            "text": "افعالهم في بدايه الحلقه وعدكم اني اقول",
            "start": 966.519,
            "duration": 6.361
        },
        {
            "text": "قصه واقعيه حصلت في المجتمع الاسلامي هذا",
            "start": 969.36,
            "duration": 7.8
        },
        {
            "text": "طفل نشا في عائله محافظه حوله الكثير من",
            "start": 972.88,
            "duration": 7.6
        },
        {
            "text": "الاخوات وهو الولد الوحيد امه كانت حريصه",
            "start": 977.16,
            "duration": 6.28
        },
        {
            "text": "انه يواظب على الصلاه ويقرا القران مع",
            "start": 980.48,
            "duration": 6.76
        },
        {
            "text": "الايام تشكلت عنده سلوكيات انثويه بسبب",
            "start": 983.44,
            "duration": 7.0
        },
        {
            "text": "علاقته مع اخوات اللي حوله وكان والده",
            "start": 987.24,
            "duration": 7.24
        },
        {
            "text": "بعيد عنه ومع وصوله لعمر انه لازم يدخل",
            "start": 990.44,
            "duration": 8.72
        },
        {
            "text": "المدرسه فاجا والدته بكلام صادم فقال لها",
            "start": 994.48,
            "duration": 8.599
        },
        {
            "text": "ما ابي ادخل مدرسه للاولاد وبعد اول يوم",
            "start": 999.16,
            "duration": 7.919
        },
        {
            "text": "دراسي قال كلمه مؤثره لوالدته هذيل",
            "start": 1003.079,
            "duration": 7.56
        },
        {
            "text": "الاولاد ما يشبهوني وللاسف هذا الطفل",
            "start": 1007.079,
            "duration": 7.161
        },
        {
            "text": "المحافظ اللي والدته تعبت عليه لما كبر",
            "start": 1010.639,
            "duration": 7.2
        },
        {
            "text": "ومع تاثره بالاعلام الغربي ووجود السوشيال",
            "start": 1014.24,
            "duration": 6.8
        },
        {
            "text": "ميديا في عالم لمنا اليوم قرر تغيير جنسه",
            "start": 1017.839,
            "duration": 6.281
        },
        {
            "text": "وامتثالا لسلوكيات المثليين في المجتمع",
            "start": 1021.04,
            "duration": 7.279
        },
        {
            "text": "المثلي اعلنها تفاخرا بتغيير هذا الميول",
            "start": 1024.12,
            "duration": 7.24
        },
        {
            "text": "النظره الاسلاميه هي ان الانسان يجاهد",
            "start": 1028.319,
            "duration": 7.72
        },
        {
            "text": "نفسه حتى ان وجد في نفسه ميول معينه هذه",
            "start": 1031.36,
            "duration": 7.679
        },
        {
            "text": "المجاهده مو خاصه لمن يملك شعور منحرف",
            "start": 1036.039,
            "duration": 7.241
        },
        {
            "text": "مثلا بل هي لكل مسلم والفوضى اللي دخل",
            "start": 1039.039,
            "duration": 7.441
        },
        {
            "text": "فيها العالم الحديث يضبطها الدين اللي",
            "start": 1043.28,
            "duration": 6.96
        },
        {
            "text": "اعطانا نظره متزنه والله سبحانه وتعالى هو",
            "start": 1046.48,
            "duration": 7.4
        },
        {
            "text": "خالق الانسان وهو اعلم بفطرته وفي العصر",
            "start": 1050.24,
            "duration": 7.559
        },
        {
            "text": "الحديث ضاعت البوصله وبعد الثوره الجنسيه",
            "start": 1053.88,
            "duration": 8.4
        },
        {
            "text": "اصبح كل شيء مباح وهني نطرح سؤال مهم شلون",
            "start": 1057.799,
            "duration": 7.401
        },
        {
            "text": "عرف المباح والممنوع وما هي البوصله",
            "start": 1062.28,
            "duration": 5.84
        },
        {
            "text": "الاخلاقيه اللي نسير عليها لو اعتمدنا على",
            "start": 1065.2,
            "duration": 6.04
        },
        {
            "text": "العلم التجريبي فالعلم لا يتكلم في",
            "start": 1068.12,
            "duration": 6.4
        },
        {
            "text": "المسائل الاخلاقيه غير الماديه فالبعض",
            "start": 1071.24,
            "duration": 7.559
        },
        {
            "text": "يزعم ان الحيوانات علميا تملك ميول مثليه",
            "start": 1074.52,
            "duration": 8.0
        },
        {
            "text": "ولكن هذه العباره لا يمكن ان تكون حجه لان",
            "start": 1078.799,
            "duration": 6.961
        },
        {
            "text": "الله سبحانه وتعالى ميز الانسان بالعقل",
            "start": 1082.52,
            "duration": 6.84
        },
        {
            "text": "فالانسان ما يسير حسب الغريزه فقط ولو",
            "start": 1085.76,
            "duration": 6.799
        },
        {
            "text": "اعتمدنا على الاقتصاد في تحديد اخلاقيات",
            "start": 1089.36,
            "duration": 7.12
        },
        {
            "text": "فهذه مشكله اكبر فالبعض للاسف يطالب بنشر",
            "start": 1092.559,
            "duration": 7.681
        },
        {
            "text": "المثليه لانها تعزز من الاقتصاد مع ان",
            "start": 1096.48,
            "duration": 8.36
        },
        {
            "text": "هناك دول ذات اقتصاد ضخم لا تبيح المثليه",
            "start": 1100.24,
            "duration": 8.28
        },
        {
            "text": "والكثير من الامور قد تكون مفيده اقتصاديا",
            "start": 1104.84,
            "duration": 7.839
        },
        {
            "text": "لكنها غير اخلاقيه ولا يمكن الاعتماد كذلك",
            "start": 1108.52,
            "duration": 7.279
        },
        {
            "text": "على النفس البشريه في تحديد الاخلاقيات",
            "start": 1112.679,
            "duration": 7.201
        },
        {
            "text": "لانها تسير على الاهواء وكذلك لا يمكن",
            "start": 1115.799,
            "duration": 6.401
        },
        {
            "text": "الاعتماد على الاكثريه فالظواهر",
            "start": 1119.88,
            "duration": 6.159
        },
        {
            "text": "الاجتماعيه السيئه كانت بتاييد اكثر",
            "start": 1122.2,
            "duration": 7.28
        },
        {
            "text": "المجتمع مثل ود البنات وغيرها فالقاع",
            "start": 1126.039,
            "duration": 6.401
        },
        {
            "text": "الوحيده اللي ممكن نستند عليها ونحدد من",
            "start": 1129.48,
            "duration": 6.04
        },
        {
            "text": "خلالها الامر الاخلاقي واللا اخلاقي هي",
            "start": 1132.44,
            "duration": 6.76
        },
        {
            "text": "الدين الصحيح الا وهو الاسلام فخالق",
            "start": 1135.52,
            "duration": 8.2
        },
        {
            "text": "الانسان ادرى بمصلحته والدين يهذب رغبات",
            "start": 1139.2,
            "duration": 8.08
        },
        {
            "text": "الانسان وشهواته وهو الثابت الاخلاق",
            "start": 1143.72,
            "duration": 8.4
        },
        {
            "text": "الوحيد وسط كل هذه التغيرات",
            "start": 1147.28,
            "duration": 4.84
        },
        {
            "text": "[موسيقى]",
            "start": 1153.9,
            "duration": 6.53
        },
        {
            "text": "[موسيقى]",
            "start": 1163.04,
            "duration": 3.9
        },
        {
            "text": "والانحرافات",
            "start": 1167.24,
            "duration": 17.48
        },
        {
            "text": "[موسيقى]",
            "start": 1169.83,
            "duration": 14.89
        },
        {
            "text": "[موسيقى]",
            "start": 1194.82,
            "duration": 3.11
        },
        {
            "text": "ه",
            "start": 1199.679,
            "duration": 3.0
        }
    ]
}