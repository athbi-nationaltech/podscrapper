        },
        {
            "text": "السياره وبدات صحته تتدهور تدريجيا وفي",
            "start": 377.4,
            "duration": 6.88
        },
        {
            "text": "دقائق معدوده وقبل وصوله لمستشفى تعرض",
            "start": 380.759,
            "duration": 6.84
        },
        {
            "text": "للشلل التام ومات فالسؤال شنو الماده",
            "start": 384.28,
            "duration": 6.68
        },
        {
            "text": "الغريبه اللي قضت عليه ب قائق ومنو النساء",
            "start": 387.599,
            "duration": 6.521
        },
        {
            "text": "اللي نفذوا هذه الجريمه الغريبه الجواب",
            "start": 390.96,
            "duration": 6.72
        },
        {
            "text": "على السؤالين كشف لنا الكثير من الاسرار",
            "start": 394.12,
            "duration": 8.68
        },
        {
            "text": "لواحده من اغرب الجرائم في هذا القرن",
            "start": 397.68,
            "duration": 5.12
        },
        {
            "text": "اعثرتك التحقيقات اللي كشفت عن قصه غير",
            "start": 403.0,
            "duration": 6.4
        },
        {
            "text": "متوقعه قبل لا اقول لكم القصه الصادمه ابي",
            "start": 406.56,
            "duration": 6.52
        },
        {
            "text": "منكم دعم هذه القناه بالاشتراك واللايك",
            "start": 409.4,
            "duration": 6.6
        },
        {
            "text": "ومشاركه الحلقه مع الناس اللي حولكم اما",
            "start": 413.08,
            "duration": 6.32
        },
        {
            "text": "الان لنروي قصه البنتين في التحقيقات",
            "start": 416.0,
            "duration": 6.199
        },
        {
            "text": "اسالوا البنات اول سؤال عن دوافع هذا",
            "start": 419.4,
            "duration": 5.56
        },
        {
            "text": "الفعل فتقول واحده من البنات ان القصه",
            "start": 422.199,
            "duration": 6.801
        },
        {
            "text": "ابتدات لما اتصل عليها شخص اسمه جيمس وقال",
            "start": 424.96,
            "duration": 7.519
        },
        {
            "text": "لها انه يشتغل كمنتج تلفزيوني فتقول البنت",
            "start": 429.0,
            "duration": 7.319
        },
        {
            "text": "ان جيمس كان رجل طيب وخفيف دم وعرض عليها",
            "start": 432.479,
            "duration": 7.16
        },
        {
            "text": "انها تكون في احد الاعمال الفنيه وطبعا",
            "start": 436.319,
            "duration": 6.681
        },
        {
            "text": "بمقابل مادي فقالت له شنو نوع العمل فقال",
            "start": 439.639,
            "duration": 7.28
        },
        {
            "text": "لها برنامج كوميدي وفكرته عمل مقالب للناس",
            "start": 443.0,
            "duration": 6.44
        },
        {
            "text": "فوافقت على العرض وبداوا العمل على",
            "start": 446.919,
            "duration": 5.601
        },
        {
            "text": "البرنامج فطلب المنتج من البنات استعمال",
            "start": 449.44,
            "duration": 5.879
        },
        {
            "text": "مستحضرات تجميليه تخفي هويتهم عن",
            "start": 452.52,
            "duration": 5.079
        },
        {
            "text": "الكاميرات اللي راح يستعملونها في تصوير",
            "start": 455.319,
            "duration": 5.32
        },
        {
            "text": "البرنامج وفعلا عملوا الكثير من المقالب",
            "start": 457.599,
            "duration": 6.481
        },
        {
            "text": "واللي كانت ناجحه ومناسبه لفكره البرنامج",
            "start": 460.639,
            "duration": 7.28
        },
        {
            "text": "واشتغلوا لمده اشهر وكان الوضع جدا طبيعي",
            "start": 464.08,
            "duration": 7.28
        },
        {
            "text": "وفي يوم من الايام طلب منهم المنتج حضور",
            "start": 467.919,
            "duration": 7.041
        },
        {
            "text": "اجتماع مهم في المطار لتحديد الشخص اللي",
            "start": 471.36,
            "duration": 6.559
        },
        {
            "text": "راح يعملوا له مقلب في هذا اليوم بحسب",
            "start": 474.96,
            "duration": 5.679
        },
        {
            "text": "البنت كانت تقول ان في هذا اليوم كان",
            "start": 477.919,
            "duration": 5.441
        },
        {
            "text": "المطار جدا مزدحم قال له المنتج اذا",
            "start": 480.639,
            "duration": 5.721
        },
        {
            "text": "الاداء كان مميز بهذه المره",
            "start": 483.36,
            "duration": 6.679
        },
        {
            "text": "فالاداريس جدا ضخمه وقال لهم في الاجتماع",
            "start": 486.36,
            "duration": 7.6
        },
        {
            "text": "ان الهدف اليوم راح يكون لشخص معين فسالت",
            "start": 490.039,
            "duration": 7.401
        },
        {
            "text": "اي شخص فيهم فاشار لها على رجل يلبس جاكيت",
            "start": 493.96,
            "duration": 6.919
        },
        {
            "text": "رمادي وبعدها المنتج حط على ايدها زيت مثل",
            "start": 497.44,
            "duration": 6.52
        },
        {
            "text": "ما هو المعتاد واعطاها قطعه قماش عليها",
            "start": 500.879,
            "duration": 6.361
        },
        {
            "text": "ماده سائله لكن هذه الماده مو ماده عاديه",
            "start": 503.96,
            "duration": 6.88
        },
        {
            "text": "مثل ما ظنت البنت كانت ه هذه الماده مميته",
            "start": 507.24,
            "duration": 7.84
        },
        {
            "text": "للاعصاب وهي ممنوعه ومحرمه دوليا ويخاف",
            "start": 510.84,
            "duration": 6.639
        },
        {
            "text": "منها الجيش الامريكي راح نتكلم عنها",
            "start": 515.08,
            "duration": 5.16
        },
        {
            "text": "بالتفصيل بعد شوي كان الاخ الاكبر متجه",
            "start": 517.479,
            "duration": 6.201
        },
        {
            "text": "لطياره فغضب المنتج على البنتين وقال لهم",
            "start": 520.24,
            "duration": 6.719
        },
        {
            "text": "لازم نبدا الحين قبل ركوبه للطياره اركضوا",
            "start": 523.68,
            "duration": 6.56
        },
        {
            "text": "البنت ووصلوا لصاحب الجاكيت الرمادي وهو",
            "start": 526.959,
            "duration": 6.481
        },
        {
            "text": "الاخ الاكبر للرئيس الكوري الشمالي ووضعوا",
            "start": 530.24,
            "duration": 6.76
        },
        {
            "text": "على وجهه قطعه قماش فيها الماده السائله",
            "start": 533.44,
            "duration": 6.56
        },
        {
            "text": "وبعد اركضوا بعيدا واتركوا المكان واغسلوا",
            "start": 537.0,
            "duration": 6.079
        },
        {
            "text": "ايدهم وغادروا المطار لان المقلب حسب",
            "start": 540.0,
            "duration": 7.36
        },
        {
            "text": "تصورهم تم تنفيذه بنفس الوقت شعر اخ زعيم",
            "start": 543.079,
            "duration": 8.2
        },
        {
            "text": "بشعور غريب شعور اول مره يصيبه في حياته",
            "start": 547.36,
            "duration": 7.159
        },
        {
            "text": "فاتجه الى موظفين المطار لطلب المساعده",
            "start": 551.279,
            "duration": 6.761
        },
        {
            "text": "فاخذوه الموظفين الى عياده المطار واستلقى",
            "start": 554.519,
            "duration": 6.88
        },
        {
            "text": "على ظهره للفحص والعلاج وفي هذه الاثناء",
            "start": 558.04,
            "duration": 6.84
        },
        {
            "text": "مر رجل على العياده والقى نظره فيها",
            "start": 561.399,
            "duration": 7.12
        },
        {
            "text": "للتاكد ان الجريمه تم تنفيذها وان الضحيه",
            "start": 564.88,
            "duration": 6.56
        },
        {
            "text": "موجوده في العياده هذا الرجل هو جزء من",
            "start": 568.519,
            "duration": 6.041
        },
        {
            "text": "الفريق والمسؤول عن العمليه كان المطار",
            "start": 571.44,
            "duration": 6.36
        },
        {
            "text": "فيه عده افراد منهم من يراقب العمليه",
            "start": 574.56,
            "duration": 6.76
        },
        {
            "text": "ومنهم كيميائي يحمل الماده المميته وكذلك",
            "start": 577.8,
            "duration": 6.96
        },
        {
            "text": "سائق خاص لنقل جميع الافراد وقائد الفريق",
            "start": 581.32,
            "duration": 7.28
        },
        {
            "text": "يلقبونه الجد الاكبر فكانت الجريمه تحت",
            "start": 584.76,
            "duration": 7.24
        },
        {
            "text": "انظار فريق كامل من المتدربين والمتمرسين",
            "start": 588.6,
            "duration": 7.08
        },
        {
            "text": "لهذا اليوم وصارت عيون كيم جونج نام تحترق",
            "start": 592.0,
            "duration": 6.6
        },
        {
            "text": "بشكل فضيع ووصلت الاسعاف لنقله الى",
            "start": 595.68,
            "duration": 6.88
        },
        {
            "text": "المستشفى وبعد ركوب الاسعاف بدقائق تعرض",
            "start": 598.6,
            "duration": 7.4
        },
        {
            "text": "لشلل تام وصار عاجز عن الحركه وبعدها",
            "start": 602.56,
            "duration": 8.48
        },
        {
            "text": "بلحظات فارغ الحياه ومات بشكل سريع ومفاجئ",
            "start": 606.0,
            "duration": 9.0
        },
        {
            "text": "تخيلوا كل هذا حدث من دون علم النساء اللي",
            "start": 611.04,
            "duration": 7.44
        },
        {
            "text": "شاركوا بجريمه الاغتيال لانهم ظنوا انهم",
            "start": 615.0,
            "duration": 6.839
        },
        {
            "text": "نفذوا المقلب بنجاح وعادوا الى بيوتهم في",
            "start": 618.48,
            "duration": 6.16
        },
        {
            "text": "التحقيقات تقول احد البنتين اللي اقتلوا",
            "start": 621.839,
            "duration": 5.521
        },
        {
            "text": "الاخ الاكبر للرئيس الكوري الشمالي تقول",
            "start": 624.64,
            "duration": 5.0
        },
        {
            "text": "الشرطه الماليزيه اطرقوا على باب بيتها",
            "start": 627.36,
            "duration": 5.36
        },
        {
            "text": "وسالوها هل كنت في المطار في تلك الليله",
            "start": 629.64,
            "duration": 6.4
        },
        {
            "text": "فقالت لهم نعم وسالتهم شنو السبب فقالوا",
            "start": 632.72,
            "duration": 6.4
        },
        {
            "text": "لها ما عرفتي التهمه الموجهه لك فسالوها",
            "start": 636.04,
            "duration": 6.12
        },
        {
            "text": "هذا السؤال وهي فعلا ما عندها اي فكره",
            "start": 639.12,
            "duration": 6.839
        },
        {
            "text": "انها اتالت اخ رئيس الدوله لما قالوا لها",
            "start": 642.16,
            "duration": 7.239
        },
        {
            "text": "عن التهمه انصدمت بشكل كبير وتم اخذ",
            "start": 645.959,
            "duration": 6.161
        },
        {
            "text": "البنتين الى التحقيقات ومع التحقيقات",
            "start": 649.399,
            "duration": 6.081
        },
        {
            "text": "اظهروا البنتين جميع الادله اللي تثبت",
            "start": 652.12,
            "duration": 6.12
        },
        {
            "text": "انهم ما يملكون اي فكره عن تنفيذهم",
            "start": 655.48,
            "duration": 6.2
        },
        {
            "text": "لاغتيال سياسي فكانوا يظنون انهم ينفذون",
            "start": 658.24,
            "duration": 6.68
        },
        {
            "text": "مقلب لرجل عادي في المطار في هذه الاثناء",
            "start": 661.68,
            "duration": 5.839
        },
        {
            "text": "القت الشرطه الماليزيه القبض على",
            "start": 664.92,
            "duration": 5.719
        },
        {
            "text": "الكيميائي المشارك في هذه العمليه واللي",
            "start": 667.519,
            "duration": 6.201
        },
        {
            "text": "كان يحمل الماده القاتله في جيبه وفي",
            "start": 670.639,
            "duration": 5.961
        },
        {
            "text": "التحقيقات تم الاكتشاف انه تم تعيينه من",
            "start": 673.72,
            "duration": 6.28
        },
        {
            "text": "قبل سفاره كوريا الشماليه لتنفيذ خطه",
            "start": 676.6,
            "duration": 5.84
        },
        {
            "text": "التخلص من اخ زعيم كوريا الشماليه",
            "start": 680.0,
            "duration": 5.399
        },
        {
            "text": "واكتشفوا ان السلطه الكوريه كانت فخوره",
            "start": 682.44,
            "duration": 5.959
        },
        {
            "text": "فيه بمساهمته في هذا الاغتيال فوجدت",
            "start": 685.399,
            "duration": 5.961
        },
        {
            "text": "الشرطه ليزيه محادثات ما بينه وبين",
            "start": 688.399,
            "duration": 9.041
        },
        {
            "text": "الكوريين فيها جميع تفاصيل واسرار العمليه",
            "start": 691.36,
            "duration": 6.08
        },
        {
            "text": "فالمسالة النقطه المحيره بالنسبه للناس هو",
            "start": 717.959,
            "duration": 6.32
        },
        {
            "text": "السلاح اللي تم استخدامه في هذا الاغتيال",
            "start": 721.399,
            "duration": 5.961
        },
        {
            "text": "شنو هذه الماده اللي تقتل الانسان بهذه",
            "start": 724.279,
            "duration": 6.641
        },
        {
            "text": "السرعه فهي اصابت الرجل بالشلل والموت",
            "start": 727.36,
            "duration": 6.839
        },
        {
            "text": "بدقائق بهذه الفتره كان وزير الصحه",
            "start": 730.92,
            "duration": 6.84
        },
        {
            "text": "الماليزي يعيش تحت ضغوطات كبيره بسبب",
            "start": 734.199,
            "duration": 6.561
        },
        {
            "text": "جريمه الاغتيال الغامضه اللي حصلت في بلده",
            "start": 737.76,
            "duration": 6.68
        },
        {
            "text": "لان المنظومه الصحيه عجزت عن علاج الضحيه",
            "start": 740.76,
            "duration": 7.0
        },
        {
            "text": "فظهر وزير الصحه الماليزي في المؤتمر لشرح",
            "start": 744.44,
            "duration": 6.839
        },
        {
            "text": "كواليس الح الحدثه الغريبه فقال ان الضحيه",
            "start": 747.76,
            "duration": 7.16
        },
        {
            "text": "توفى في سياره الاسعاف ومات خلال ربع ساعه",
            "start": 751.279,
            "duration": 7.441
        },
        {
            "text": "تقريبا ووصف وفاته بانها اليمه وقاسيه",
            "start": 754.92,
            "duration": 7.2
        },
        {
            "text": "بسبب تعرض كيم جون نام للشلل المفاجئ وبعد",
            "start": 758.72,
            "duration": 6.6
        },
        {
            "text": "دراسه المتخصصين للماده اللي اصيب فيها",
            "start": 762.12,
            "duration": 6.24
        },
        {
            "text": "اتضح انها ماده خطيره جدا مثل ما قال",
            "start": 765.32,
            "duration": 6.4
        },
        {
            "text": "الوزير الماده عباره عن سائل بلا لون",
            "start": 768.36,
            "duration": 6.76
        },
        {
            "text": "وقوامها يشبه الزيت الماده يسمونها الجيش",
            "start": 771.72,
            "duration": 6.64
        },
        {
            "text": "الامريكي في اكس ويعتبرونها قادره على",
            "start": 775.12,
            "duration": 6.32
        },
        {
            "text": "القتل بشكل سريع لان الماده اذا اصابتك",
            "start": 778.36,
            "duration": 6.08
        },
        {
            "text": "فال الاعراض تظهر بعد ثواني في اكس هو",
            "start": 781.44,
            "duration": 6.199
        },
        {
            "text": "عباره عن غاز يدمر الاعصاب اذا استنشقته",
            "start": 784.44,
            "duration": 6.639
        },
        {
            "text": "تم تطويره في فتره الخمسينات فالبريفي ذاك",
            "start": 787.639,
            "duration": 6.401
        },
        {
            "text": "الوقت كان يطورون مبيدات الحشرات ومع",
            "start": 791.079,
            "duration": 6.56
        },
        {
            "text": "تطوير المبيدات وصلوا لتركيبات خطيره تقتل",
            "start": 794.04,
            "duration": 7.12
        },
        {
            "text": "البشر وليس الحشرات فقط طريقه عمل الغاز",
            "start": 797.639,
            "duration": 6.601
        },
        {
            "text": "باختصار هو انه يمنع الانزيم اللي يحتاجه",
            "start": 801.16,
            "duration": 6.0
        },
        {
            "text": "الجسم هذا الانزيم وظيفته اراحه الغدد",
            "start": 804.24,
            "duration": 6.159
        },
        {
            "text": "والعضلات فلما يتع الانزيم شنو يصير تصير",
            "start": 807.16,
            "duration": 6.64
        },
        {
            "text": "الغدد والعضلات نشطه بشكل مستمر فالجسم",
            "start": 810.399,
            "duration": 6.761
        },
        {
            "text": "يشعر بالتعب ويفقد القدره على التوازن ومن",
            "start": 813.8,
            "duration": 6.32
        },
        {
            "text": "اخطر مواصفات الماده هي انها ماده من غير",
            "start": 817.16,
            "duration": 5.64
        },
        {
            "text": "روائع فالشخص اللي تعرض له قد يكون",
            "start": 820.12,
            "duration": 6.2
        },
        {
            "text": "استنشقها ولا يعلم ابدا ومن يصاب فيها",
            "start": 822.8,
            "duration": 6.839
        },
        {
            "text": "يتعرض لانخفاض في الدم وعدم وضوح في",
            "start": 826.32,
            "duration": 7.879
        },
        {
            "text": "الرؤيه وضيق التنفس وكذلك احتراق في العين",
            "start": 829.639,
            "duration": 8.241
        },
        {
            "text": "والاخطر ارتفاع بمعدل ضربات القلب واذا",
            "start": 834.199,
            "duration": 6.44
        },
        {
            "text": "اشتدت على الش شخص الحاله الصحيه يصاب",
            "start": 837.88,
            "duration": 6.56
        },
        {
            "text": "بالتشنجات والاغماء والشلل الى ان تتوقف",
            "start": 840.639,
            "duration": 8.681
        },
        {
            "text": "وظائف الجسم ويموت كل هذا في دقائق معدوده",
            "start": 844.44,
            "duration": 7.399
        },
        {
            "text": "بررت بعض وسائل الاعلام والصحف في كوريا",
            "start": 849.32,
            "duration": 5.92
        },
        {
            "text": "الشماليه هذه الجريمه قالت ان الاخ الاكبر",
            "start": 851.839,
            "duration": 6.44
        },
        {
            "text": "كان عميل عند المخابرات الامريكيه السي اي",
            "start": 855.24,
            "duration": 6.64
        },
        {
            "text": "ايه وانه كان يقابلهم ويجلس معاهم وانه",
            "start": 858.279,
            "duration": 7.0
        },
        {
            "text": "سفره لماليزيا كان لهذا السبب وربما لهذا",
            "start": 861.88,
            "duration": 6.079
        },
        {
            "text": "السبب تم قتله لان المخابرات الامريكيه",
            "start": 865.279,
            "duration": 5.56
        },
        {
            "text": "قادره على الاطاحه برؤساء غير مرغوبين",
            "start": 867.959,
            "duration": 6.201
        },
        {
            "text": "فشعر الزعيم الكوري انه امام خطر حقيقي",
            "start": 870.839,
            "duration": 7.56
        },
        {
            "text": "ودبر هذه الجريمه الغريبه لشخص قريب منه",
            "start": 874.16,
            "duration": 8.32
        },
        {
            "text": "الا وهو اخوه مسحوا وجهه بغاز الالفي اكس",
            "start": 878.399,
            "duration": 7.601
        },
        {
            "text": "وانه حياته بدقائق صحيح ان كوريا الشماليه",
            "start": 882.48,
            "duration": 6.4
        },
        {
            "text": "تم تصويرها في الافلام الامريكيه والاخبار",
            "start": 886.0,
            "duration": 6.079
        },
        {
            "text": "التلفزيونيه على انها دوله غريبه الاطوار",
            "start": 888.88,
            "duration": 6.24
        },
        {
            "text": "من ناحيه الحكم وكذلك الشعب هذه الصوره",
            "start": 892.079,
            "duration": 6.641
        },
        {
            "text": "وان كانت غير دقيقه تماما بسبب كثره",
            "start": 895.12,
            "duration": 7.079
        },
        {
            "text": "الاشاعات والاخبار الساخره لكن فيها جزء",
            "start": 898.72,
            "duration": 6.44
        },
        {
            "text": "من الصحه فارتكب الرئيس الكوري الشمالي",
            "start": 902.199,
            "duration": 6.841
        },
        {
            "text": "هذه الجريمه ضد اخوه بطريقه لا تصدق ودفع",
            "start": 905.16,
            "duration": 7.599
        },
        {
            "text": "فيها نساء للقتل وهم لا يملكون اي فكره",
            "start": 909.04,
            "duration": 7.32
        },
        {
            "text": "بهذا المخطط واستخدم فيها سلاح نادر ومحرم",
            "start": 912.759,
            "duration": 6.961
        },
        {
            "text": "دوليا يقتل المصاب بثواني معدوده هذا",
            "start": 916.36,
            "duration": 6.52
        },
        {
            "text": "الرجل الان يقود امبراطوريه اسمها كوريا",
            "start": 919.72,
            "duration": 6.479
        },
        {
            "text": "الشماليه امبراطوريه تملك احدث المعدات",
            "start": 922.88,
            "duration": 7.399
        },
        {
            "text": "العسكريه وتضع راسها براس اكبر دول العالم",
            "start": 926.199,
            "duration": 7.241
        },
        {
            "text": "مثل الولايات المتحده الامريكيه اللي تعود",
            "start": 930.279,
            "duration": 6.441
        },
        {
            "text": "من ابرز الدول اللي تصنع قرارات اليوم",
            "start": 933.44,
            "duration": 8.519
        },
        {
            "text": "كانت هذه قصه اغرب اغتيال في القرن ال",
            "start": 936.72,
            "duration": 5.239
        },
        {
            "text": "[موسيقى]",
            "start": 942.08,
            "duration": 8.209
        },
        {
            "text": "21",
            "start": 951.639,
            "duration": 3.0
        }
    ]
}