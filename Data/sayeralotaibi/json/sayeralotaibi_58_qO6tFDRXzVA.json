            "text": "نتجه الى اسئلتكم اول سؤال من هو سايل",
            "start": 445.86,
            "duration": 6.3
        },
        {
            "text": "العتيبي وما هو سبب حبك للقصص القصص",
            "start": 448.86,
            "duration": 6.179
        },
        {
            "text": "وخصوصا مجال التاريخ فيه توثيق للاحداث",
            "start": 452.16,
            "duration": 6.42
        },
        {
            "text": "والشخصيات بمعنى انك تعيش تجربه الانسان",
            "start": 455.039,
            "duration": 7.081
        },
        {
            "text": "بالكامل تتعلم منها وكذلك تستمتع في",
            "start": 458.58,
            "duration": 6.119
        },
        {
            "text": "المعرفه السؤال الثاني شنو شهادتك العلميه",
            "start": 462.12,
            "duration": 5.34
        },
        {
            "text": "شهاده العلميه هي الاعلام في يوم من",
            "start": 464.699,
            "duration": 4.741
        },
        {
            "text": "الايام انا كنت في الهندسه استمريت فيها",
            "start": 467.46,
            "duration": 5.28
        },
        {
            "text": "لمده ثلاث سنوات وبعدها حولت الى مجالي",
            "start": 469.44,
            "duration": 6.599
        },
        {
            "text": "مجال الاعلام وهذه رساله للكثير من طلبه",
            "start": 472.74,
            "duration": 4.44
        },
        {
            "text": "الثانوي",
            "start": 476.039,
            "duration": 4.201
        },
        {
            "text": "عندك طلبه الجامعه في الزمن الحالي انت",
            "start": 477.18,
            "duration": 6.54
        },
        {
            "text": "امام الانترنت هذا العالم المفتوح ابحث",
            "start": 480.24,
            "duration": 7.26
        },
        {
            "text": "وجرب واخذ المعلومات واختار التخصص اللي",
            "start": 483.72,
            "duration": 7.199
        },
        {
            "text": "تراه في حياتك فانت راح تخوض مسيره مهنيه",
            "start": 487.5,
            "duration": 6.72
        },
        {
            "text": "راح تاخذ نصف حياتك فاختار التخصص على",
            "start": 490.919,
            "duration": 7.441
        },
        {
            "text": "ثلاث معايير الفرصه القدره والشغف السؤال",
            "start": 494.22,
            "duration": 6.9
        },
        {
            "text": "الثالث هالاهتمامك بالقصص اتى من الهوايه",
            "start": 498.36,
            "duration": 5.58
        },
        {
            "text": "او صناعه المحتوى فقط علاقتي مع اليوتيوب",
            "start": 501.12,
            "duration": 5.94
        },
        {
            "text": "والفيديوهات كانت وما زالت هي لصناعه",
            "start": 503.94,
            "duration": 6.06
        },
        {
            "text": "المحتوى فقط لاني باختصار احب التاريخ",
            "start": 507.06,
            "duration": 6.479
        },
        {
            "text": "وتوثيق الاحداث من صغر سني اقرا في",
            "start": 510.0,
            "duration": 6.06
        },
        {
            "text": "الجرائد والاخبار يعني كان وضعي غريب",
            "start": 513.539,
            "duration": 4.92
        },
        {
            "text": "بالنسبه لبقيه المراهقين والشباب اللي",
            "start": 516.06,
            "duration": 5.46
        },
        {
            "text": "بعمري يمكن هذي لاول مره اقولها لكم كنت",
            "start": 518.459,
            "duration": 6.961
        },
        {
            "text": "احب اقرا الجرايد اخذ فيها قصص ريابيه قصص",
            "start": 521.52,
            "duration": 7.379
        },
        {
            "text": "الجرائم وكذلك القصص السياسيه فكنت احب",
            "start": 525.42,
            "duration": 6.359
        },
        {
            "text": "احفظ اسامي الشخصيات نواب مجلس الامه في",
            "start": 528.899,
            "duration": 5.701
        },
        {
            "text": "الكويت الوزراء وكذلك الشخصيات العامه",
            "start": 531.779,
            "duration": 7.261
        },
        {
            "text": "فهذا الامر تطور معاي الى عام 2020 وهذه",
            "start": 534.6,
            "duration": 7.2
        },
        {
            "text": "كانت بدايتي مع صناعه المحتوى سؤال ليش",
            "start": 539.04,
            "duration": 5.82
        },
        {
            "text": "حبيت التاريخ وكيف بدات اليوتيوب ومتى",
            "start": 541.8,
            "duration": 5.88
        },
        {
            "text": "قررت وشنو الهدف او الغايه منه ممكن الجزء",
            "start": 544.86,
            "duration": 5.099
        },
        {
            "text": "الاول من سؤال جوابت عنه لكن الجزء الثاني",
            "start": 547.68,
            "duration": 5.159
        },
        {
            "text": "الحين بجاوبه بدات الفكره معاي في اول",
            "start": 549.959,
            "duration": 5.701
        },
        {
            "text": "ايام الحجر الكلي في الكويت كنا في عز",
            "start": 552.839,
            "duration": 5.881
        },
        {
            "text": "رمضان وتحت الحجر الكلي ومثل ما قلت لكم",
            "start": 555.66,
            "duration": 6.299
        },
        {
            "text": "كنت احب وعندي هواء ستوثيق وكنت دائما",
            "start": 558.72,
            "duration": 6.179
        },
        {
            "text": "ابحث عن الاجوبه لزوايا لم تروى للناس",
            "start": 561.959,
            "duration": 5.361
        },
        {
            "text": "فبداتها بالانستقرام عن طريق سلسله",
            "start": 564.899,
            "duration": 5.88
        },
        {
            "text": "اسميتها واقع الجيل الذهبي فدائما كنت",
            "start": 567.32,
            "duration": 5.98
        },
        {
            "text": "اسمع عن الكويت انه فيها نجاحات من فتره",
            "start": 570.779,
            "duration": 5.221
        },
        {
            "text": "الستينات الى الثمانينات وكانت على جميع",
            "start": 573.3,
            "duration": 5.7
        },
        {
            "text": "الاصعده ففي الرياضه وصلنا كاول منتخب",
            "start": 576.0,
            "duration": 6.12
        },
        {
            "text": "خليجي الى كاس العالم وبالنسبه للفن في",
            "start": 579.0,
            "duration": 5.04
        },
        {
            "text": "الاعمال الفنيه في الكويت انتشرت على",
            "start": 582.12,
            "duration": 4.94
        },
        {
            "text": "مستوى دول الخليج فكنت ابحث عن هذه القصه",
            "start": 584.04,
            "duration": 6.239
        },
        {
            "text": "ورويتها عن طريق ست حلقات ثلاثه منهم",
            "start": 587.06,
            "duration": 6.339
        },
        {
            "text": "كانوا عن الفن والثلاثه الاخرين كانوا عن",
            "start": 590.279,
            "duration": 6.06
        },
        {
            "text": "الرياضه بعدها تعمقت اكثر بفكره التوثيق",
            "start": 593.399,
            "duration": 6.0
        },
        {
            "text": "عن طريق برنامج انستقرامي اسمه مسرح الامه",
            "start": 596.339,
            "duration": 6.18
        },
        {
            "text": "كان عباره عن خمس حلقات تكلمنا فيها عن",
            "start": 599.399,
            "duration": 6.421
        },
        {
            "text": "قضايا حساسه اول حلقه سميناها مستقبل وطن",
            "start": 602.519,
            "duration": 6.121
        },
        {
            "text": "تتكلم عن مستقبل النفط في الكويت ثاني",
            "start": 605.82,
            "duration": 5.88
        },
        {
            "text": "حلقه اسمها مصدر وهمي تتكلم عن تاثير",
            "start": 608.64,
            "duration": 6.24
        },
        {
            "text": "تويتر والحسابات الوهميه على الناس ثالث",
            "start": 611.7,
            "duration": 6.9
        },
        {
            "text": "حلقه اسمها ثقافه جمهور تتكلم عن كيف",
            "start": 614.88,
            "duration": 7.079
        },
        {
            "text": "تاثير الفن على الناس ورابع حلقه سميناها",
            "start": 618.6,
            "duration": 7.32
        },
        {
            "text": "دمار مجتمع وتتكلم عن الادمان وتاثيره على",
            "start": 621.959,
            "duration": 7.261
        },
        {
            "text": "الشباب وخامس حلقه اسمها شعب ديمقراطي",
            "start": 625.92,
            "duration": 6.5
        },
        {
            "text": "وتتكلم عن الحريه وتاثيرها على الوطن",
            "start": 629.22,
            "duration": 6.96
        },
        {
            "text": "فتطور الموضوع معاي وقلت اتوسع في رسالتي",
            "start": 632.42,
            "duration": 7.96
        },
        {
            "text": "من خلال تقديم قصص لشخصيات وحدات نستله",
            "start": 636.18,
            "duration": 7.74
        },
        {
            "text": "منها ونتسلح بها لحياتنا السؤال اللي بعده",
            "start": 640.38,
            "duration": 6.24
        },
        {
            "text": "من متى انت في اليوتيوب وهل احد قدم لك",
            "start": 643.92,
            "duration": 5.88
        },
        {
            "text": "دعم معنوي او مادي مثل ما قلت لكم كتقدم",
            "start": 646.62,
            "duration": 6.54
        },
        {
            "text": "محتوى في الانستغرام بعدها قررت قرار جريء",
            "start": 649.8,
            "duration": 6.479
        },
        {
            "text": "باني اتحول الى اليوتيوب قرار جريء لانه",
            "start": 653.16,
            "duration": 7.26
        },
        {
            "text": "في وقتها قناتي ما تتعدل 200 مشترك وقررت",
            "start": 656.279,
            "duration": 6.3
        },
        {
            "text": "اني انتقل من الانستقرام الى اليوتيوب لان",
            "start": 660.42,
            "duration": 4.74
        },
        {
            "text": "الانستقرام كانت سياستهم في النشر جدا",
            "start": 662.579,
            "duration": 5.82
        },
        {
            "text": "سيئه فاحيانا كثيره كانوا يمسحون حلقاتي",
            "start": 665.16,
            "duration": 6.78
        },
        {
            "text": "لانها ما كانت تتوافق مع سياستهم اما",
            "start": 668.399,
            "duration": 5.88
        },
        {
            "text": "بالنسبه للدعم المادي فالبعض منكم كان",
            "start": 671.94,
            "duration": 5.88
        },
        {
            "text": "يتساءل عن هذا الموضوع ما حصلت ولا فلس",
            "start": 674.279,
            "duration": 6.0
        },
        {
            "text": "على انتاج هذه الحلقات والاستوديو اللي",
            "start": 677.82,
            "duration": 5.579
        },
        {
            "text": "تشوفونه هذا كله من انتاج الخاص السؤال",
            "start": 680.279,
            "duration": 5.521
        },
        {
            "text": "اللي بعده ليش اخترت المحتوى الوثائقي",
            "start": 683.399,
            "duration": 5.581
        },
        {
            "text": "وخصوصا ان المهتمين فيه اقل من بقيه",
            "start": 685.8,
            "duration": 5.7
        },
        {
            "text": "المحتويات هدفي الرئيسي من بدايه الدخول",
            "start": 688.98,
            "duration": 5.599
        },
        {
            "text": "اللي صناعه المحتوى هو تقديم محتوى تاريخي",
            "start": 691.5,
            "duration": 7.98
        },
        {
            "text": "وبالاخص تقصي لحقائق غائبه عنا والهدف ان",
            "start": 694.579,
            "duration": 7.961
        },
        {
            "text": "تطور من فهمنا اللي حاضرنا ولمستقبلنا",
            "start": 699.48,
            "duration": 6.9
        },
        {
            "text": "فانا مؤمن بان من لا يقرا التاريخ لا يكتب",
            "start": 702.54,
            "duration": 6.6
        },
        {
            "text": "المستقبل السؤال اللي بعده وهذا الكثير",
            "start": 706.38,
            "duration": 5.459
        },
        {
            "text": "سالني عنه من اين تستسقي المعلومات",
            "start": 709.14,
            "duration": 5.04
        },
        {
            "text": "والاحداث اللي ترويها بالدرجه الاولى",
            "start": 711.839,
            "duration": 6.021
        },
        {
            "text": "الكتب التاريخيه والمذكرات الدرجه الثانيه",
            "start": 714.18,
            "duration": 7.26
        },
        {
            "text": "المقالات من المختصين وخصوصا اللي تكتب في",
            "start": 717.86,
            "duration": 6.539
        },
        {
            "text": "تلك الفتره الدرجه الثالثه والمهمه كذلك",
            "start": 721.44,
            "duration": 6.18
        },
        {
            "text": "المواقع الحكوميه اللي فيها الوثائق",
            "start": 724.399,
            "duration": 6.581
        },
        {
            "text": "الرسميه فمثلا موقع الارشيف الرقمي للخليج",
            "start": 727.62,
            "duration": 6.06
        },
        {
            "text": "العربي التابع لابو ظبي والثاني موقع",
            "start": 730.98,
            "duration": 5.76
        },
        {
            "text": "مكتبه قطر الوطنيه السؤال اللي بعده الجزء",
            "start": 733.68,
            "duration": 5.339
        },
        {
            "text": "الاول جوابت عنه لكن الجزء الثاني شنو",
            "start": 736.74,
            "duration": 5.339
        },
        {
            "text": "الكتب اللي تنصحنا فيها بالنسبه للكتب انا",
            "start": 739.019,
            "duration": 6.841
        },
        {
            "text": "بدات القراءه من عمر 17 سنه وبهذا العمر",
            "start": 742.079,
            "duration": 7.5
        },
        {
            "text": "كان صعب علي اني يبتدي بقراءه الكتب ولذلك",
            "start": 745.86,
            "duration": 7.14
        },
        {
            "text": "بعطيكم نصائح بسيطه لكل شخص وده انه يبتدي",
            "start": 749.579,
            "duration": 6.181
        },
        {
            "text": "في القراءه في البدايه اقرا الكتب اللي",
            "start": 753.0,
            "duration": 6.36
        },
        {
            "text": "تستهويك وتبحث من خلالها عن اجابات بمعنى",
            "start": 755.76,
            "duration": 7.319
        },
        {
            "text": "ودك تعرف كيف تدير اموالك الشخصيه ابحث عن",
            "start": 759.36,
            "duration": 6.0
        },
        {
            "text": "هذه الاجابه من خلال كتاب اقتصادي",
            "start": 763.079,
            "duration": 5.94
        },
        {
            "text": "والنصيحه الثانيه اقرا من كل مجال مقدمات",
            "start": 765.36,
            "duration": 7.039
        },
        {
            "text": "وبالنسبه للمجال اللي تحبه اقرا عن كل شيء",
            "start": 769.019,
            "duration": 6.241
        },
        {
            "text": "بمعنى اذا تحب التاريخ اقرا عن دوله",
            "start": 772.399,
            "duration": 4.481
        },
        {
            "text": "الخلافه الراشده",
            "start": 775.26,
            "duration": 4.5
        },
        {
            "text": "الدوله العثمانيه وايضا عن الحرب العالميه",
            "start": 776.88,
            "duration": 5.459
        },
        {
            "text": "الثانيه هذا الامر راح يخليك تقرا من",
            "start": 779.76,
            "duration": 4.68
        },
        {
            "text": "الصفحه الاولى الى الصفحه الثلاثيه",
            "start": 782.339,
            "duration": 4.921
        },
        {
            "text": "وبالنسبه للمجالات الاخرى اللي ما تستهويك",
            "start": 784.44,
            "duration": 6.42
        },
        {
            "text": "اقرا مقدماتهم مقدمه عن الفلسفه ومقدمه",
            "start": 787.26,
            "duration": 6.66
        },
        {
            "text": "للاقتصاد ومقدمه عن كل المجالات السؤال",
            "start": 790.86,
            "duration": 5.4
        },
        {
            "text": "اللي بعده شنو افضل قصه رويتها في هذه",
            "start": 793.92,
            "duration": 5.58
        },
        {
            "text": "القناه بالنسبه لي قصه الاحواز بحكم انه",
            "start": 796.26,
            "duration": 6.72
        },
        {
            "text": "هي قضيه عربيه ما رويت ولا عرفت بين الجيل",
            "start": 799.5,
            "duration": 5.94
        },
        {
            "text": "الحالي من العرب في سؤالين على صيغه",
            "start": 802.98,
            "duration": 4.5
        },
        {
            "text": "اقتراح والكثير منكم اقتراح اللي فيهم",
            "start": 805.44,
            "duration": 5.04
        },
        {
            "text": "اللي هي انه ممكن اعمل سلسله تتكلم عن",
            "start": 807.48,
            "duration": 5.82
        },
        {
            "text": "دوله الخلافه الراشده وحتى سقوط الدوله",
            "start": 810.48,
            "duration": 6.0
        },
        {
            "text": "العثمانيه اذا حابين ودكم انه نسوي سلسله",
            "start": 813.3,
            "duration": 5.88
        },
        {
            "text": "كامله تمتد من دوله الخلافه الراشده وحتى",
            "start": 816.48,
            "duration": 5.7
        },
        {
            "text": "سقوط الدوله العثمانيه ابي منكم تضعونها",
            "start": 819.18,
            "duration": 6.0
        },
        {
            "text": "في التعليقات اخر سؤال شنو جدولك في تنزيل",
            "start": 822.18,
            "duration": 5.64
        },
        {
            "text": "القصص ما ادري اذا انتم مركزين معاي انا",
            "start": 825.18,
            "duration": 6.36
        },
        {
            "text": "كل سبت انزل قصه في سلسله مالم يروان لكن",
            "start": 827.82,
            "duration": 6.54
        },
        {
            "text": "راح تكون في مفاجاه في القناه فالسلسلات",
            "start": 831.54,
            "duration": 6.419
        },
        {
            "text": "مال مروى راح يضاف لها اخرى على شكل",
            "start": 834.36,
            "duration": 6.36
        },
        {
            "text": "برنامج فان شاء الله يا رب من بعد الله ثم",
            "start": 837.959,
            "duration": 6.06
        },
        {
            "text": "دعمكم راح تكون هناك حلقتين في الاسبوع",
            "start": 840.72,
            "duration": 6.66
        },
        {
            "text": "وهذا يجرني اني اقول لكم عن القادم",
            "start": 844.019,
            "duration": 6.141
        },
        {
            "text": "ومستقبل هذه القناه بعد ما شفت تفاعلكم",
