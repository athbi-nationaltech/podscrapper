        },
        {
            "text": "تهاجر بشكل سري من ساحل الشمال الى",
            "start": 370.62,
            "duration": 5.22
        },
        {
            "text": "اسبانيا كان كبير فلجاوا الى حميد الديب",
            "start": 372.9,
            "duration": 6.18
        },
        {
            "text": "اللي كان موفر للدوله امن البحر فمنع اي",
            "start": 375.84,
            "duration": 6.72
        },
        {
            "text": "مهرب يمر على المنطقه اللي يحرسونها رجال",
            "start": 379.08,
            "duration": 6.66
        },
        {
            "text": "الى ان استطاع ان يوقف التهريب من الساحل",
            "start": 382.56,
            "duration": 6.12
        },
        {
            "text": "الشمالي من منطقه طنجه فاصبحت اصابته",
            "start": 385.74,
            "duration": 6.299
        },
        {
            "text": "مسيطره على الامن في منطقه طنجه والشمال",
            "start": 388.68,
            "duration": 6.42
        },
        {
            "text": "المغربي الى درجه ان عصابته في فتره من",
            "start": 392.039,
            "duration": 5.641
        },
        {
            "text": "الفترات كانوا يعتقلون المجرمين وقطاع",
            "start": 395.1,
            "duration": 5.099
        },
        {
            "text": "الطرق وقدروا انه يحبطون عمليات ارهابيه",
            "start": 397.68,
            "duration": 5.82
        },
        {
            "text": "كادت انه تحصل في مراكش وطنجه وبعد ما",
            "start": 400.199,
            "duration": 5.94
        },
        {
            "text": "توسعت العصابه بدات تتدخل على الدوله",
            "start": 403.5,
            "duration": 6.06
        },
        {
            "text": "مليارات الدولارات والاجهزه الامنيه كانت",
            "start": 406.139,
            "duration": 6.361
        },
        {
            "text": "متفاعله مع علاقتها مع عصابه حميدو الديب",
            "start": 409.56,
            "duration": 6.419
        },
        {
            "text": "الى درجه ان في احد المرات تم قتل الاجانب",
            "start": 412.5,
            "duration": 6.24
        },
        {
            "text": "سياح في احد المنتجعات في منطقه طنجه",
            "start": 415.979,
            "duration": 5.521
        },
        {
            "text": "والاجهزه الامنيه ما استطاعت انها تقبض",
            "start": 418.74,
            "duration": 5.04
        },
        {
            "text": "على المجرمين فاتصلوا على حميد الديب",
            "start": 421.5,
            "duration": 6.0
        },
        {
            "text": "وابلغه ان هناك مجرمين هاربين وبعدها فتره",
            "start": 423.78,
            "duration": 7.139
        },
        {
            "text": "بسيطه قبض على المجرم الهارب وسلمه الى",
            "start": 427.5,
            "duration": 6.12
        },
        {
            "text": "الاجهزه الامنيه وايضا عصابه حميد الديب",
            "start": 430.919,
            "duration": 4.981
        },
        {
            "text": "كان لها دور كبير في القبض على المجرمين",
            "start": 433.62,
            "duration": 4.919
        },
        {
            "text": "في الاحداث الارهابيه اللي حصلت في فندق",
            "start": 435.9,
            "duration": 8.16
        },
        {
            "text": "اطلس ازني في سنه 1994 في التاليه انتشرت",
            "start": 438.539,
            "duration": 8.641
        },
        {
            "text": "المخدرات المغربيه بكميات كبيره في اوروبا",
            "start": 444.06,
            "duration": 6.66
        },
        {
            "text": "وكل هذا بسبب حميدو الديب اللي عمل لنفسه",
            "start": 447.18,
            "duration": 6.6
        },
        {
            "text": "امبراطوريه بعد ما كبرت عصابته وتقريبا",
            "start": 450.72,
            "duration": 6.419
        },
        {
            "text": "بنى دوله داخل دوله الى درجه ان عصابه",
            "start": 453.78,
            "duration": 6.24
        },
        {
            "text": "حميدو الديب اصبح لها جهاز امني وجهاز",
            "start": 457.139,
            "duration": 6.18
        },
        {
            "text": "مخابرات عندهم قوات مسلحه مثل مركزه في",
            "start": 460.02,
            "duration": 6.119
        },
        {
            "text": "المناطق اللي يتبعونها الغريب ان اغلب",
            "start": 463.319,
            "duration": 5.82
        },
        {
            "text": "الناس كانت مبسوطه بهذه العصابه لانهم بس",
            "start": 466.139,
            "duration": 5.641
        },
        {
            "text": "يشوفون الجانب اللي فيه حميدو الديب ينقذه",
            "start": 469.139,
            "duration": 6.0
        },
        {
            "text": "من الفقر ويعطي اعمال خيريه وبالفعل عصابه",
            "start": 471.78,
            "duration": 5.819
        },
        {
            "text": "الديب كل فتره يعملون مشاريع مختلفه",
            "start": 475.139,
            "duration": 5.641
        },
        {
            "text": "فيبنون الطرق والمستشفيات اللي يعالجون",
            "start": 477.599,
            "duration": 6.061
        },
        {
            "text": "فيها الناس مجانا وتخيلوا وحميدو الديب",
            "start": 480.78,
            "duration": 5.88
        },
        {
            "text": "بنى بيوت لكل العاملين اللي يشتغلون معاه",
            "start": 483.66,
            "duration": 6.18
        },
        {
            "text": "وكان يتكفل لكل العاجزين والعاطلين عن",
            "start": 486.66,
            "duration": 6.539
        },
        {
            "text": "العمل وهم من يصرف راتب شهري لكل فقير في",
            "start": 489.84,
            "duration": 6.419
        },
        {
            "text": "منطقه طنجه فانتشر اسم حميدو الديب في كل",
            "start": 493.199,
            "duration": 6.201
        },
        {
            "text": "المغرب الى درجه انه وصل اسمه الى اوروبا",
            "start": 496.259,
            "duration": 6.601
        },
        {
            "text": "فكانوا يكتبون في الصحف الاوروبيه ان شخص",
            "start": 499.4,
            "duration": 6.519
        },
        {
            "text": "من المغرب يهرب المخدرات والحشيش الى",
            "start": 502.86,
            "duration": 5.58
        },
        {
            "text": "اوروبا والامر الخطير انهم كانوا يكتبون",
            "start": 505.919,
            "duration": 5.941
        },
        {
            "text": "ان السلطات المغربيه ما لها دور في ايقافه",
            "start": 508.44,
            "duration": 5.76
        },
        {
            "text": "رغم بدايه انتشار الاخبار في الصحف",
            "start": 511.86,
            "duration": 4.919
        },
        {
            "text": "الاوروبيه عن عصابه حميدو الديب الا ان",
            "start": 514.2,
            "duration": 6.839
        },
        {
            "text": "هذه العصابه ماشيه في دربها الى سنه 1996",
            "start": 516.779,
            "duration": 7.981
        },
        {
            "text": "طلع تقرير من مكتب الامم المتحده عن",
            "start": 521.039,
            "duration": 6.901
        },
        {
            "text": "المخدرات فاعتبروا ان المغرب هي الاولى",
            "start": 524.76,
            "duration": 6.78
        },
        {
            "text": "عالميا في تهريب المخدرات واتهموا ملك",
            "start": 527.94,
            "duration": 7.32
        },
        {
            "text": "المغرب وقتها الحسن الثاني انه متعاون مع",
            "start": 531.54,
            "duration": 7.32
        },
        {
            "text": "تجار المخدرات والحشيش هذا الامر سبب ضجه",
            "start": 535.26,
            "duration": 6.84
        },
        {
            "text": "كبيره في المغرب لان من بعدها اوروبا قطعت",
            "start": 538.86,
            "duration": 6.06
        },
        {
            "text": "كل علاقاتها مع المغرب فتوقفت كل",
            "start": 542.1,
            "duration": 5.34
        },
        {
            "text": "استثمارات والاموال اللي فيها نسبه كبيره",
            "start": 544.92,
            "duration": 5.34
        },
        {
            "text": "من ميزانيه المغرب بعد ما اصبحت المغرب",
            "start": 547.44,
            "duration": 6.06
        },
        {
            "text": "بؤره الفساد والمخدرات والحشيش عند الامم",
            "start": 550.26,
            "duration": 6.3
        },
        {
            "text": "المتحده قررت حكومه المغرب انها تمحي",
            "start": 553.5,
            "duration": 6.6
        },
        {
            "text": "الصوره السيئه باي طريقه ممكنه عشان تثبت",
            "start": 556.56,
            "duration": 7.5
        },
        {
            "text": "للعالم والراي العام انها ضد والحشيش وان",
            "start": 560.1,
            "duration": 6.66
        },
        {
            "text": "المعلومات غير صحيحه ملك المغرب الحسن",
            "start": 564.06,
            "duration": 5.779
        },
        {
            "text": "الثاني كلف وزير الداخليه ادريس البصري",
            "start": 566.76,
            "duration": 6.48
        },
        {
            "text": "باعلان حمله على اكبر تجار المخدرات في",
            "start": 569.839,
            "duration": 5.761
        },
        {
            "text": "البلد وكذلك رجال الاعمال الكبار",
            "start": 573.24,
            "duration": 5.64
        },
        {
            "text": "المتورطين في تهريب المخدرات فانتشرت",
            "start": 575.6,
            "duration": 6.64
        },
        {
            "text": "الاجهزه الامنيه في كل مكان وقبض على اكثر",
            "start": 578.88,
            "duration": 6.78
        },
        {
            "text": "من 400 تاجر مخدرات ومنهم حميدو الديب",
            "start": 582.24,
            "duration": 6.24
        },
        {
            "text": "قبضوا عليه من غير دليل اما كان عنده",
            "start": 585.66,
            "duration": 6.54
        },
        {
            "text": "مخدرات اثناء القبض قبضوا عليه فقط بسبب",
            "start": 588.48,
            "duration": 6.9
        },
        {
            "text": "اسمه المعروف خبر القبض على حميدو الديب",
            "start": 592.2,
            "duration": 6.3
        },
        {
            "text": "كان صدمه كبيره لاهالي طنجه لانه كان",
            "start": 595.38,
            "duration": 6.3
        },
        {
            "text": "يعتبر الاب الروحي لهم اما باقي مناطق",
            "start": 598.5,
            "duration": 6.54
        },
        {
            "text": "المغرب فكان هذا الخبر مفرح لهم لان حميد",
            "start": 601.68,
            "duration": 6.779
        },
        {
            "text": "الديب وعصابته سببه لهم ضرر كبير فقبض",
            "start": 605.04,
            "duration": 6.18
        },
        {
            "text": "عليه بتهمه التجاره الدوليه على حسب كلام",
            "start": 608.459,
            "duration": 5.521
        },
        {
            "text": "حميدو الديب يقول انه اعتقلوني رجال الامن",
            "start": 611.22,
            "duration": 6.059
        },
        {
            "text": "وهما متخفين بلبس مدني واعتقلوني في الليل",
            "start": 613.98,
            "duration": 6.68
        },
        {
            "text": "بعد ما اعتقلوه رجال الامن فتشوا بيوته",
            "start": 617.279,
            "duration": 8.161
        },
        {
            "text": "وشركاته وا وكذلك مصانعه بالرغم كل ذلك ما",
            "start": 620.66,
            "duration": 8.5
        },
        {
            "text": "وجدوا اي دليل على حميدو الديب والسبب ان",
            "start": 625.44,
            "duration": 6.24
        },
        {
            "text": "حميد الذيب كان عنده مخبرين في الاجهزه",
            "start": 629.16,
            "duration": 4.619
        },
        {
            "text": "الامنيه فاول ما طلعت الحمله الامنيه",
            "start": 631.68,
            "duration": 5.099
        },
        {
            "text": "للقبض عليه اخبره المخبرين بهذه الحمله",
            "start": 633.779,
            "duration": 6.081
        },
        {
            "text": "فاستطاع انه يخفي جميع بضاعته من المخدرات",
            "start": 636.779,
            "duration": 6.961
        },
        {
            "text": "وما ترك اي اثر خلفه مع انه معروف في",
            "start": 639.86,
            "duration": 6.58
        },
        {
            "text": "العالم انه احتاج المخدرات لكن لا يوجد",
            "start": 643.74,
            "duration": 5.52
        },
        {
            "text": "دليل على ذلك بعدها انقلوه الى سجن",
            "start": 646.44,
            "duration": 6.36
        },
        {
            "text": "القنيطره ومنها اخذوه الى معتقل تماره",
            "start": 649.26,
            "duration": 7.74
        },
        {
            "text": "وهناك حققوا معاه لمده 17 يوم واسالوه عن",
            "start": 652.8,
            "duration": 6.9
        },
        {
            "text": "الثروه اللي جمعها وعلاقتها مع بقيه تجار",
            "start": 657.0,
            "duration": 5.64
        },
        {
            "text": "المخدرات بعدها دخل عليه ضابط من اكبر",
            "start": 659.7,
            "duration": 5.699
        },
        {
            "text": "الضباط في الاجهزه الامنيه قال له على طول",
            "start": 662.64,
            "duration": 6.48
        },
        {
            "text": "الدوله مقدره دورك وخيرك عليها واحنا",
            "start": 665.399,
            "duration": 6.661
        },
        {
            "text": "مقدرين وقفتك لظهر الدوله وكل الخدمات",
            "start": 669.12,
            "duration": 5.88
        },
        {
            "text": "اللي قدمتها لنا رغم انه لا يوجد دليل ضدك",
            "start": 672.06,
            "duration": 6.12
        },
        {
            "text": "الا ان الاوروبيين ما راح يسمحون لنا فقال",
            "start": 675.0,
            "duration": 6.12
        },
        {
            "text": "له اعتبر اعتقالك تضحيه للدوله فنحتاج",
            "start": 678.18,
            "duration": 4.08
        },
        {
            "text": "فتره قصيره",
            "start": 681.12,
            "duration": 4.38
        },
        {
            "text": "فيها عن العيون ونكسب ثقه المجتمع الدولي",
            "start": 682.26,
            "duration": 5.819
        },
        {
            "text": "وبالنسبه لحميدو الديب كانت عنده علاقات",
            "start": 685.5,
            "duration": 5.7
        },
        {
            "text": "قويه في المغرب فكان ضامن ان اعتقاله راح",
            "start": 688.079,
            "duration": 6.0
        },
        {
            "text": "يكون لفتره قصيره وكذلك الضابط اعطى وعد",
            "start": 691.2,
            "duration": 5.879
        },
        {
            "text": "ان الاعتقال راح يكون لي فتره بسيطه وراح",
            "start": 694.079,
            "duration": 6.241
        },
        {
            "text": "يطلع عفو ملكي لاخراجه وفوقه راح يعطونه",
            "start": 697.079,
            "duration": 6.601
        },
        {
            "text": "وسام البطوله للمعلوميه حميد الديب يعتبر",
            "start": 700.32,
            "duration": 6.86
        },
        {
            "text": "امي فهو جاهل لا يعرف لا يقرا ولا يكتب",
            "start": 703.68,
            "duration": 6.899
        },
        {
            "text": "فاستطاعوا انه يغرسون في راسه ان اعتقاله",
            "start": 707.18,
            "duration": 5.74
        },
        {
            "text": "راح يكون مؤقت واللي كان وراء هذه الخطه",
            "start": 710.579,
            "duration": 6.481
        },
        {
            "text": "وزير الداخليه ادريس البصري ادريس اقنع",
            "start": 712.92,
            "duration": 7.14
        },
        {
            "text": "حميد الديب بالتوقيع على المحضر والمحضر",
            "start": 717.06,
            "duration": 6.48
        },
        {
            "text": "فيه اعتراف كامل بتوريد حميدو الديب",
            "start": 720.06,
            "duration": 6.42
        },
        {
            "text": "بتهريبه للمخدرات فوزير الداخليه كان يعرف",
            "start": 723.54,
            "duration": 5.82
        },
        {
            "text": "ان حميدو الديب لا يعرف لا يقرا ولا يكتب",
            "start": 726.48,
            "duration": 6.18
        },
        {
            "text": "فما كان يعرف مضمون المحضر فانحكم على",
            "start": 729.36,
            "duration": 6.9
        },
        {
            "text": "حميد الديب بالسجن لمده عشر سنوات في هذه",
            "start": 732.66,
            "duration": 5.64
        },
        {
            "text": "العمليه التطهيريه اللي عملها ادريس",
            "start": 736.26,
            "duration": 5.519
        },
        {
            "text": "البصري سجنوا عدد كبير من الناس وحجزوا",
            "start": 738.3,
            "duration": 7.02
        },
        {
            "text": "جميع ادت هذه العمليه الى ضجه كبيره في",
            "start": 741.779,
            "duration": 6.5
        },
        {
            "text": "المغرب وكذلك تدخل جمعيه حقوق الانسان",
            "start": 745.32,
            "duration": 5.88
        },
        {
            "text": "فالجمعيه اتقدت الطريقه القاسيه اللي",
            "start": 748.279,
            "duration": 5.981
        },
        {
            "text": "نفذها وزير الداخليه بعد ما تم سجن حميدو",
            "start": 751.2,
            "duration": 7.02
        },
        {
            "text": "الديب الدوله احتزت جميع امواله وممتلكاته",
            "start": 754.26,
            "duration": 7.199
        },
        {
            "text": "اللي وصلت لي اكثر من اربعين مليار درهم",
            "start": 758.22,
            "duration": 6.419
        },
        {
            "text": "مغربي لكن الصدمه كانت لما درى حميد الذيب",
            "start": 761.459,
            "duration": 6.261
        },
        {
            "text": "ان هذه لعبه لعبها ادريس البصري ورجاله",
            "start": 764.639,
            "duration": 6.541
        },
        {
            "text": "فاستغلوا جهله واميته فاستولوا على جميع",
            "start": 767.72,
            "duration": 6.28
        },
        {
            "text": "ممتلكاته يقول حميدو الديب قبل لا يتم",
            "start": 771.18,
            "duration": 5.82
        },
        {
            "text": "القبض عليه وصلي خبر انه ادريس البصري",
            "start": 774.0,
            "duration": 6.54
        },
        {
            "text": "اتصل على جميع اقربائه واصحابه الاغنياء",
            "start": 777.0,
            "duration": 6.0
        },
        {
            "text": "وقال لهم راح اشتري ممتلكات حميد الدين",
            "start": 780.54,
            "duration": 6.78
        },
        {
            "text": "باقل سعر وراح اعرضها على مزاد وفعلا تاجر",
            "start": 783.0,
            "duration": 7.62
        },
        {
            "text": "بممتلكات حميدو الديب باقل سعر واشتروها",
            "start": 787.32,
            "duration": 6.72
        },
        {
            "text": "اصحابه الاغنياء ولذلك ادريس البصري قبلها",
            "start": 790.62,
            "duration": 7.14
        },
        {
            "text": "اصدر اوامر بتجميد جميع ممتلكات حميدو",
            "start": 794.04,
            "duration": 7.26
        },
        {
            "text": "الديب للابد وبالنهايه لم يبقى شيء لحميدو",
            "start": 797.76,
            "duration": 4.379
        },
        {
            "text": "الديب",
            "start": 801.3,
            "duration": 4.979
        },
        {
            "text": "البصري حصل ثروه من ممتلكاته وجلس حميدو",
            "start": 802.139,
            "duration": 7.32
        },
        {
            "text": "الديب بالسجن لمده عشر سنوات ينتظر العفو",
            "start": 806.279,
            "duration": 5.581
        },
        {
            "text": "الملكي الى ان توفى الملك الحسن الثاني",
            "start": 809.459,
            "duration": 6.721
        },
        {
            "text": "واخذ مكانه ابنه الملك محمد السادس وبعدها",
            "start": 811.86,
            "duration": 7.2
        },
        {
            "text": "تغير عدد كبير من رجالات الدوله ومنهم",
            "start": 816.18,
            "duration": 5.76
        },
        {
            "text": "ادريس البصري وبعد سبع سنوات من سجنه",
            "start": 819.06,
            "duration": 6.42
        },
        {
            "text": "وبالتحديد سنه 2003 خرج حميدو الديب من",
            "start": 821.94,
            "duration": 7.139
        },
        {
            "text": "السجن بعفو ملكي كل شخص خرج من السجن بعفو",
            "start": 825.48,
            "duration": 7.5
        },
        {
            "text": "ملكي تم استرجاع جميع ممتلكاته وفعلا هذا",
            "start": 829.079,
            "duration": 6.721
        },
        {
            "text": "اللي حصل مع جميع الاشخاص اللي تم سجنهم",
            "start": 832.98,
            "duration": 6.06
        },
        {
            "text": "مع حميدو الديب الا الديب اختفت كل ثروته",
            "start": 835.8,
            "duration": 6.479
        },
        {
            "text": "وما بقى معاه ولا درهم مغربي في النهايه",
            "start": 839.04,
            "duration": 7.44
        },
        {
            "text": "حميد الديب حتى لو قدم اعمال خيريه فهو",
            "start": 842.279,
            "duration": 7.62
        },
        {
            "text": "سعى للفساد في الارض ولذلك قيمه الافعال",
            "start": 846.48,
            "duration": 7.919
        },
        {
            "text": "لا تدرك الا بالنتائج فاجعل لفعلي كمرد",
            "start": 849.899,
            "duration": 6.801
        },
        {
            "text": "جميل",
            "start": 854.399,
            "duration": 2.301
        },
