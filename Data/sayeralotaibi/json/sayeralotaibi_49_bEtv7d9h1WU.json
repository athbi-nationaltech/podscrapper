            "start": 392.72,
            "duration": 7.44
        },
        {
            "text": "كشمير وبلوشستان وكانوا يدينون بالدين",
            "start": 396.24,
            "duration": 7.92
        },
        {
            "text": "الاسلامي كان الرعاه لهم مهام محدده فاولا",
            "start": 400.16,
            "duration": 6.96
        },
        {
            "text": "كانوا يحددون مواقع المياه في الصحراء",
            "start": 404.16,
            "duration": 6.319
        },
        {
            "text": "ثانيا يحافظون على امن المسافرين معهم",
            "start": 407.12,
            "duration": 6.639
        },
        {
            "text": "ثالثا ينقلون الامدادات من المدن الى",
            "start": 410.479,
            "duration": 5.921
        },
        {
            "text": "البلدات ومناجم الذهب والهدف الرئيسي",
            "start": 413.759,
            "duration": 6.401
        },
        {
            "text": "والاشمل هو تاسيس خطوط القطارات والطرق",
            "start": 416.4,
            "duration": 7.44
        },
        {
            "text": "كانوا رعاه الابل عباره عن مرشدين وموجهين",
            "start": 420.16,
            "duration": 6.759
        },
        {
            "text": "في الصحراء تمشي خلفهم الوفود على يقين",
            "start": 423.84,
            "duration": 6.44
        },
        {
            "text": "بخبرتهم في قطع المسافات في هذه الفتره",
            "start": 426.919,
            "duration": 6.96
        },
        {
            "text": "واجهوا المسلمين ظروف شاقه زياده على",
            "start": 430.28,
            "duration": 6.599
        },
        {
            "text": "الظروف القاسيه في الصحره فالطرق ما كانت",
            "start": 433.879,
            "duration": 6.6
        },
        {
            "text": "مؤهله لنقل المياه والمعدات والادوات لكن",
            "start": 436.879,
            "duration": 6.481
        },
        {
            "text": "اظهرت قدره الرعاه على قياده الابل",
            "start": 440.479,
            "duration": 5.801
        },
        {
            "text": "وتوجيهها وكانوا يستكشفون المناطق",
            "start": 443.36,
            "duration": 6.279
        },
        {
            "text": "الداخليه الغير معروفه وينقلون كل الموار",
            "start": 446.28,
            "duration": 5.84
        },
        {
            "text": "على ظهر الابل وايضا يديرون رحلات",
            "start": 449.639,
            "duration": 5.601
        },
        {
            "text": "استكشافيه في المناطق الريفيه والنائي وفي",
            "start": 452.12,
            "duration": 3.919
        },
        {
            "text": "عام",
            "start": 455.24,
            "duration": 5.12
        },
        {
            "text": "1895 حدثت واحده من اكبر الازمات في",
            "start": 456.039,
            "duration": 7.201
        },
        {
            "text": "التاريخ الاسترالي فاقترب شبح الموت من",
            "start": 460.36,
            "duration": 7.239
        },
        {
            "text": "عده مناطق ولكن الافغان انقذوا حياه",
            "start": 463.24,
            "duration": 8.079
        },
        {
            "text": "الاستراليين فما هو هذا الموقف التاريخي",
            "start": 467.599,
            "duration": 5.56
        },
        {
            "text": "ففي تلك السنه",
            "start": 471.319,
            "duration": 6.44
        },
        {
            "text": "1895 اصاب شرق استراليا جفاف كبير عانى",
            "start": 473.159,
            "duration": 8.16
        },
        {
            "text": "منه الكثير من الاستراليين لكن الافغان",
            "start": 477.759,
            "duration": 6.88
        },
        {
            "text": "انقلوا الامدادات اللازمه الى هذه المناطق",
            "start": 481.319,
            "duration": 7.88
        },
        {
            "text": "وحلوا ازمه تاريخيه امتدت لمده سبع سنوات",
            "start": 484.639,
            "duration": 6.761
        },
        {
            "text": "هذا الوجود للمسلمين في استراليا",
            "start": 489.199,
            "duration": 6.081
        },
        {
            "text": "والاحتكاك القوي مع المجتمع اعطى المسلمين",
            "start": 491.4,
            "duration": 7.079
        },
        {
            "text": "تاثير قوي وملحوظ فالمفاهيم الاسلاميه",
            "start": 495.28,
            "duration": 6.28
        },
        {
            "text": "صارت بارزه وواضحه في المجتمع الاسترالي",
            "start": 498.479,
            "duration": 6.0
        },
        {
            "text": "والتاثير برز اكثر من ناحيه اقتصاديه",
            "start": 501.56,
            "duration": 6.68
        },
        {
            "text": "واجتماعيه وكذلك ثقافيه ففي سنه",
            "start": 504.479,
            "duration": 7.881
        },
        {
            "text": "1882 بنوا المسلمين الافغان اول مسجد في",
            "start": 508.24,
            "duration": 8.279
        },
        {
            "text": "استراليا فكان يعتبر مناره للعباده وشاهد",
            "start": 512.36,
            "duration": 6.52
        },
        {
            "text": "على حضورهم اللي يشوف استراليا اليوم",
            "start": 516.519,
            "duration": 5.96
        },
        {
            "text": "يلاحظ عدم وجود احفاد الجمالين الافغان",
            "start": 518.88,
            "duration": 6.72
        },
        {
            "text": "فالسؤال اللي يطرح نفسه كيف تعاملوا معهم",
            "start": 522.479,
            "duration": 6.281
        },
        {
            "text": "الاستراليين وليش ما نجد لهم حضور اليوم",
            "start": 525.6,
            "duration": 5.919
        },
        {
            "text": "مع ان رعاه الابل الافغان كان لهم دور",
            "start": 528.76,
            "duration": 5.72
        },
        {
            "text": "كبير في النهضه الحضاريه لكن التعامل",
            "start": 531.519,
            "duration": 6.201
        },
        {
            "text": "معاهم كان فيه الكثير من العنصريه ورغم",
            "start": 534.48,
            "duration": 6.039
        },
        {
            "text": "انهم كانوا اصحاب تاثير كبير في المجتمع",
            "start": 537.72,
            "duration": 6.52
        },
        {
            "text": "الا انهم عاشوا في مدن مستقله عن البيض",
            "start": 540.519,
            "duration": 7.281
        },
        {
            "text": "فوضعوه في مساكن بدائيه لا تليق فيهم",
            "start": 544.24,
            "duration": 6.52
        },
        {
            "text": "فتخيلوا في البدايه سكنوا في نفس المخيمات",
            "start": 547.8,
            "duration": 6.599
        },
        {
            "text": "اللي تاسست للابل وبعدها عاشوا في بيوت",
            "start": 550.76,
            "duration": 7.16
        },
        {
            "text": "افضل لكن فيها قصور كبير والجمالين",
            "start": 554.399,
            "duration": 6.521
        },
        {
            "text": "الافغان عاشوا وتحملوا هذه الظروف ومن",
            "start": 557.92,
            "duration": 5.4
        },
        {
            "text": "ناحيه الاندماج في المجتمع فما كانوا",
            "start": 560.92,
            "duration": 5.76
        },
        {
            "text": "البيض يقبلون فيهم كازواج فكانوا يلجؤون",
            "start": 563.32,
            "duration": 6.36
        },
        {
            "text": "الى الزواج بالسكان الاصليين في السر",
            "start": 566.68,
            "duration": 6.32
        },
        {
            "text": "هذه التفرقه كانت مبنيه على اسس واضحه",
            "start": 569.68,
            "duration": 6.32
        },
        {
            "text": "الاسس نفسها اللي يرتكزون عليها العنصريين",
            "start": 573.0,
            "duration": 6.72
        },
        {
            "text": "عبر التاريخ وهي اللون والدين والمظهر على",
            "start": 576.0,
            "duration": 6.6
        },
        {
            "text": "العموم بعد فتره ادخلوا الافغان المسلمين",
            "start": 579.72,
            "duration": 6.559
        },
        {
            "text": "مناجم الذهب وتميزوا بالعمل فيها وكانوا",
            "start": 582.6,
            "duration": 6.28
        },
        {
            "text": "يستفيدون من جمالهم من ناحيه تحميلها",
            "start": 586.279,
            "duration": 5.081
        },
        {
            "text": "فكانوا يضعون عليها كل ما يحتاجون من",
            "start": 588.88,
            "duration": 6.0
        },
        {
            "text": "مؤونه وكل ما يحتاجونه من اداه تنقيب",
            "start": 591.36,
            "duration": 6.88
        },
        {
            "text": "الذهب رغم تميزهم في النشاط الاقتصادي الا",
            "start": 594.88,
            "duration": 7.8
        },
        {
            "text": "ان الاعلام شن عليهم هجمه شرسه ولذلك",
            "start": 598.24,
            "duration": 7.32
        },
        {
            "text": "لنعرف السبب في هذه الفتره اشعروا البيض",
            "start": 602.68,
            "duration": 6.48
        },
        {
            "text": "ان الافغان تميزوا في عملهم واصبح هناك",
            "start": 605.56,
            "duration": 6.64
        },
        {
            "text": "منافسه شديده في سوق العمل فكانوا يقولون",
            "start": 609.16,
            "duration": 7.08
        },
        {
            "text": "ان الافغان اخذوا فرص وظيفيه كثيره واتى",
            "start": 612.2,
            "duration": 8.52
        },
        {
            "text": "الان وقت تصفيه الحسابات هذا الحسد النابع",
            "start": 616.24,
            "duration": 8.36
        },
        {
            "text": "من العنصريه جعل الافتراءات تدور حول",
            "start": 620.72,
            "duration": 7.92
        },
        {
            "text": "الافغان من كل جانب فقررت الصحافه شيطنه",
            "start": 624.6,
            "duration": 8.239
        },
        {
            "text": "هذه الفئه واطلقت حمله ضدهم فصورت الحملات",
            "start": 628.64,
            "duration": 7.6
        },
        {
            "text": "الصحفيه الافغان على انهم شخصيات سيئه",
            "start": 632.839,
            "duration": 6.921
        },
        {
            "text": "المظهر قليله النظافه سارقه الموارد",
            "start": 636.24,
            "duration": 6.52
        },
        {
            "text": "وغيرها الكثير استمرت هذه الحملات فتره",
            "start": 639.76,
            "duration": 6.68
        },
        {
            "text": "طويله وحققت مرادها في النهايه ولان",
            "start": 642.76,
            "duration": 6.68
        },
        {
            "text": "استراليا كانت تحت الحكم الاستعماري قررت",
            "start": 646.44,
            "duration": 6.24
        },
        {
            "text": "الحكومه الاستعماريه التضييق على المسلمين",
            "start": 649.44,
            "duration": 7.199
        },
        {
            "text": "الافغان فاصدر تشريعات كثيره تميز ما بين",
            "start": 652.68,
            "duration": 7.399
        },
        {
            "text": "الغان والفئات الاخرى فضي ضيقوا عليهم",
            "start": 656.639,
            "duration": 6.44
        },
        {
            "text": "الاعمال المشروعه مثل عملهم في مناجم",
            "start": 660.079,
            "duration": 6.121
        },
        {
            "text": "الذهب وبعدها اصدروا قوانين فيها شده",
            "start": 663.079,
            "duration": 6.56
        },
        {
            "text": "فضيقوا عليهم من ناحيه الهجرات والجنسيات",
            "start": 666.2,
            "duration": 6.639
        },
        {
            "text": "ومن المواقف العجيبه اللي تبين عبثيه",
            "start": 669.639,
            "duration": 6.241
        },
        {
            "text": "التعامل مع الغان كان اي مسلم يخرج من",
            "start": 672.839,
            "duration": 7.24
        },
        {
            "text": "استراليا لا يمكن انه يقدر يرجع لها فيصبح",
            "start": 675.88,
            "duration": 7.759
        },
        {
            "text": "فجاه ممنوع من الدخول رغم انه ممتلكاته",
            "start": 680.079,
            "duration": 7.361
        },
        {
            "text": "موجوده في استراليا وهو مسافر سفر مؤقت",
            "start": 683.639,
            "duration": 7.081
        },
        {
            "text": "هذا الاسلوب التعسفي كان يهدف الى تنفير",
            "start": 687.44,
            "duration": 6.6
        },
        {
            "text": "المسلمين من استراليا هذه التضييقات جعلت",
            "start": 690.72,
            "duration": 7.96
        },
        {
            "text": "البعض يهاجر لدوله اخرى لكن الاغلب بقى في",
            "start": 694.04,
            "duration": 7.84
        },
        {
            "text": "استراليا واستمروا على هذا الحال لمده",
            "start": 698.68,
            "duration": 8.36
        },
        {
            "text": "سنوات لكن احدثت طفره جعلت الاغلب ينوي",
            "start": 701.88,
            "duration": 8.759
        },
        {
            "text": "الهجره هذه الطفره حصلت بسبب ظهور الطرق",
            "start": 707.04,
            "duration": 8.08
        },
        {
            "text": "بشكلها الحديث وقله الحاجه للابل فتغير",
            "start": 710.639,
            "duration": 6.481
        },
        {
            "text": "حال المجتمع في عام",
            "start": 715.12,
            "duration": 6.279
        },
        {
            "text": "1930 مع ظهور الدراجات والسيارات وايضا",
            "start": 717.12,
            "duration": 7.64
        },
        {
            "text": "ظهرت الشوارع الاسفلتيه الحديثه فاصبحت",
            "start": 721.399,
            "duration": 6.201
        },
        {
            "text": "مهنه الافغان الرئيسيه وهي العمل على",
            "start": 724.76,
            "duration": 6.6
        },
        {
            "text": "البنيه التحتيه غير مطلوبه فتشتت المسلمين",
            "start": 727.6,
            "duration": 6.84
        },
        {
            "text": "ما بين طريقين",
            "start": 731.36,
            "duration": 3.08
        },
        {
            "text": "فالاغتصاب هاجر و خارج استراليا لكن",
            "start": 737.399,
            "duration": 8.361
        },
        {
            "text": "السؤال ما هو مصير الجمال يمكن مر عليكم",
            "start": 741.56,
            "duration": 6.839
        },
        {
            "text": "الجدل اللي اثارته الجمال في استراليا وكل",
            "start": 745.76,
            "duration": 5.92
        },
        {
            "text": "يوم يع لنا خبر عن عدم ضروره وجود هذه",
            "start": 748.399,
            "duration": 6.68
        },
        {
            "text": "الجمال والحكومه الاستراليه تبحث عن حلول",
            "start": 751.68,
            "duration": 7.0
        },
        {
            "text": "هذه المساله اصلها من الغان فبعد هجرته من",
            "start": 755.079,
            "duration": 7.641
        },
        {
            "text": "استراليا تم تسريح الجمال بعدد كبير منها",
            "start": 758.68,
            "duration": 8.12
        },
        {
            "text": "في الصحراء فتكا بشكل كبير فهذا التعامل",
            "start": 762.72,
            "duration": 7.32
        },
        {
            "text": "الخاطئ من الحكومه الاستراليه ادخلها في",
            "start": 766.8,
            "duration": 5.92
        },
        {
            "text": "ازمه كبيره ورايت الحكومه الاستراليه ان",
            "start": 770.04,
            "duration": 5.96
        },
        {
            "text": "وجود الجمال اصبح عب على الدوله لانها",
            "start": 772.72,
            "duration": 5.64
        },
        {
            "text": "تستهلك المياه القليله الموجوده في جنوب",
            "start": 776.0,
            "duration": 5.36
        },
        {
            "text": " راليا والجمال اذا ما وجدت مصدر من",
            "start": 778.36,
            "duration": 7.599
        },
        {
            "text": "مصادر المياه تعطش وتثور وتهاجم من حولها",
            "start": 781.36,
            "duration": 7.68
        },
        {
            "text": "فاشت تكوا المزارعين من اعتداء الجمال على",
            "start": 785.959,
            "duration": 6.041
        },
        {
            "text": "صنابير المياه والخزانات بالبدايه قررت",
            "start": 789.04,
            "duration": 6.08
        },
        {
            "text": "الحكومه الاستراليه تهجير هذه الجمال الى",
            "start": 792.0,
            "duration": 5.8
        },
        {
            "text": "دول ممكن انها تستفيد منها لكنها رات فيما",
            "start": 795.12,
            "duration": 6.68
        },
        {
            "text": "بعد ان هذا الحل مكلف ماديا وطويل زمنيا",
            "start": 797.8,
            "duration": 8.599
        },
        {
            "text": "ولجات الى حل اكثر قسوه فاتجهت الى",
            "start": 801.8,
            "duration": 7.44
        },
        {
            "text": "الاعدامات الجماعيه فالسؤال",
            "start": 806.399,
            "duration": 6.321
        },
        {
            "text": "كيف حدث هذا القتل جهزت وزاره البيئه",
            "start": 809.24,
            "duration": 7.719
        },
        {
            "text": "والمياه الاستراليه طائرات فيها قناصين",
            "start": 812.72,
            "duration": 7.84
        },
        {
            "text": "متمرسين وبداوا باطلاق النار على الجمال",
            "start": 816.959,
            "duration": 7.44
        },
        {
            "text": "اللي عددها مئات الالاف فبدلا من ايجاد حل",
            "start": 820.56,
            "duration": 7.8
        },
        {
            "text": "لمشكله الجفاف وندره المياه لجوا الى",
            "start": 824.399,
            "duration": 7.401
        },
        {
            "text": "اعدام الثروه الحيوانيه هذه الجمال اللي",
            "start": 828.36,
            "duration": 7.76
        },
        {
            "text": "يتم ابادتها تدريجيا تشهد على وجود الغان",
            "start": 831.8,
            "duration": 9.2
        },
        {
            "text": "كما شهدت عليهم الجدران والمناجم والمساجد",
            "start": 836.12,
            "duration": 8.2
        },
        {
            "text": "وفي عام 2023 اعلن المركز المصرفي",
            "start": 841.0,
            "duration": 6.88
        },
        {
            "text": "الاسترالي عن تغيير شكل العمله فعمله اللي",
            "start": 844.32,
            "duration": 7.199
        },
        {
            "text": "كانت تحمل صور ملوك بريطانيا اصبحت تحمل",
            "start": 847.88,
            "duration": 7.04
        },
        {
            "text": "صور السكان الاصليين لاستراليا وفي هذه",
            "start": 851.519,
            "duration": 6.68
        },
        {
            "text": "المبادره توثيق لتاريخ وجودهم في الارض",
            "start": 854.92,
