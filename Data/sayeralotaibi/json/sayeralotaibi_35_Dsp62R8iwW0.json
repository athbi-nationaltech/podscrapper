        },
        {
            "text": "واقنى الانف يحكم سبع سنين ويملا الارض",
            "start": 877.24,
            "duration": 8.64
        },
        {
            "text": "بالعدل فيتجه المسلمين بقيادته لتحرير",
            "start": 881.48,
            "duration": 7.68
        },
        {
            "text": "المسجد الاقصى واستعاده القدس مره اخرى",
            "start": 885.88,
            "duration": 7.44
        },
        {
            "text": "بعد ان سلبها اليهود بعدها يظهر عدو ضخم",
            "start": 889.16,
            "duration": 8.08
        },
        {
            "text": "يصارع المسلمين والروم كذلك هذا العدو غير",
            "start": 893.32,
            "duration": 7.92
        },
        {
            "text": "معروف والله اعلم من يكون فيتحد المسلمين",
            "start": 897.24,
            "duration": 7.92
        },
        {
            "text": "والروم مؤقتا وينتصرون على هذا العدو",
            "start": 901.24,
            "duration": 6.8
        },
        {
            "text": "المشترك وبعد انتصار المسلمين والروم على",
            "start": 905.16,
            "duration": 7.44
        },
        {
            "text": "العدو وهزيمته في ارض داره يشتعل هناك",
            "start": 908.04,
            "duration": 8.12
        },
        {
            "text": "صراع اخر هذا الصراع هو ما بين المسلمين",
            "start": 912.6,
            "duration": 7.2
        },
        {
            "text": "والروم اللي تنتهي الهدنه بينهم فتحدث",
            "start": 916.16,
            "duration": 7.64
        },
        {
            "text": "بينهم معركه كبرى في بلاد الشام جيش الروم",
            "start": 919.8,
            "duration": 7.279
        },
        {
            "text": "بهذه اللحظه يكون اكبر من جيش المسلمين",
            "start": 923.8,
            "duration": 8.839
        },
        {
            "text": "بمراحل ولكن ينتصر المسلمين رغم ذلك بعدها",
            "start": 927.079,
            "duration": 9.841
        },
        {
            "text": "يظهر المسيح الدجال وتبدا علامات الساعه",
            "start": 932.639,
            "duration": 8.361
        },
        {
            "text": "الكبرى بالظهور الى ان ينتهي العالم بامر",
            "start": 936.92,
            "duration": 7.479
        },
        {
            "text": "من الله عز وجل يتفق المسيحيين واليهود",
            "start": 941.0,
            "duration": 6.639
        },
        {
            "text": "بطريقه نهايه العالم ويوجد اختلاف بسيط",
            "start": 944.399,
            "duration": 5.68
        },
        {
            "text": "بين رواياته اما الروايه الاسلاميه",
            "start": 947.639,
            "duration": 5.76
        },
        {
            "text": "الصحيحه فهي تربط نهايه العالم بعلامات",
            "start": 950.079,
            "duration": 6.281
        },
        {
            "text": "الساعه اللي ما يعرف موعدها غير الله",
            "start": 953.399,
            "duration": 6.721
        },
        {
            "text": "سبحانه وتعالى قدمت لكم اليوم معركه نهايه",
            "start": 956.36,
            "duration": 7.599
        },
        {
            "text": "الدنيا بوجهه نظر المسيحيين واليهود هذه",
            "start": 960.12,
            "duration": 6.199
        },
        {
            "text": "المعركه اللي يؤمن فيها الكثير من",
            "start": 963.959,
            "duration": 6.32
        },
        {
            "text": "المؤثرين ويتخذون قراراتهم السياسيه بناء",
            "start": 966.319,
            "duration": 6.561
        },
        {
            "text": "عليها",
            "start": 970.279,
            "duration": 2.601
        },
        {
            "text": "[موسيقى]",
            "start": 982.05,
            "duration": 3.19
        },
        {
            "text": "فالمعركة",
            "start": 987.519,
            "duration": 3.0
        },
