            "start": 370.74,
            "duration": 6.66
        },
        {
            "text": "والتلفزيون وكذلك القنوات العالميه بحكم",
            "start": 373.8,
            "duration": 6.66
        },
        {
            "text": "ان هذه العصابه كانت خطيره بالفتره هذيك",
            "start": 377.4,
            "duration": 6.239
        },
        {
            "text": "كان هناك الكثير من عصابات قطاع الطرق لكن",
            "start": 380.46,
            "duration": 6.9
        },
        {
            "text": "عصابه رشاش كانت معروفه بحكم انه هي قريبه",
            "start": 383.639,
            "duration": 6.481
        },
        {
            "text": "من الرياض بداوا الناس اللي يروحون الحج",
            "start": 387.36,
            "duration": 5.82
        },
        {
            "text": "والعمره وي على الطريق اللي فيه عصابه",
            "start": 390.12,
            "duration": 7.079
        },
        {
            "text": "رشاش ياخذون معاهم مجموعات مسلحه بمعنى ان",
            "start": 393.18,
            "duration": 6.84
        },
        {
            "text": "الحجاج يطلعون ومعاهم مرافقين يحمونهم",
            "start": 397.199,
            "duration": 6.06
        },
        {
            "text": "واللي يحمونهم كانوا مسلحين وهذا ياكد ان",
            "start": 400.02,
            "duration": 5.88
        },
        {
            "text": "عصابه رشاش كانت خطره البعض منكم ممكن",
            "start": 403.259,
            "duration": 5.641
        },
        {
            "text": "يتساءل ويتفاجئ وين دور الشرطه والامن",
            "start": 405.9,
            "duration": 6.18
        },
        {
            "text": "وشلون مو قادرين على افراد مسلحين اللي",
            "start": 408.9,
            "duration": 5.94
        },
        {
            "text": "جعل مهمه القبض على عصابه رشاش بهذه",
            "start": 412.08,
            "duration": 5.58
        },
        {
            "text": "الصعوبه هو بسبب المساحات الكبيره في",
            "start": 414.84,
            "duration": 5.639
        },
        {
            "text": "الصحراء فكان سهل على العصابه انه تخفون",
            "start": 417.66,
            "duration": 5.759
        },
        {
            "text": "وصعب على الشرطه ان يطلعونهم وكانت عصابه",
            "start": 420.479,
            "duration": 6.66
        },
        {
            "text": "رشاش تستخدم اسلوب خاص فيها فكانوا يضعون",
            "start": 423.419,
            "duration": 7.141
        },
        {
            "text": "مسامير واسلاك شائكه على خط السريع فاذا",
            "start": 427.139,
            "duration": 6.18
        },
        {
            "text": "مروا السيارات تنفجر الاطارات او الكفرات",
            "start": 430.56,
            "duration": 5.82
        },
        {
            "text": "او التاير بالكويتي احنا نسميه فيوقفون",
            "start": 433.319,
            "duration": 5.82
        },
        {
            "text": "وبعدها يسلبونهم وهم كان عندهم اسلوب ثاني",
            "start": 436.38,
            "duration": 5.7
        },
        {
            "text": "فكانوا يوقفون مركبه كبيره يسكرون فيها",
            "start": 439.139,
            "duration": 5.661
        },
        {
            "text": "الطريق سواء شاحنه او تنكر او لوري",
            "start": 442.08,
            "duration": 5.459
        },
        {
            "text": "فبهاللحظه لما المسافرين يشوفون شاحنه",
            "start": 444.8,
            "duration": 5.92
        },
        {
            "text": "كبيره مسكره الشارع اكيد يوقفون فيخرجون",
            "start": 447.539,
            "duration": 7.141
        },
        {
            "text": "لهم العصابه من كل مكان وينهبونه فاستمرت",
            "start": 450.72,
            "duration": 7.5
        },
        {
            "text": "العصابه تمارس عملياتها بشكل مستمر الى ان",
            "start": 454.68,
            "duration": 6.54
        },
        {
            "text": "اتى اليوم اللي ما احسبوا حسابه قبل لا",
            "start": 458.22,
            "duration": 5.699
        },
        {
            "text": "اكمل القصه ابي منكم طلب بسيط انكم تضغطون",
            "start": 461.22,
            "duration": 6.12
        },
        {
            "text": "على زر الاشتراك ودي انه نوصل الى 100 الف",
            "start": 463.919,
            "duration": 6.18
        },
        {
            "text": "مشترك خلال الفتره القادمه وطبعا معاها",
            "start": 467.34,
            "duration": 5.04
        },
        {
            "text": "اللايك ومشاركه الحلقه اذا اعجبتكم",
            "start": 470.099,
            "duration": 4.681
        },
        {
            "text": "وتشوفون في ناس راح يحبون الموضوع المهم",
            "start": 472.38,
            "duration": 5.28
        },
        {
            "text": "في يوم من الايام كانت قوات الامن تلاحق",
            "start": 474.78,
            "duration": 6.419
        },
        {
            "text": "عصابه رشاش لكن الصدمه الكبيره ان احد",
            "start": 477.66,
            "duration": 7.319
        },
        {
            "text": "رجال الامن تم قتله من قبل عصابه رشاش",
            "start": 481.199,
            "duration": 8.101
        },
        {
            "text": "بهاللحظه الدوله استنفرت وقررت انه توقف",
            "start": 484.979,
            "duration": 7.381
        },
        {
            "text": "اصابه رشاش عند حدها فقاموا يترصدون لهم",
            "start": 489.3,
            "duration": 6.299
        },
        {
            "text": "ويلاحقونهم وانتشرت سيارات الشرطه على كل",
            "start": 492.36,
            "duration": 6.119
        },
        {
            "text": "الطرق السريعه حتى طيارات الهليكوبتر",
            "start": 495.599,
            "duration": 6.0
        },
        {
            "text": "طيروها وكانت موجوده في كل مكان فالشرطه",
            "start": 498.479,
            "duration": 5.461
        },
        {
            "text": "كانوا ناوين انه يلهون موضوع العصابه",
            "start": 501.599,
            "duration": 5.341
        },
        {
            "text": "لغايه احد الايام تم الاشتباه باحد",
            "start": 503.94,
            "duration": 6.3
        },
        {
            "text": "السيارات فيها افراد العصابه وكانوا قاعد",
            "start": 506.94,
            "duration": 6.659
        },
        {
            "text": "يخططون انه يقولون على سياره ثانيه فاتت",
            "start": 510.24,
            "duration": 6.539
        },
        {
            "text": "قوات الامن واعتقلت هذيل الاشخاص فاعترفوا",
            "start": 513.599,
            "duration": 5.281
        },
        {
            "text": "اللي كانوا موجودين في السياره بانهم احد",
            "start": 516.779,
            "duration": 5.7
        },
        {
            "text": "افراد عصابه رشاش فكان هذا اول الخيط من",
            "start": 518.88,
            "duration": 5.88
        },
        {
            "text": "الجريمه فاذكروا الكثير من المعلومات",
            "start": 522.479,
            "duration": 5.881
        },
        {
            "text": "الخاصه عن العصابه وعن عملياتهم اللي وصلت",
            "start": 524.76,
            "duration": 7.199
        },
        {
            "text": "الى اكثر من 400 عمليه وهم اعترفوا على",
            "start": 528.36,
            "duration": 6.3
        },
        {
            "text": "افراد العصابه وعلى الرؤوس الكبيره اللي",
            "start": 531.959,
            "duration": 6.541
        },
        {
            "text": "فيها فتم اعتقال 17 شخص منهم واعترفوا",
            "start": 534.66,
            "duration": 7.679
        },
        {
            "text": "بجميع جرائمهم وتم القصاص ب 12 شخص من اصل",
            "start": 538.5,
            "duration": 6.899
        },
        {
            "text": "17 تبقى الان الرؤوس الكبيره من العصابه",
            "start": 542.339,
            "duration": 6.141
        },
        {
            "text": "فتم نشر صورهم في جميع انحاء المملكه",
            "start": 545.399,
            "duration": 6.901
        },
        {
            "text": "وعمموا بعد ما زاد الضغط الامني والخناق",
            "start": 548.48,
            "duration": 6.52
        },
        {
            "text": "عليهم فاجتمعت الرؤوس الكبيره من اعضاء",
            "start": 552.3,
            "duration": 6.599
        },
        {
            "text": "العصابه ووضعوا خطه اللي هي انه يتفرقون",
            "start": 555.0,
            "duration": 6.899
        },
        {
            "text": "باماكن مختلفه فتفرقوا عن بعض وكل واحد",
            "start": 558.899,
            "duration": 5.94
        },
        {
            "text": "فيهم راح مكان معين المتبقين الان من",
            "start": 561.899,
            "duration": 7.44
        },
        {
            "text": "الاصابه ارشاش قحص قعيد الاخوان مهل",
            "start": 564.839,
            "duration": 8.701
        },
        {
            "text": "وسلطان ومعاهم هدايا للسته المطلوبين عند",
            "start": 569.339,
            "duration": 7.56
        },
        {
            "text": "الشرطه قعيد تم القبض عليه بعد مطاردات",
            "start": 573.54,
            "duration": 5.94
        },
        {
            "text": "طويله واستسلم بالاخير اما بالنسبه",
            "start": 576.899,
            "duration": 6.241
        },
        {
            "text": "للاخوان مهل وسلطان فكانت قصه اعتقالهم",
            "start": 579.48,
            "duration": 6.78
        },
        {
            "text": "جدا غريبه الشرطه ارصدت مكانهم وتمت",
            "start": 583.14,
            "duration": 6.78
        },
        {
            "text": "مطاردتهم فتركوا سيارتهم وتوجهوا بسياره",
            "start": 586.26,
            "duration": 6.48
        },
        {
            "text": "ثانيه بعد ما اتركه سيارتهم الاولى وراحوا",
            "start": 589.92,
            "duration": 5.64
        },
        {
            "text": "الى سياره ثانيه الشرطه فتشوا السياره",
            "start": 592.74,
            "duration": 6.84
        },
        {
            "text": "الاولى ولقوا فيها وصل مصبغه هذه المصبغه",
            "start": 595.56,
            "duration": 7.2
        },
        {
            "text": "كانت مصبغه ملابس موجوده في القصيم فراحوا",
            "start": 599.58,
            "duration": 6.24
        },
        {
            "text": "الشرطه وقعدوا يراقبون المغسله فجلسوا",
            "start": 602.76,
            "duration": 6.18
        },
        {
            "text": "يراقبون هذا المكان لمده ايام الى ان اتت",
            "start": 605.82,
            "duration": 6.9
        },
        {
            "text": "المفاجاه فتفاجئ واحد من عمال المغسله انه",
            "start": 608.94,
            "duration": 7.019
        },
        {
            "text": "قدامه واحد من اخطر المطلوبين للعداله",
            "start": 612.72,
            "duration": 6.42
        },
        {
            "text": "فحطوا عنده ملابسهم على اساس انه يجون",
            "start": 615.959,
            "duration": 5.761
        },
        {
            "text": "اليوم الثاني ويستلمونهم وطبعنا المباحث",
            "start": 619.14,
            "duration": 6.12
        },
        {
            "text": "كانت ترصد الحدث وكانت الخطه ان المباحث",
            "start": 621.72,
            "duration": 6.48
        },
        {
            "text": "يتنكرون بملابس عمال المغسله فاتوا",
            "start": 625.26,
            "duration": 5.1
        },
        {
            "text": "الاخوان الاثنين ثاني يوم عشان يستلمون",
            "start": 628.2,
            "duration": 5.579
        },
        {
            "text": "ملابسهم المهل عشان يستلم الملابس وكان",
            "start": 630.36,
            "duration": 6.24
        },
        {
            "text": "اخوه سلطان ينتظره في السياره اول ما دخل",
            "start": 633.779,
            "duration": 6.841
        },
        {
            "text": "مهل تم القبض عليه وهو داخل المغسله ولما",
            "start": 636.6,
            "duration": 6.78
        },
        {
            "text": "تاخر مهل وما طلع سلطان كان شاك بالموضوع",
            "start": 640.62,
            "duration": 6.839
        },
        {
            "text": "فنزل معاه سلاحه اول ما دخل سلطان تفاجئ",
            "start": 643.38,
            "duration": 8.04
        },
        {
            "text": "ان اخوه مهل تم القبض عليه بهاللحظه سلطان",
            "start": 647.459,
            "duration": 7.741
        },
        {
            "text": "ارتبك ورجال المباحث كانوا حوالينا فصارخ",
            "start": 651.42,
            "duration": 6.359
        },
        {
            "text": "مهل على اخوه سلطان وقال له اقتلني",
            "start": 655.2,
            "duration": 5.1
        },
        {
            "text": "واقتلهم لانه كان يعرف انه مصيرهم",
            "start": 657.779,
            "duration": 5.281
        },
        {
            "text": "بالنهايه الاعدام فكان يبي على الاقل انه",
            "start": 660.3,
            "duration": 5.64
        },
        {
            "text": "اخوه سلطان يقدر ينحاش وما كان يبي انه",
            "start": 663.06,
            "duration": 6.24
        },
        {
            "text": "يستسلم للشرطه لكن كان سلطان متردد وما",
            "start": 665.94,
            "duration": 7.079
        },
        {
            "text": "يبي يقتل اخوه هنا مهل صرخ مره ثانيه وقال",
            "start": 669.3,
            "duration": 7.2
        },
        {
            "text": "له يا ولد فلانه اذبحني واذبحهم اهني",
            "start": 673.019,
            "duration": 7.621
        },
        {
            "text": "سلطان ما قدر فتراجع واستسلم وتم اعتقالهم",
            "start": 676.5,
            "duration": 7.98
        },
        {
            "text": "بدون لا تطلق الشرطه رصاصه واحده اما",
            "start": 680.64,
            "duration": 6.42
        },
        {
            "text": "بالنسبه لمصلح فما كان يبي انه تكون",
            "start": 684.48,
            "duration": 7.44
        },
        {
            "text": "نهايته عاديه فخطت حق شيء اكبر واخطر مصلح",
            "start": 687.06,
            "duration": 7.86
        },
        {
            "text": "كان يراقب بوابه الديوان الملكي على اكثر",
            "start": 691.92,
            "duration": 6.72
        },
        {
            "text": "من يوم وخصوصا بوابه الخروج لان بوابه",
            "start": 694.92,
            "duration": 6.96
        },
        {
            "text": "الخروج التفتيش فيها ضعيف بحكم انه هي",
            "start": 698.64,
            "duration": 5.58
        },
        {
            "text": "بوابه الطلعه بحكم ان الناس اللي يمرون",
            "start": 701.88,
            "duration": 5.82
        },
        {
            "text": "فيها يكونون متفتشين سابقا فتم مصلح يراقب",
            "start": 704.22,
            "duration": 6.84
        },
        {
            "text": "بوابه الخروج لغايه ساعه الصفر شاف ان",
            "start": 707.7,
            "duration": 6.12
        },
        {
            "text": "الوقت المناسب فدخل بسيارته من البوابه",
            "start": 711.06,
            "duration": 6.3
        },
        {
            "text": "واقتحم الديوان الملكي كانت نيته انه يقتل",
            "start": 713.82,
            "duration": 6.12
        },
        {
            "text": "الملك فهد رحمه الله عليه بعد ما تجاوز",
            "start": 717.36,
            "duration": 5.76
        },
        {
            "text": "البوابات ما كان يفصله عن الملك الا باب",
            "start": 719.94,
            "duration": 6.72
        },
        {
            "text": "واحد لكن ولله الحمد صاده واحد من رجال",
            "start": 723.12,
            "duration": 7.14
        },
        {
            "text": "الامن واطلق عليه النار واقتله طبعا نيه",
            "start": 726.66,
            "duration": 6.0
        },
        {
            "text": "مصلح الاغتيال الملك فهد رحمه الله عليه",
            "start": 730.26,
            "duration": 5.28
        },
        {
            "text": "بسبب انه ما اوقف حكم القصاص على احد",
            "start": 732.66,
            "duration": 6.66
        },
        {
            "text": "اقربائه الان اللي تبقوا من العصابه شخصين",
            "start": 735.54,
            "duration": 7.859
        },
        {
            "text": "الاول رشاش والثاني قريبه قحص فتم التعميم",
            "start": 739.32,
            "duration": 7.019
        },
        {
            "text": "على اوصافهم وانشروا اصفرهم بالجرائد وكل",
            "start": 743.399,
            "duration": 6.541
        },
        {
            "text": "محطات التلفزيون وكذلك حطوا مكافاه ماليه",
            "start": 746.339,
            "duration": 7.321
        },
        {
            "text": "على اي شخص يدلي باي معلومه عن الشخصين",
            "start": 749.94,
            "duration": 7.98
        },
        {
            "text": "بدات الشرطه تصغر الدائره عليهم لغايه ما",
            "start": 753.66,
            "duration": 7.739
        },
        {
            "text": "تمرضت مكان قحص قحص لما عرف ان الشرطه",
            "start": 757.92,
            "duration": 6.78
        },
        {
            "text": "والمباحث لاحقينه على طول انحاش وتمت",
            "start": 761.399,
            "duration": 6.18
        },
        {
            "text": "مطاردته مطاردته كانت طويله وايضا",
            "start": 764.7,
            "duration": 6.0
        },
        {
            "text": "بالاسلحه فكان هناك تبادل اطلاق نار ما",
            "start": 767.579,
            "duration": 6.0
        },
        {
            "text": "بين قحص والشرطه الى ان اصابت الشرطه",
            "start": 770.7,
            "duration": 5.0
        },
        {
            "text": "سياره",
            "start": 773.579,
            "duration": 6.541
        },
        {
            "text": "ووقفت لكن قحص ما استسلم وانهاش وصعد الى",
            "start": 775.7,
            "duration": 7.9
        },
        {
            "text": "احد الجبال وكان معروف انه قناص فاختفى",
            "start": 780.12,
            "duration": 6.24
        },
        {
            "text": "بين الجبال وبدا يطلق النار على الشرطه",
            "start": 783.6,
            "duration": 6.0
        },
        {
            "text": "الشرطه كانوا يطلبون منه انه يستسلم لكن",
            "start": 786.36,
            "duration": 6.539
        },
        {
            "text": "قحص كان يرد عليهم باطلاق النار فجاب",
            "start": 789.6,
            "duration": 6.78
        },
        {
            "text": "والدته الى الجبل بطائره هليكوبتر قالوا",
            "start": 792.899,
            "duration": 6.481
        },
        {
            "text": "يمكن ان والدته يكون لها دور وتقنعه",
            "start": 796.38,
            "duration": 6.3
        },
        {
            "text": "بالاستسلام لكن بعد ما جابه والدتها قالت",
            "start": 799.38,
            "duration": 7.56
        },
        {
            "text": "لهم قحص ما راح تعتقلونه ووعي فتقول هذا",
            "start": 802.68,
            "duration": 7.44
        },
        {
            "text": "ولدي وانا اعرفه ما راح يسمع كلامكم ولا",
            "start": 806.94,
            "duration": 7.38
        },
        {
            "text": "كلامي اكرر ما راح تصيدونه روحي وفعلا",
            "start": 810.12,
            "duration": 7.62
        },
        {
            "text": "كانوا الشرطه يتبادلون باطلاق النار لغايه",
            "start": 814.32,
            "duration": 7.92
        },
        {
            "text": "ما خلصت ذخيره قحص وما عنده الا رصاصه",
            "start": 817.74,
            "duration": 7.8
        },
        {
            "text": "واحده فعرف قحص ان هذه الرصاصه ما راح",
            "start": 822.24,
            "duration": 5.88
        },
        {
            "text": "يقتل فيها الشرطه وما راح يكون لها فايده",
            "start": 825.54,
            "duration": 7.32
        },
        {
            "text": "فقرر انه يذبح نفسه ولا انه يصيدونه وفعلا",
            "start": 828.12,
            "duration": 7.82
        },
        {
            "text": "هذا اللي صار هو وضع المسدس على راسه",
            "start": 832.86,
            "duration": 7.32
        },
        {
            "text": "وقتها لنفسه فكانت رصاصه الموت والحين",
            "start": 835.94,
            "duration": 8.32
        },
        {
            "text": "باقي الشخص المهم الشخص الاخضر اللي وضعه",
            "start": 840.18,
            "duration": 8.64
        },
        {
            "text": "مكافاه ماليه كبيره جدا للوصول له فاللي",
            "start": 844.26,
            "duration": 7.62
        },
        {
            "text": "كان باقي رئيس العصابه اللي تسمت العصابه",
            "start": 848.82,
            "duration": 6.78
        },
        {
            "text": "باسمه ارشاش في هذه الفتره ارشاش اختفى",
            "start": 851.88,
            "duration": 7.139
        },
        {
            "text": "وما كان له اثر فبعد البحث والتحري",
            "start": 855.6,
            "duration": 7.56
        },
        {
            "text": "والتقصي وصلوا المباحث لمعلومه ما احسبوا",
            "start": 859.019,
            "duration": 7.461
        },
        {
            "text": "حساب فتخيلوا ارشاش هرب وراح لليمن",
            "start": 863.16,
            "duration": 6.66
        },
        {
            "text": "وبالاخص الى احد القبائل اليمنيه الكبيره",
            "start": 866.48,
            "duration": 6.34
        },
        {
            "text": "وهناك",
            "start": 869.82,
            "duration": 3.0
        },
        {
            "text": "[موسيقى]",
            "start": 883.29,
            "duration": 3.06
        },
        {
            "text": "على راسه واستحوه في الارض وخذوا منه",
            "start": 930.899,
            "duration": 7.201
        },
        {
            "text": "سلاحه واربطوه وسلوا الشرطه هنا رشاش",
            "start": 934.019,
            "duration": 7.38
        },
        {
            "text": "انصدم متضايق بشكل كبير لان الناس اللي",
            "start": 938.1,
            "duration": 6.179
        },
        {
            "text": "كانوا قراب منه قالوا له ان القبيله اللي",
            "start": 941.399,
            "duration": 5.821
        },
        {
            "text": "انت قاعد عندها احتمال كبير انه يخونون",
            "start": 944.279,
            "duration": 5.941
        },
        {
            "text": "فيك وانصحوه انه يروح لقبيله ثانيه عشان",
            "start": 947.22,
            "duration": 6.54
        },
        {
            "text": "يحمونه لكن ما صدقهم وقال راح اقعد عند",
            "start": 950.22,
            "duration": 6.299
        },
        {
            "text": "شيخ القبيله لانه ما قصر معاي فبهذه",
            "start": 953.76,
            "duration": 5.879
        },
        {
            "text": "الطريقه تم تسليم ارشاش للامن السعودي وفي",
            "start": 956.519,
            "duration": 7.38
        },
        {
            "text": "سنه 1989 تم تنفيذ حكم القصاص على رشاش",
            "start": 959.639,
            "duration": 8.521
        },
        {
            "text": "وكان عمره في ذاك الوقت 29 سنه بهاللحظه",
            "start": 963.899,
            "duration": 8.161
        },
        {
            "text": "انتهت قصه اخطر عصابه امتهنت عمليات النهب",
            "start": 968.16,
            "duration": 7.799
        },
        {
            "text": "وترويع المسافرين والحجاج فتم تاسيس قطاع",
            "start": 972.06,
            "duration": 5.76
        },
        {
            "text": "امن الطرق بالمملكه العربيه السعوديه",
            "start": 975.959,
            "duration": 5.761
        },
        {
            "text": "علشان يحمون المسافرين ولله الحمد اليوم",
            "start": 977.82,
            "duration": 7.62
        },
        {
            "text": "ظاهره قطاع الطرق انتهت يقول نبينا محمد",
            "start": 981.72,
            "duration": 7.619
        },
        {
            "text": "صلى الله عليه وسلم ياتي على الناس زمان",
            "start": 985.44,
            "duration": 8.459
        },
        {
            "text": "لا يبالي الم رء ما اخذ ام من حلال ام من",
            "start": 989.339,
            "duration": 9.201
        },
        {
            "text": "حرام لذلك ان العدل اقل تكلفه من الظلم",
            "start": 993.899,
            "duration": 9.921
        },
        {
            "text": "والامن اقل كلفه من الحرب",
            "start": 998.54,
            "duration": 5.28
        },
        {
            "text": "[موسيقى]",
            "start": 1007.78,
            "duration": 9.829
        }
    ]
}