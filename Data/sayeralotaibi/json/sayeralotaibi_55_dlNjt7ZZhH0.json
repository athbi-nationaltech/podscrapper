        },
        {
            "text": "الصحراء فهناك يتعلم الفروسيه في وقت مبكر",
            "start": 389.34,
            "duration": 7.799
        },
        {
            "text": "فيبرز بين اقرانه ويثبت فروسيته في ميادين",
            "start": 392.72,
            "duration": 7.599
        },
        {
            "text": "وفي وقتهم كانت الحياه قاسيه خاليه من",
            "start": 397.139,
            "duration": 6.06
        },
        {
            "text": "الرفاهيه فاعتادوا في الجاهليه على الفقر",
            "start": 400.319,
            "duration": 6.181
        },
        {
            "text": "والجوع والشده وبسبب هذه الظروف القاسيه",
            "start": 403.199,
            "duration": 6.541
        },
        {
            "text": "اظهرت عادات برزت عند العرب فكان اذا",
            "start": 406.5,
            "duration": 6.9
        },
        {
            "text": "احدهم وصل الى شده الفقر ياخذ اسرته الى",
            "start": 409.74,
            "duration": 7.019
        },
        {
            "text": "مكان يسمى بالخبره يمكثون فيه حتى يموت",
            "start": 413.4,
            "duration": 6.0
        },
        {
            "text": "الجميع من الجوع هذه العاده في الجاهيه",
            "start": 416.759,
            "duration": 6.66
        },
        {
            "text": "تسمى بالاعذار وبني مخزوم كادو ان يموتون",
            "start": 419.4,
            "duration": 8.22
        },
        {
            "text": "جميعا من الجوع لكن وصل خبرهم الى هاشم بن",
            "start": 423.419,
            "duration": 8.22
        },
        {
            "text": "عد مناف وهو يعتبر من كبار التجار في ذاك",
            "start": 427.62,
            "duration": 7.56
        },
        {
            "text": "الوقت هاشم بنعد مناف استاء من وجود الجهل",
            "start": 431.639,
            "duration": 7.261
        },
        {
            "text": "والفقر اللي كان موجود عند اهل مكه فسعى",
            "start": 435.18,
            "duration": 7.56
        },
        {
            "text": "لتغيير هذه العاده اول خطوه اخطاها بتقسيم",
            "start": 438.9,
            "duration": 7.44
        },
        {
            "text": "القبيله الى عشائر وامر كل غني منهم",
            "start": 442.74,
            "duration": 7.2
        },
        {
            "text": "بتقسيم ماله مع الفقراء من عشيرته فصار",
            "start": 446.34,
            "duration": 6.72
        },
        {
            "text": "الفقير في القبيله يتساوى مع الغني وما",
            "start": 449.94,
            "duration": 5.699
        },
        {
            "text": "كانت هذه الخطوه هي الوحيده فكانت خطوه",
            "start": 453.06,
            "duration": 7.02
        },
        {
            "text": "مؤقته فعلم اصول التجاره ونظم لهم رحلتين",
            "start": 455.639,
            "duration": 7.921
        },
        {
            "text": "رحله الى الشام ورحله الى اليمن في الشام",
            "start": 460.08,
            "duration": 6.54
        },
        {
            "text": "علمهم تجاره الفواكه في فصل الصيف وفي",
            "start": 463.56,
            "duration": 6.18
        },
        {
            "text": "اليمن علمهم تجاره المحصولات الزراعيه في",
            "start": 466.62,
            "duration": 6.479
        },
        {
            "text": "فصل الشتاء وكانت النتيجه بان جاء الخير",
            "start": 469.74,
            "duration": 7.62
        },
        {
            "text": "من الشام واليمن الى مكه واصبح سكان مكه",
            "start": 473.099,
            "duration": 8.281
        },
        {
            "text": "في حال افضل وانتهت ظاهره الاعذار رغم",
            "start": 477.36,
            "duration": 6.959
        },
        {
            "text": "تحسن الاحوال الاقتصاديه في مكه الا انهم",
            "start": 481.38,
            "duration": 6.24
        },
        {
            "text": "كانوا يكفرون بالنعمه ولا يشكرون الله ومن",
            "start": 484.319,
            "duration": 6.0
        },
        {
            "text": "الامور اللي عززت الحاله الاقتصاديه لاهل",
            "start": 487.62,
            "duration": 5.639
        },
        {
            "text": "الجاهليه هو المكان الجغرافي المميز",
            "start": 490.319,
            "duration": 6.961
        },
        {
            "text": "فكانوا متواجدين في وسط مسطحات مائيه فبحر",
            "start": 493.259,
            "duration": 6.06
        },
        {
            "text": "الروم كان موجود في الشمال اللي يسمى",
            "start": 497.28,
            "duration": 4.74
        },
        {
            "text": "اليوم البحر المتوسط ومن الجنوب بحر",
            "start": 499.319,
            "duration": 5.1
        },
        {
            "text": "الحبشه واللي يسمى اليوم المحيط الهندي",
            "start": 502.02,
            "duration": 6.119
        },
        {
            "text": "هذا من جهه البحر اما من جهه البر فليابسه",
            "start": 504.419,
            "duration": 7.86
        },
        {
            "text": "ممتده وواسعه ويتخللها مسطحات مائيه فمن",
            "start": 508.139,
            "duration": 7.681
        },
        {
            "text": "الغرب البحر الاحمر ومن الشرق بحر فارس",
            "start": 512.279,
            "duration": 6.721
        },
        {
            "text": "واللي يسمى اليوم الخليج العربي وفي ذاك",
            "start": 515.82,
            "duration": 5.82
        },
        {
            "text": "الوقت قام هاشم وهو جد النبي صلى الله",
            "start": 519.0,
            "duration": 6.24
        },
        {
            "text": "عليه وسلم بخطوه حكيمه فقام بترتيب",
            "start": 521.64,
            "duration": 7.44
        },
        {
            "text": "القوافل التجاريه التي تاتي ذهابا وايابا",
            "start": 525.24,
            "duration": 8.039
        },
        {
            "text": "من مكه والى مكه وقام بتامين هذه القوافل",
            "start": 529.08,
            "duration": 8.22
        },
        {
            "text": "عن طريق عمل عهود مع رؤساء القبائل اكرم",
            "start": 533.279,
            "duration": 7.081
        },
        {
            "text": "الله العرب وبعث فيهم النبي محمد صلى الله",
            "start": 537.3,
            "duration": 6.24
        },
        {
            "text": "عليه وسلم وهو من نسل هاشم بن عبد مناف",
            "start": 540.36,
            "duration": 6.24
        },
        {
            "text": "ورفع من شان العرب بعد ما كانوا امه",
            "start": 543.54,
            "duration": 5.52
        },
        {
            "text": "جاهليه وبعد وفاه النبي صلى الله عليه",
            "start": 546.6,
            "duration": 6.419
        },
        {
            "text": "وسلم بدات مرحله جديده من تاريخ العرب وهي",
            "start": 549.06,
            "duration": 7.44
        },
        {
            "text": "مرحله الفتوحات الاسلاميه الكبرى فهزموا",
            "start": 553.019,
            "duration": 6.541
        },
        {
            "text": "اعظم امبراطوريتين وهم فارس والروم",
            "start": 556.5,
            "duration": 6.54
        },
        {
            "text": "وازدادت قوه العرب مع هذا الوقت لكن الحين",
            "start": 559.56,
            "duration": 7.2
        },
        {
            "text": "خل نمشي في مسار اصل العرب من ذاك الوقت",
            "start": 563.04,
            "duration": 6.96
        },
        {
            "text": "الى يومنا هذا والان عندنا عرب العاربه",
            "start": 566.76,
            "duration": 6.72
        },
        {
            "text": "وعرب المستعربه اما العرب البائده فهم",
            "start": 570.0,
            "duration": 6.959
        },
        {
            "text": "اوبيدو عرب العاربه وعرب المستعربه كانوا",
            "start": 573.48,
            "duration": 7.799
        },
        {
            "text": "في قبائل تسمى القبائل العدنانيه هذه",
            "start": 576.959,
            "duration": 6.981
        },
        {
            "text": "القبائل كان تسكن في اماكن خاصه فيها",
            "start": 581.279,
            "duration": 6.12
        },
        {
            "text": "وهناك قبائل اخرى اسمها القبائل القحطانيه",
            "start": 583.94,
            "duration": 7.54
        },
        {
            "text": "كذلك لها اماكن خاصه فيها لكن كلهم متفقين",
            "start": 587.399,
            "duration": 7.141
        },
        {
            "text": "انهم عرب فاذا حصلت حروب من اطراف خارجيه",
            "start": 591.48,
            "duration": 6.66
        },
        {
            "text": "هذه القبائل تتحد في المواجهه لكن بين",
            "start": 594.54,
            "duration": 6.239
        },
        {
            "text": "بعضهم هم عارفين الاختلاف لو تلاحظون",
            "start": 598.14,
            "duration": 5.28
        },
        {
            "text": "تسلسل العرب كانت بدات في العراق بعدها",
            "start": 600.779,
            "duration": 6.361
        },
        {
            "text": "القبائل والان اتجهنا بان تتوسع الى",
            "start": 603.42,
            "duration": 6.359
        },
        {
            "text": "البطون وتفرعات فهؤلاء العرب ما كانوا",
            "start": 607.14,
            "duration": 4.4
        },
        {
            "text": "مقتصرين على الجزيره العربيه",
            "start": 609.779,
            "duration": 4.861
        },
        {
            "text": "فامتدوا الى الشام والعراق والاراضي",
            "start": 611.54,
            "duration": 6.7
        },
        {
            "text": "المقدسه وهذا الكلام يدل على وجود ممالك",
            "start": 614.64,
            "duration": 7.02
        },
        {
            "text": "عربيه ما قبل الاسلام فعلى سبيل المثال",
            "start": 618.24,
            "duration": 6.719
        },
        {
            "text": "العراق كان فيها مملكه المناذره وايضا",
            "start": 621.66,
            "duration": 6.299
        },
        {
            "text": "مملكه الحظر وايضا مثال اخر ففي الاردن",
            "start": 624.959,
            "duration": 6.44
        },
        {
            "text": "حاليا او في بلاد الشام بشكل عام كان فيها",
            "start": 627.959,
            "duration": 7.261
        },
        {
            "text": "مملكه الانباط وهي مملكه عربيه بنات",
            "start": 631.399,
            "duration": 6.761
        },
        {
            "text": "البتراء وكذلك في سوريا الان وهي بلاد",
            "start": 635.22,
            "duration": 6.239
        },
        {
            "text": "الشام سابقا كانت في مملكه الغساسنه",
            "start": 638.16,
            "duration": 6.239
        },
        {
            "text": "ومملكه تدمر التي كانت ملكتها زنوبيه",
            "start": 641.459,
            "duration": 6.721
        },
        {
            "text": "والتي امتدت الى الاناضول وكذلك مصر وكذلك",
            "start": 644.399,
            "duration": 7.141
        },
        {
            "text": "مملكه قدار الموجوده في شمال المملكه",
            "start": 648.18,
            "duration": 6.44
        },
        {
            "text": "العربيه السعوديه هذا التوسع العربي",
            "start": 651.54,
            "duration": 6.72
        },
        {
            "text": "واماكن وجود العرب ما قبل الاسلام وكذلك",
            "start": 654.62,
            "duration": 7.3
        },
        {
            "text": "هذه الممالك تشهد على وجود العرب خارج",
            "start": 658.26,
            "duration": 6.06
        },
        {
            "text": "الجزيره العربيه بالاضافه الى الممالك",
            "start": 661.92,
            "duration": 4.979
        },
        {
            "text": "التي ذكرناها الكثير منها الموجود في",
            "start": 664.32,
            "duration": 5.04
        },
        {
            "text": "الجزيره العربيه وايضا العرب كان لهم",
            "start": 666.899,
            "duration": 5.461
        },
        {
            "text": "مملكه في بلاد فارس في منطقه الاحواز فهي",
            "start": 669.36,
            "duration": 6.659
        },
        {
            "text": "بالاساس تعتبر منطقه عربيه لذلك لا يمكن",
            "start": 672.36,
            "duration": 6.719
        },
        {
            "text": "القول بان العرب تاريخهم فقط في الجزيره",
            "start": 676.019,
            "duration": 6.781
        },
        {
            "text": "العربيه بل تاريخ العرب هو اوسع من ذلك",
            "start": 679.079,
            "duration": 7.681
        },
        {
            "text": "صحيح ان عصر العرب ما قبل الاسلام هو عصر",
            "start": 682.8,
            "duration": 7.8
        },
        {
            "text": "الجاهليه وكان مجتمع الجاهليه مجتمع حيوي",
            "start": 686.76,
            "duration": 7.079
        },
        {
            "text": "فيه الاخلاق واتى النبي محمد صلى الله",
            "start": 690.6,
            "duration": 4.46
        },
        {
            "text": "عليه وسلم",
            "start": 693.839,
            "duration": 8.18
        },
        {
            "text": "ليتمم مكارم الاخلاق فغير تاريخهم للابد",
            "start": 695.06,
            "duration": 6.959
        }
    ]
}