        },
        {
            "text": "عشان يطلبون منه اقناع عمر المختار بايقاف",
            "start": 369.12,
            "duration": 6.6
        },
        {
            "text": "المقاومه ضد جيوشهم فوافق الشيخ على السلم",
            "start": 372.419,
            "duration": 6.481
        },
        {
            "text": "المشروط وتم توقيع عقد الزويتينا هذه",
            "start": 375.72,
            "duration": 6.06
        },
        {
            "text": "المعاهده اثرت على جميع المعاهدات الليبيه",
            "start": 378.9,
            "duration": 6.38
        },
        {
            "text": "اللي راح تحصل بعدها ففي يناير عام 1923",
            "start": 381.78,
            "duration": 7.32
        },
        {
            "text": "هاجر الشيخ محمد ادريس بعد سقوط العاصمه",
            "start": 385.28,
            "duration": 7.72
        },
        {
            "text": "طرابلس على ايدي الطليان فرجع عمر المختار",
            "start": 389.1,
            "duration": 8.039
        },
        {
            "text": "يقود المجاهدين في منطقه برقه اهالي الجبل",
            "start": 393.0,
            "duration": 7.919
        },
        {
            "text": "الاخضر للقتال وعد جيش شجاع ضد الطليان",
            "start": 397.139,
            "duration": 6.961
        },
        {
            "text": "ففتح باب التطوع للانضمام للكفاح ضدهم",
            "start": 400.919,
            "duration": 6.84
        },
        {
            "text": "وشكل لجنه من اعيان قبائل الجبل واتبع",
            "start": 404.1,
            "duration": 6.96
        },
        {
            "text": "اسلوب الغارات وحرب العصابات فكان يصطحب",
            "start": 407.759,
            "duration": 6.78
        },
        {
            "text": "معاه الى 300 من المحاربين في كل غاره",
            "start": 411.06,
            "duration": 7.02
        },
        {
            "text": "فكان طريقته انه يهجم بشكل مفاجئ وينسحب",
            "start": 414.539,
            "duration": 5.88
        },
        {
            "text": "بعدها بسرعه وعدد افراد المقاومه اللي",
            "start": 418.08,
            "duration": 4.86
        },
        {
            "text": "انضموا اليه ما كانوا يزيدون عن الف رجل",
            "start": 420.419,
            "duration": 6.181
        },
        {
            "text": "وكلهم كانوا مسلحين ببنادق خفيفه وبالرغم",
            "start": 422.94,
            "duration": 6.599
        },
        {
            "text": "من كل ذلك الا ان غارات عمر المختار مع",
            "start": 426.6,
            "duration": 6.78
        },
        {
            "text": "المجاهدين كانت اشبه بالكابوس المرعب على",
            "start": 429.539,
            "duration": 6.66
        },
        {
            "text": "الجيش الايطالي هذا لان عمر المختار كان",
            "start": 433.38,
            "duration": 6.06
        },
        {
            "text": "عليم بخبايا الصحراء فكان متعود على القيظ",
            "start": 436.199,
            "duration": 6.481
        },
        {
            "text": "والجفاف وندره المياه فقطع طرق المواصلات",
            "start": 439.44,
            "duration": 6.36
        },
        {
            "text": "والامدادات على الجيش الايطالي وبدا يهاجم",
            "start": 442.68,
            "duration": 6.84
        },
        {
            "text": "سكناتهم ويبيد مسؤولين عسكريين برتب عاليه",
            "start": 445.8,
            "duration": 6.54
        },
        {
            "text": "ورضوم تحصين الجيش الايطالي باسلحه قويه",
            "start": 449.52,
            "duration": 6.36
        },
        {
            "text": "وجيش نظامي الا ان المقاومه الرهيبه هذه",
            "start": 452.34,
            "duration": 6.18
        },
        {
            "text": "تسببت في احراج الجيش الايطالي امام الراي",
            "start": 455.88,
            "duration": 5.819
        },
        {
            "text": "العام في بلادهم واتهمون بالفشل في التغلب",
            "start": 458.52,
            "duration": 6.239
        },
        {
            "text": "على مقاومه شعبيه بسيطه المكونه من مجموعه",
            "start": 461.699,
            "duration": 6.361
        },
        {
            "text": "مجاهدين لا يزيد عددهم عن الف رجل فتحول",
            "start": 464.759,
            "duration": 5.641
        },
        {
            "text": "عمر المختار من شيخ مهتم في الشؤون",
            "start": 468.06,
            "duration": 5.28
        },
        {
            "text": "الدعويه الى قائد في المقاومه الليبيه",
            "start": 470.4,
            "duration": 6.48
        },
        {
            "text": "واصبح له منصب اشبه بالتكليف فبدا يجمع",
            "start": 473.34,
            "duration": 6.12
        },
        {
            "text": "اكبر عدد من المجاهدين ويحرص على جمع",
            "start": 476.88,
            "duration": 5.759
        },
        {
            "text": "المال الكافي لاستمرار المقاومه وفي نهايه",
            "start": 479.46,
            "duration": 7.56
        },
        {
            "text": "عام 1923 سافر عمر المختار لمصر الى شيخه",
            "start": 482.639,
            "duration": 7.441
        },
        {
            "text": "محمد ادريس وعرض عليه نتائج المقاومه",
            "start": 487.02,
            "duration": 5.82
        },
        {
            "text": "فتواصلت الحكومه الايطاليه مع عمر المختار",
            "start": 490.08,
            "duration": 6.179
        },
        {
            "text": "بشتى الطرق عشان يعرضون عليه امرين الامر",
            "start": 492.84,
            "duration": 6.02
        },
        {
            "text": "الاول انه يرجع الى ليبيا مره ثانيه",
            "start": 496.259,
            "duration": 6.241
        },
        {
            "text": "ويتنازل عن زعامه المقاومه ويجبر رجاله",
            "start": 498.86,
            "duration": 6.7
        },
        {
            "text": "على ترك الجهاد وفي المقابل يجعلون منه",
            "start": 502.5,
            "duration": 5.759
        },
        {
            "text": "رجل ليبيا الاول واهم شخصيه في تاريخه",
            "start": 505.56,
            "duration": 5.16
        },
        {
            "text": "وايضا يوفرون له قصر في المنطقه اللي",
            "start": 508.259,
            "duration": 5.28
        },
        {
            "text": "اختارها ويكلفون الجنود الطليان لحراسته",
            "start": 510.72,
            "duration": 5.999
        },
        {
            "text": "الخيار الثاني انه يضل في مصر ويقطع",
            "start": 513.539,
            "duration": 6.18
        },
        {
            "text": "علاقته مع الشيخ محمد ادريس وايضا يطلب",
            "start": 516.719,
            "duration": 6.0
        },
        {
            "text": "اللجوء الرسمي وفي المقابل له كل امتيازات",
            "start": 519.719,
            "duration": 6.361
        },
        {
            "text": "اللي يطلبها بالاضافه الى راتب شهري ضخم",
            "start": 522.719,
            "duration": 6.721
        },
        {
            "text": "بالمبلغ اللي يطلبه وكان عمر المختار كل",
            "start": 526.08,
            "duration": 7.62
        },
        {
            "text": "مره يرد عليهم ويقول انا خلقنا للجهاد",
            "start": 529.44,
            "duration": 8.1
        },
        {
            "text": "فرجع بعدها الى ليبيا عشان يستمر في",
            "start": 533.7,
            "duration": 7.139
        },
        {
            "text": "المقاومه لكن اثناء عودته حصلت الحادثه",
            "start": 537.54,
            "duration": 6.9
        },
        {
            "text": "المشهوره باسم بئر الغبي كان عمر المختار",
            "start": 540.839,
            "duration": 6.141
        },
        {
            "text": "بصحبه خمسين رجل عجوز من المجاهدين",
            "start": 544.44,
            "duration": 5.72
        },
        {
            "text": "فيفاجئهم الجيش الايطالي بسبع مصفحات",
            "start": 546.98,
            "duration": 6.58
        },
        {
            "text": "محمله بجنود لا حصر لهم واول ما حصل",
            "start": 550.16,
            "duration": 6.76
        },
        {
            "text": "الاشتباك انهال عمر ورجاله بالرصاص عليهم",
            "start": 553.56,
            "duration": 7.32
        },
        {
            "text": "لغايه ما حرقوا المصفحات بالكامل فقضوا",
            "start": 556.92,
            "duration": 6.539
        },
        {
            "text": "على عدد كبير من الجنود الطليان واللي",
            "start": 560.88,
            "duration": 5.88
        },
        {
            "text": "تبقى منهم كان يهرب بوسط الصحراء بعد هذه",
            "start": 563.459,
            "duration": 6.601
        },
        {
            "text": "المعركه بدات المستعمرات الايطاليه تعاني",
            "start": 566.76,
            "duration": 7.019
        },
        {
            "text": "بسبب عدم سيطرتها بشكل كافي على الاراضي",
            "start": 570.06,
            "duration": 7.02
        },
        {
            "text": "الليبيه بعد ما اثبت عمر وجماعته من",
            "start": 573.779,
            "duration": 6.981
        },
        {
            "text": "المجاهدين كفاءتهم بمواجهه جيش الاحتلال",
            "start": 577.08,
            "duration": 7.62
        },
        {
            "text": "انضمت لهم بقيه القبائل في المقاومه في",
            "start": 580.76,
            "duration": 7.12
        },
        {
            "text": "هذه اللحظه غيرت ايطاليا سياستها في",
            "start": 584.7,
            "duration": 6.54
        },
        {
            "text": "محاربه المجاهدين تماما فاعلنت الحرب على",
            "start": 587.88,
            "duration": 6.78
        },
        {
            "text": "معسكرات المقاومه خاصه معسكر البراغيث",
            "start": 591.24,
            "duration": 6.36
        },
        {
            "text": "اللي كان عمر المختار يدير المقاومه من",
            "start": 594.66,
            "duration": 6.119
        },
        {
            "text": "خلاله فالغيت جميع الاتفاقيات اللي كانت",
            "start": 597.6,
            "duration": 6.66
        },
        {
            "text": "ما بين ايطاليا والقبائل السنوسيه فاشتعلت",
            "start": 600.779,
            "duration": 6.661
        },
        {
            "text": "المعارك والغارات بين الطرفين واصبح عمر",
            "start": 604.26,
            "duration": 6.06
        },
        {
            "text": "المختار بمثابه نذير شؤم على القوات",
            "start": 607.44,
            "duration": 5.94
        },
        {
            "text": "الايطاليه وبالمقابل موطن الامان للشعب",
            "start": 610.32,
            "duration": 6.0
        },
        {
            "text": "الليبي باكمله وفي منتصف العشرينات تغير",
            "start": 613.38,
            "duration": 5.399
        },
        {
            "text": "عدد كبير من قاده قوات الاحتلال الايطالي",
            "start": 616.32,
            "duration": 5.6
        },
        {
            "text": "وتغيرت معاهم سياسه الحكومه للمره الثانيه",
            "start": 618.779,
            "duration": 6.24
        },
        {
            "text": "فالقاده العسكريين الجدد بداوا يطالبون",
            "start": 621.92,
            "duration": 6.58
        },
        {
            "text": "بالسلام وعقد مفاوضات مع القاده في الحركه",
            "start": 625.019,
            "duration": 7.5
        },
        {
            "text": "السنوسيه وبقيه القبائل مقابل اغرائهم",
            "start": 628.5,
            "duration": 7.8
        },
        {
            "text": "بالاموال وللاسف عدد كبير منهم على",
            "start": 632.519,
            "duration": 7.26
        },
        {
            "text": "المطالب الجديده بوقف المقاومه رغم",
            "start": 636.3,
            "duration": 6.36
        },
        {
            "text": "محاولات عمر المختار المستميته في اقناعهم",
            "start": 639.779,
            "duration": 5.461
        },
        {
            "text": "بعدم ابرام اتفاقات مع الجانب الايطالي",
            "start": 642.66,
            "duration": 6.84
        },
        {
            "text": "ففي عام 1927 اصدر الرضا السنوسي اوامر",
            "start": 645.24,
            "duration": 7.2
        },
        {
            "text": "للقائد عمر المختار بوصف اطلاق النار",
            "start": 649.5,
            "duration": 6.42
        },
        {
            "text": "فوافق عمر وتوقف عن المقاومه بشكل مؤقت",
            "start": 652.44,
            "duration": 6.6
        },
        {
            "text": "لان ما قدر يشوف اراضي بلاده يتم اغتصابها",
            "start": 655.92,
            "duration": 6.359
        },
        {
            "text": "امامه وهو ساكت فشنغارات على معسكرات",
            "start": 659.04,
            "duration": 6.299
        },
        {
            "text": "الاحتلال عشان يجبرهم على محاربته وبعد",
            "start": 662.279,
            "duration": 6.18
        },
        {
            "text": "معارك شرسه وطاحنه بين الطرفين الامر",
            "start": 665.339,
            "duration": 6.841
        },
        {
            "text": "الصادم ان القوات الايطاليه هربت وتركت",
            "start": 668.459,
            "duration": 7.021
        },
        {
            "text": "سياراتها ومدافعها الجبليه وصناديق",
            "start": 672.18,
            "duration": 6.48
        },
        {
            "text": "الذخيره وخاف بعدها عمر المختار عدد كبير",
            "start": 675.48,
            "duration": 6.539
        },
        {
            "text": "من المعارك الملحميه اللي دائما يكون",
            "start": 678.66,
            "duration": 7.26
        },
        {
            "text": "النصر حليفه ومن اهمها معركه الشافتير هذه",
            "start": 682.019,
            "duration": 7.861
        },
        {
            "text": "المعركه اللي ادلت الجيش الايطالي واجبرته",
            "start": 685.92,
            "duration": 7.74
        },
        {
            "text": "على مغادره منطقه الجبل الاخضر وفي وقتها",
            "start": 689.88,
            "duration": 6.26
        },
        {
            "text": "ظهر قاده الجيش الايطالي امام",
            "start": 693.66,
            "duration": 6.66
        },
        {
            "text": "بمظهر مذل بسبب الخسائر المتكرره رغم",
            "start": 696.14,
            "duration": 7.319
        },
        {
            "text": "امتلاكهم للطائرات والمدافع والدبابات",
            "start": 700.32,
            "duration": 6.66
        },
        {
            "text": "والجيش النظامي اللي يقدر بعدد عشرات",
            "start": 703.459,
            "duration": 5.801
        },
        {
            "text": "الالاف والامر اللي ساهم في نشر هذه",
            "start": 706.98,
            "duration": 5.4
        },
        {
            "text": "الفضيحه انه بعد انتهاء المعركه قرر عمر",
            "start": 709.26,
            "duration": 6.42
        },
        {
            "text": "المختار انه يشهر بالقضيه الليبيه امام",
            "start": 712.38,
            "duration": 6.12
        },
        {
            "text": "المجتمع الدولي فاصدر اوامر الى عدد من",
            "start": 715.68,
            "duration": 4.94
        },
        {
            "text": "رجاله بالسفر الى الدول الاسلاميه",
            "start": 718.5,
            "duration": 5.76
        },
        {
            "text": "والاوروبيه وهناك يعرفون العالم عن حقيقه",
            "start": 720.62,
            "duration": 6.7
        },
        {
            "text": "الاوضاع اللي تحصل في ليبيا في وقتها خرج",
            "start": 724.26,
            "duration": 6.3
        },
        {
            "text": "الحاكم الفاشلي لايطاليا بنيتو موسيليني",
            "start": 727.32,
            "duration": 6.959
        },
        {
            "text": "صرح بمقوله معروفه وقال لشعبه اننا لا",
            "start": 730.56,
            "duration": 6.719
        },
        {
            "text": "نحارب ذئبا كما يقول الجهراء الكرستياني",
            "start": 734.279,
            "duration": 6.661
        },
        {
            "text": "بل نحارب اسودا يدافعون بشجاعه عن بلاده",
            "start": 737.279,
            "duration": 6.721
        },
        {
            "text": "وبعد نجاحات القائد عمر المختار المتكرره",
            "start": 740.94,
            "duration": 5.82
        },
        {
            "text": "في ميدان المعركه اطلقوا عليه لقب الزعيم",
            "start": 744.0,
            "duration": 6.3
        },
        {
            "text": "الذي لا يقهر ابدا لغايه اكتوبر من عام",
            "start": 746.76,
            "duration": 7.98
        },
        {
            "text": "1930 وقتها القوات الايطاليه اشتبكت مع",
            "start": 750.3,
            "duration": 7.2
        },
        {
            "text": "المجاهدين في معركه كبيره وفي نهايه",
            "start": 754.74,
            "duration": 5.64
        },
        {
            "text": "المعركه عثروا على نظاره عمر المختار",
            "start": 757.5,
            "duration": 6.72
        },
        {
            "text": "وكذلك حصانه فخرج الجنرال كريستياني وقال",
            "start": 760.38,
            "duration": 7.019
        },
        {
            "text": "تصريح المشهور اللي اقلق الشعب الليبي لقد",
            "start": 764.22,
            "duration": 7.26
        },
        {
            "text": "اخذنا اليوم نظاره المختار وغدا سناتي",
            "start": 767.399,
            "duration": 7.801
        },
        {
            "text": "براسه بعدها بسنه توجع عمر المختار بصحبه",
            "start": 771.48,
            "duration": 7.74
        },
        {
            "text": "عدد صغير من رجاله الى ضريح الصحابي رويفع",
            "start": 775.2,
            "duration": 7.079
        },
        {
            "text": "بن ثابت الموجود بمدينه البيضاء في شرق",
            "start": 779.22,
            "duration": 5.88
        },
        {
            "text": "ليبيا فشافتهم وحدات الاستطلاع الجوي",
            "start": 782.279,
            "duration": 5.821
        },
        {
            "text": "الايطاليه فتحركه فرق وفصائل الجيش",
            "start": 785.1,
            "duration": 6.78
        },
        {
            "text": "الايطالي لمواجهتهم وبعد قتال عنيف جرحوا",
            "start": 788.1,
            "duration": 6.84
        },
        {
            "text": "حصان عمر المختار فواقع الزعيم الليبي على",
            "start": 791.88,
            "duration": 6.6
        },
        {
            "text": "الارض وتم القبض عليه قاده الجيش الايطالي",
            "start": 794.94,
            "duration": 7.38
        },
        {
            "text": "يقولون لما اعتقلنا عمر المختار كان هادئ",
            "start": 798.48,
            "duration": 7.14
        },
        {
            "text": "البال وكان لم يقع في الاسر فجميع الجنود",
            "start": 802.32,
            "duration": 6.36
        },
        {
            "text": "الايطاليين اصابهم الذهول بسبب حاله الرضا",
            "start": 805.62,
            "duration": 6.24
        },
        {
            "text": "ظل العمر المختار في زنزانه رديئه وطول",
            "start": 808.68,
            "duration": 6.839
        },
        {
            "text": "الوقت كان يصلي ويقرا القران في هدوء لما",
            "start": 811.86,
            "duration": 7.44
        },
        {
            "text": "يحققون معه كانوا يبون انه يدلونه وكذلك",
            "start": 815.519,
            "duration": 6.961
        },
        {
            "text": "يجعلونه ينفي كل افعاله لكن عمر المختار",
            "start": 819.3,
            "duration": 6.42
        },
        {
            "text": "اعترف بجميع معاركه وقال انا المسؤول",
            "start": 822.48,
            "duration": 6.299
        },
        {
            "text": "الوحيد عن خسائر الجيش الايطالي تصريحاته",
            "start": 825.72,
            "duration": 6.78
        },
        {
            "text": "هذه استفزت الجنرال جراتسياني فاصر على",
            "start": 828.779,
            "duration": 7.021
        },
        {
            "text": "اعدامه بين اهله بطريقه قاسيه بعدها",
            "start": 832.5,
            "duration": 6.3
        },
        {
            "text": "باربعه ايام حصلت المحكمه العبثيه",
            "start": 835.8,
            "duration": 6.3
        },
        {
            "text": "والهزليه لانهم نصبوا المشنقه قبل حضور",
            "start": 838.8,
            "duration": 6.719
        },
        {
            "text": "القضاه وبدء التحقيق فعرف الجميع ان اعدام",
            "start": 842.1,
            "duration": 6.6
        },
        {
            "text": "عمر المختار هي مساله وقت الامر العجيب ان",
            "start": 845.519,
            "duration": 5.581
        },
        {
            "text": "المحامي اللي دافع عن عمر المختار هو",
            "start": 848.7,
            "duration": 5.28
        },
        {
            "text": "محامي ايطالي في البدايه طلب من المحكمه",
            "start": 851.1,
            "duration": 6.479
        },
        {
            "text": "انها تخفف الحكم مراعاه للشيخوخه لكن تم",
            "start": 853.98,
            "duration": 6.299
        },
        {
            "text": "رفض الطلبه بشكل صريح فرد هذا المحامي",
            "start": 857.579,
            "duration": 6.06
        },
        {
            "text": "الايطالي اللي اسمه روبرتو لونتانو بتصريح",
            "start": 860.279,
            "duration": 7.141
        },
        {
            "text": "تاريخي فيقول ان هذا المتهم الذي انتدبت",
            "start": 863.639,
            "duration": 8.281
        },
        {
            "text": "للدفاع عنه انما يدافع عن حقيقه كلنا",
            "start": 867.42,
            "duration": 9.12
        },
        {
            "text": "نعرفها جيدا وهي الوطن الذي طالم نحن في",
            "start": 871.92,
            "duration": 8.039
        },
        {
            "text": "سبيل تحريره فان هذا الرجل هو ابن هذه",
            "start": 876.54,
            "duration": 7.62
        },
        {
            "text": "الارض قبل ان تطاها اقدامكم واني امل ان",
            "start": 879.959,
            "duration": 7.68
        },
        {
            "text": "تحذروا حكم التاريخ فانه لا يرحم فان",
            "start": 884.16,
            "duration": 7.26
        },
        {
            "text": "عجلته تدور وتسجل كل ما يحدث في هذا",
            "start": 887.639,
            "duration": 6.901
        },
        {
            "text": "العالم المضطرب تمت مقاطعه المحامي اثناء",
            "start": 891.42,
            "duration": 6.419
        },
        {
            "text": "محاوله الدفاع وبعدها بساعه نطق القاضي",
            "start": 894.54,
            "duration": 6.72
        },
        {
            "text": "الحكم على عمر المختار بالاعدام شنقا حتى",
            "start": 897.839,
            "duration": 6.421
        },
        {
            "text": "الموت لما المترجم حاول انه يشرح الحكم",
            "start": 901.26,
            "duration": 6.42
        },
        {
            "text": "لعمر المختار فقال عمر ان الحكم الا لله",
            "start": 904.26,
            "duration": 7.439
        },
        {
            "text": "لا لحكمكم المزيف انا لله وانا اليه",
            "start": 907.68,
            "duration": 8.659
        },
        {
            "text": "راجعون وفي صباح 16 سبتمبر عام 1931",
            "start": 911.699,
            "duration": 8.101
        },
        {
            "text": "تجمعوا الاهالي الليبيين ليودعون عمر",
            "start": 916.339,
            "duration": 7.36
        },
        {
            "text": "المختار وكان عددهم عشرين الف من رجال",
            "start": 919.8,
            "duration": 6.839
        },
        {
            "text": "ونساء فاستعدت القوات الايطاليه لاي",
            "start": 923.699,
            "duration": 6.121
        },
        {
            "text": "محاوله للتهريب وكذلك الطائرات كانت على",
            "start": 926.639,
            "duration": 5.76
        },
        {
            "text": "ارتفاع منخفض وعشان يقضون على اي فرصه",
            "start": 929.82,
            "duration": 5.639
        },
        {
            "text": "لعمر المختار انه يوصي وصيه او رساله",
            "start": 932.399,
            "duration": 6.421
        },
        {
            "text": "لاستكمال الجهاد وصدرت اوامر بضرب او",
            "start": 935.459,
            "duration": 6.721
        },
        {
            "text": "تعذيب اي شخص يحزن او يبكي على الاعدام",
            "start": 938.82,
            "duration": 7.56
        },
        {
            "text": "فصعد عمر لملاقاه مصيره بصدر رحب ونطق",
            "start": 942.18,
            "duration": 8.159
        },
        {
            "text": "الشهاده في سلام ان الظلم يجعل من المظلوم",
            "start": 946.38,
            "duration": 8.34
        },
        {
            "text": "بطلا اما الجريمه فلا بد من ان يرتجف قلبه",
            "start": 950.339,
            "duration": 8.161
        },
        {
            "text": "صاحبها مهما تظاهر بالكبرياء الله سبحانه",
            "start": 954.72,
            "duration": 9.44
        },
        {
            "text": "وتعالى يجعل من الظالمين عبره للناس",
            "start": 958.5,
            "duration": 5.66
        },
        {
            "text": "[موسيقى]",
            "start": 965.42,
            "duration": 3.18
        },
        {
            "text": "[موسيقى]",
            "start": 972.4,
            "duration": 9.829
        }
    ]
}