        {
            "text": "الايطاليين للابد فكان هناك صديقين يعيشون",
            "start": 377.58,
            "duration": 6.48
        },
        {
            "text": "طفولتهم في مدينه باليرمو الايطاليه الاول",
            "start": 381.12,
            "duration": 6.359
        },
        {
            "text": "جيوفاني فالكوني انولد في عام 1939",
            "start": 384.06,
            "duration": 7.94
        },
        {
            "text": "والثاني بورسليني انولد في عام 1940",
            "start": 387.479,
            "duration": 7.201
        },
        {
            "text": "هالطفلين كانوا يعيشون في بيئه خطره",
            "start": 392.0,
            "duration": 6.58
        },
        {
            "text": "يلاحظون فيها انخراط شباب جيلهم باعمال",
            "start": 394.68,
            "duration": 7.019
        },
        {
            "text": "المافيا الايطاليه فيشوفون الجثث ملقاه",
            "start": 398.58,
            "duration": 6.6
        },
        {
            "text": "امامهم في الشوارع والمخدرات تنتشر ما بين",
            "start": 401.699,
            "duration": 7.081
        },
        {
            "text": "اقرانهم وعلب الكحول موجوده امام كل بيت",
            "start": 405.18,
            "duration": 6.959
        },
        {
            "text": "في احد الايام كانت يوفاني وباولو يلعبون",
            "start": 408.78,
            "duration": 6.84
        },
        {
            "text": "تنس مع مجموعه من الشباب وكانوا يعرفون ان",
            "start": 412.139,
            "duration": 6.12
        },
        {
            "text": "هذول الشباب اللي امامهم ادخلوا في عالم",
            "start": 415.62,
            "duration": 5.88
        },
        {
            "text": "الاجرام فكانت عندهم الشراره لتبني مبدا",
            "start": 418.259,
            "duration": 6.181
        },
        {
            "text": "محاربه العصابات وللعلم كانت العصابات",
            "start": 421.5,
            "duration": 6.44
        },
        {
            "text": "تتدخل بكل صغيره وكبيره وعندهم صلاحيات",
            "start": 424.44,
            "duration": 7.319
        },
        {
            "text": "اكبر من الدوله والقانون فتخيلوا ان",
            "start": 427.94,
            "duration": 6.879
        },
        {
            "text": "العصابات كانت تتدخل حتى بالمشاكل الشخصيه",
            "start": 431.759,
            "duration": 6.421
        },
        {
            "text": "مع الوقت صارت العصابات في ايطاليا امر",
            "start": 434.819,
            "duration": 7.201
        },
        {
            "text": "واقع ورمز من رموز ايطاليا حاله كحال",
            "start": 438.18,
            "duration": 6.72
        },
        {
            "text": "البيتزا والكولوسيوم على العموم الطفلين",
            "start": 442.02,
            "duration": 6.78
        },
        {
            "text": "اللي تكلمنا عنهم باولو وجوفاني كبروا",
            "start": 444.9,
            "duration": 6.9
        },
        {
            "text": "اكبر معاهم الهاجس فادخلوا الجامعه لدراسه",
            "start": 448.8,
            "duration": 6.119
        },
        {
            "text": "القانون وتدربوا على السلك القانوني بشكل",
            "start": 451.8,
            "duration": 6.899
        },
        {
            "text": "مكثف رغم ان اهاليهم انصحهم انه يدرسون",
            "start": 454.919,
            "duration": 7.861
        },
        {
            "text": "فقط الطب او الهندسه رغم ان جيوفاني كانوا",
            "start": 458.699,
            "duration": 6.601
        },
        {
            "text": "مختلفين من الناحيه الفكريه لكن جمعهم",
            "start": 462.78,
            "duration": 6.0
        },
        {
            "text": "مبدا محاربه العصابات فوصلوا اثنين هم",
            "start": 465.3,
            "duration": 6.6
        },
        {
            "text": "لمنصب القضاء وافتحوا ملف المافيا",
            "start": 468.78,
            "duration": 6.3
        },
        {
            "text": "الايطاليه بكل جراه فسعوا بالبحث عن",
            "start": 471.9,
            "duration": 6.6
        },
        {
            "text": "الملفات اللي تدين العصابات ونجح القاضي",
            "start": 475.08,
            "duration": 7.019
        },
        {
            "text": "جيفاني فالكوني باثبات التهم على عدد كبير",
            "start": 478.5,
            "duration": 7.38
        },
        {
            "text": "من اعضاء المافيا وسجن العديد منهم وكان",
            "start": 482.099,
            "duration": 6.781
        },
        {
            "text": "يتميز القاضي الجوفاني بعدم الخوف من",
            "start": 485.88,
            "duration": 6.84
        },
        {
            "text": "المافيا وكان يحكم بجراه وعلى حسب الدليل",
            "start": 488.88,
            "duration": 7.74
        },
        {
            "text": "رغم معرفته بالعواقب الشديده والموقف",
            "start": 492.72,
            "duration": 7.319
        },
        {
            "text": "الابرز اللي هز ايطاليا في تلك الفتره هو",
            "start": 496.62,
            "duration": 7.4
        },
        {
            "text": "استدعاء لاول مره رئيس المافيا الايطاليه",
            "start": 500.039,
            "duration": 8.701
        },
        {
            "text": "توماسو بسكيتا الرئيس اعترف للقضاء ولا",
            "start": 504.02,
            "duration": 8.68
        },
        {
            "text": "مره عن عمل المافيا من الداخل وفي",
            "start": 508.74,
            "duration": 7.26
        },
        {
            "text": "التحقيقات قال انه ندم بدخول المافيا وهو",
            "start": 512.7,
            "duration": 7.44
        },
        {
            "text": "صغير وكان مخدوع بمفاهيم الاخوه والولاء",
            "start": 516.0,
            "duration": 7.979
        },
        {
            "text": "وفي المحاكمه قال بسكيته معلومه خطيره ان",
            "start": 520.14,
            "duration": 7.74
        },
        {
            "text": "المافيا تعتبر كيان واحد كبير وليست",
            "start": 523.979,
            "duration": 7.441
        },
        {
            "text": "كيانات متفرقه وبعدها قدم معلومات للمحكمه",
            "start": 527.88,
            "duration": 6.78
        },
        {
            "text": "راح تضيق على العصابه في المستقبل استمر",
            "start": 531.42,
            "duration": 5.94
        },
        {
            "text": "القاضي فالكوني بالبحث عن اسرار المافيا",
            "start": 534.66,
            "duration": 7.34
        },
        {
            "text": "مع صديق دربه باولو وفي ديسمبر عام 1987",
            "start": 537.36,
            "duration": 9.659
        },
        {
            "text": "اصدر القاضي جيراني بالحكم بالسجن على 360",
            "start": 542.0,
            "duration": 9.1
        },
        {
            "text": "شخص جميعهم متهمين بالارتباط بالمافيا",
            "start": 547.019,
            "duration": 7.621
        },
        {
            "text": "الايطاليه هذا الحكم كان يعتبر ضربه كبيره",
            "start": 551.1,
            "duration": 7.14
        },
        {
            "text": "بحق المافيا الايطاليه فقررت المافيا",
            "start": 554.64,
            "duration": 7.56
        },
        {
            "text": "بالانتقام من القاضي ففي احد الايام وضعت",
            "start": 558.24,
            "duration": 7.68
        },
        {
            "text": "العصابه متفجرات تي ان تي بانابيب الصرف",
            "start": 562.2,
            "duration": 8.04
        },
        {
            "text": "الصحي تحت الخط السريع وانت مرور القاضي",
            "start": 565.92,
            "duration": 8.22
        },
        {
            "text": "واهله ولما مر القاضي ومعه موكب من ثلاث",
            "start": 570.24,
            "duration": 8.64
        },
        {
            "text": "سيارات فعلو التفجيرات عن بعد فتوفى ومعاه",
            "start": 574.14,
            "duration": 8.879
        },
        {
            "text": "خمس اشخاص ومنهم زوجته وانجرح اكثر من 25",
            "start": 578.88,
            "duration": 8.1
        },
        {
            "text": "شخص كانت جنازه فالكون ضخمه وصار بطل من",
            "start": 583.019,
            "duration": 7.26
        },
        {
            "text": "ابطال التاريخ الايطالي واعلن الحداد من",
            "start": 586.98,
            "duration": 6.0
        },
        {
            "text": "خلال البرلمان الايطالي وبالنسبه للقاضي",
            "start": 590.279,
            "duration": 6.841
        },
        {
            "text": "الاخر رفيق درب فالكوني وصديق طفولته حاول",
            "start": 592.98,
            "duration": 7.5
        },
        {
            "text": "انه يترك بصمه في مواجهه العصابات لكن تم",
            "start": 597.12,
            "duration": 6.6
        },
        {
            "text": "اغتياله بعد شهرين من اغتيال فالكوني صحيح",
            "start": 600.48,
            "duration": 6.12
        },
        {
            "text": "ان الاغتيال تحقق لكن المافيا الايطاليه",
            "start": 603.72,
            "duration": 7.26
        },
        {
            "text": "بعد هذه الحادثه تغييره بشكل كبير فحالات",
            "start": 606.6,
            "duration": 8.7
        },
        {
            "text": "التفجير والقتل قلو بشكل ملحوظ وكان تاثير",
            "start": 610.98,
            "duration": 7.44
        },
        {
            "text": "المحاكمه واضح على سلوك العصابه وهذا",
            "start": 615.3,
            "duration": 6.02
        },
        {
            "text": "الامر خلى الايطاليين ما ينسون المحاكمه",
            "start": 618.42,
            "duration": 6.18
        },
        {
            "text": "فانتهت مسيره شخصين افنى وحياتهم في",
            "start": 621.32,
            "duration": 7.3
        },
        {
            "text": "مواجهه العصابات واصبحوا رموز في التاريخ",
            "start": 624.6,
            "duration": 7.38
        },
        {
            "text": "في يوم الاغتيال انتشرت بين الناس مقوله",
            "start": 628.62,
            "duration": 7.44
        },
        {
            "text": "كتبت في الجرائد وعلى الجدران لم تقتلوهم",
            "start": 631.98,
            "duration": 7.68
        },
        {
            "text": "ما زالت افكارهم حيه في بدايه الحلقه",
            "start": 636.06,
            "duration": 7.08
        },
        {
            "text": "تكلمنا عن هجره الايطاليين الى امريكا ومن",
            "start": 639.66,
            "duration": 6.419
        },
        {
            "text": "اثارها دخول مفهوم المافيا الى السينما",
            "start": 643.14,
            "duration": 6.0
        },
        {
            "text": "العالميه لكن شنو سبب هذه الهجره وكيف",
            "start": 646.079,
            "duration": 7.141
        },
        {
            "text": "كانت سبب بظهور المافيا داخل امريكا لما",
            "start": 649.14,
            "duration": 6.24
        },
        {
            "text": "هاجروا الايطالين الى امريكا في نهايه",
            "start": 653.22,
            "duration": 6.059
        },
        {
            "text": "القرن 19 استقروا في نيويورك واستمروا",
            "start": 655.38,
            "duration": 7.139
        },
        {
            "text": "بنشاطهم الغير قانوني فكانوا يربحون اموال",
            "start": 659.279,
            "duration": 6.0
        },
        {
            "text": "كثيره من خلال القمار وكانوا يستخدمون",
            "start": 662.519,
            "duration": 5.94
        },
        {
            "text": "مواقع مختلفه عن مواقع العاديه مواقع تم",
            "start": 665.279,
            "duration": 6.661
        },
        {
            "text": "تاسيسها في دول لا تجرمها مثل كوستاريكا",
            "start": 668.459,
            "duration": 6.181
        },
        {
            "text": "ومن الشخصيات التي كشفت اسرار عصابه",
            "start": 671.94,
            "duration": 6.42
        },
        {
            "text": "نيويورك رجل اسمه جون بنسيني انضمون في",
            "start": 674.64,
            "duration": 8.879
        },
        {
            "text": "عام 2013 وتركهم عام 2018 يقول جون ان",
            "start": 678.36,
            "duration": 7.44
        },
        {
            "text": "انضمامه للعصابه ما كان باختياره",
            "start": 683.519,
            "duration": 5.901
        },
        {
            "text": "فالعصابات تتعمد انه تضعك مشكله معينه",
            "start": 685.8,
            "duration": 7.68
        },
        {
            "text": "ويقولوا لك ما تخرج منها الا بعد تواصلك",
            "start": 689.42,
            "duration": 8.68
        },
        {
            "text": "معانا فتضطر رغم انك بريء ان تصبح اداه",
            "start": 693.48,
            "duration": 7.799
        },
        {
            "text": "لهم جون يقول انه صحيح العصابه في نيويورك",
            "start": 698.1,
            "duration": 6.54
        },
        {
            "text": "لها علاقه بالعصابه في ايطاليا لكن هناك",
            "start": 701.279,
            "duration": 5.881
        },
        {
            "text": "فروقات فيما بينهم ففي نيويورك ما",
            "start": 704.64,
            "duration": 5.34
        },
        {
            "text": "يستخدمون القنابل ولا يلقون الجثث في",
            "start": 707.16,
            "duration": 5.94
        },
        {
            "text": "الشوارع ويحاولون انه ما يلفتون انظار",
            "start": 709.98,
            "duration": 5.82
        },
        {
            "text": "الناس وتتكون العصابه في نيويورك من خمس",
            "start": 713.1,
            "duration": 6.419
        },
        {
            "text": "عوائل جينيفر قامبينو لوكيزي ومعاهم",
            "start": 715.8,
            "duration": 6.719
        },
        {
            "text": "كولومبو وبابانو ولهذا انشهرت الاصابه",
            "start": 719.519,
            "duration": 6.601
        },
        {
            "text": "باسم عصابه العوائل الخمس فيعتبرونه تاريخ",
            "start": 722.519,
            "duration": 6.541
        },
        {
            "text": "خاص لا يمكن التنازل عنه فهم مستمرين",
            "start": 726.12,
            "duration": 6.42
        },
        {
            "text": "بالانشطه ليكملون مسيره السابقين وبالنسبه",
            "start": 729.06,
            "duration": 6.42
        },
        {
            "text": "لجون ترك هذه العصابه بعد ما اتهموه",
            "start": 732.54,
            "duration": 6.12
        },
        {
            "text": "بالخيانه وانه يسرب اخبارهم للشرطه صحيح",
            "start": 735.48,
            "duration": 6.0
        },
        {
            "text": "ان الخروج من المافيا يعني القتل لكن جون",
            "start": 738.66,
            "duration": 5.58
        },
        {
            "text": "يقول ان المافيا في نيويورك ما تبي تلفت",
            "start": 741.48,
            "duration": 6.06
        },
        {
            "text": "الانتباه بقتله فهو خرج وما زال حي",
            "start": 744.24,
            "duration": 6.719
        },
        {
            "text": "العصابات للعالم كثيره في عصابه كوزا",
            "start": 747.54,
            "duration": 6.12
        },
        {
            "text": "نوسترا في ايطاليا وعصابه العوائل الخمس",
            "start": 750.959,
            "duration": 4.861
        },
        {
            "text": "في نيويورك وكذلك الاصابه المكسيكيه",
            "start": 753.66,
            "duration": 5.22
        },
        {
            "text": "كارتيل المعروفه بتهريب المخدرات وايضا",
            "start": 755.82,
            "duration": 5.82
        },
        {
            "text": "العصابه اليابانيه ياكوزا اللي شرحناها",
            "start": 758.88,
            "duration": 5.759
        },
        {
            "text": "بالتفصيل في حلقه سابقه السؤال شنو علاقه",
            "start": 761.64,
            "duration": 6.6
        },
        {
            "text": "جميع هذه الاصابات ببعضهم فاحيانا تستعين",
            "start": 764.639,
            "duration": 6.421
        },
        {
            "text": "المافيا ببعضها في عمليات غسيل الاموال",
            "start": 768.24,
            "duration": 5.719
        },
        {
            "text": "فالعصابات في روسيا واليابان والصين",
            "start": 771.06,
            "duration": 5.94
        },
        {
            "text": "تعاملوا مع المافيا الايطاليه في عمليه",
            "start": 773.959,
            "duration": 6.401
        },
        {
            "text": "غسل الاموال واحيانا تدخل العصابات بصراع",
            "start": 777.0,
            "duration": 7.019
        },
        {
            "text": "فيما بينها بسبب عملهم في سوق واحد رغم ان",
            "start": 780.36,
            "duration": 6.12
        },
        {
            "text": "هذا العالم من العصابات يزينها البعض في",
            "start": 784.019,
            "duration": 6.961
        },
        {
            "text": "الروايات والافلام الا ان كل شخص دخل ندم",
            "start": 786.48,
            "duration": 7.919
        },
        {
            "text": "بسبب السجن او القتل هذا التاريخ الطويل",
            "start": 790.98,
            "duration": 7.94
        },
        {
            "text": "للعصابات يطرح سؤال مهم هل ظاهره المافيا",
            "start": 794.399,
            "duration": 8.101
        },
        {
            "text": "ستنتهي من العالم فيقول القاضي الايطالي",
            "start": 798.92,
            "duration": 8.44
        },
        {
            "text": "فالكوني قبل وفاته عباره جدا مهمه المافيا",
            "start": 802.5,
            "duration": 10.019
        },
        {
            "text": "ليست ظاهره لا تقهر فهي واقع انساني ومثل",
            "start": 807.36,
            "duration": 9.539
        },
        {
            "text": "كل واقع انسانيه يكون لها بدايه وسيكون",
            "start": 812.519,
            "duration": 5.861
        },
        {
            "text": "لها نهايه",
            "start": 816.899,
            "duration": 15.37
        },
        {
            "text": "[موسيقى]",
            "start": 818.38,
            "duration": 13.889
        },
        {
            "text": "[موسيقى]",
            "start": 842.33,
            "duration": 3.209
        }
    ]
}