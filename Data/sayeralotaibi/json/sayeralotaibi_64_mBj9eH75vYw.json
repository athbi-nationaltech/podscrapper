        },
        {
            "text": "لما بدات الحرب ما العرب والصهاينه في عام",
            "start": 387.56,
            "duration": 7.5
        },
        {
            "text": "1948 دخلت القوات المصريه الى خان يونس",
            "start": 391.02,
            "duration": 7.799
        },
        {
            "text": "بقياده الضابط احمد عبد العزيز فقام جهاز",
            "start": 395.06,
            "duration": 7.66
        },
        {
            "text": "الموساد بتوجيه يهوذا فاضل انه يتواصل مع",
            "start": 398.819,
            "duration": 7.5
        },
        {
            "text": "القوات المصريه وبالفعل بدا يتقرب من قائد",
            "start": 402.72,
            "duration": 6.06
        },
        {
            "text": "الكتيبه المصريه اللي هو القائد احمد عبد",
            "start": 406.319,
            "duration": 6.241
        },
        {
            "text": "العزيز وكذلك بدا يتواصل مع نائبه الضابط",
            "start": 408.78,
            "duration": 6.96
        },
        {
            "text": "كمال الدين حسين فاقنعهم بانهم يسمحون",
            "start": 412.56,
            "duration": 6.66
        },
        {
            "text": "بالدخول الى المعسكر المصري بصفته شيخ دين",
            "start": 415.74,
            "duration": 6.6
        },
        {
            "text": "يام الجنود المصريين وكذلك يلقي خطب دينيه",
            "start": 419.22,
            "duration": 6.36
        },
        {
            "text": "لرفع معنوياتهم وبالفعل يهوذا او الشيخ",
            "start": 422.34,
            "duration": 6.54
        },
        {
            "text": "عبد الله اصبح امام للجنود المصريه وكان",
            "start": 425.58,
            "duration": 6.179
        },
        {
            "text": "يدعو لهم بالنصر ويكرر عبارات التكبير",
            "start": 428.88,
            "duration": 5.939
        },
        {
            "text": "فيما بينهم فوثقوا المصريين فيه لانه كان",
            "start": 431.759,
            "duration": 5.641
        },
        {
            "text": "يتظاهر بالتقوى والصلاه وقراءه القران",
            "start": 434.819,
            "duration": 5.22
        },
        {
            "text": "والشيخ عبد الله فاضل كان يجاوبهم على",
            "start": 437.4,
            "duration": 5.46
        },
        {
            "text": "اسئلتهم الدينيه ويتردد على المعسكر",
            "start": 440.039,
            "duration": 6.421
        },
        {
            "text": "المصري بصفه دائمه فاصبح قريب منهم وصار",
            "start": 442.86,
            "duration": 6.66
        },
        {
            "text": "يسمع احاديثهم اليوميه عن الجيش والعس",
            "start": 446.46,
            "duration": 6.48
        },
        {
            "text": "خططهم الخاصه وبالليل يتسلل لمعسكرات",
            "start": 449.52,
            "duration": 7.079
        },
        {
            "text": "اليهود وينقل لهم اخبار الكتيبه عددهم",
            "start": 452.94,
            "duration": 7.379
        },
        {
            "text": "خططهم اسلحتهم واستمر على هذا الحال لحد",
            "start": 456.599,
            "duration": 6.54
        },
        {
            "text": "يوم من الايام قائد الكتيبه المصريه اللي",
            "start": 460.319,
            "duration": 6.361
        },
        {
            "text": "هو احمد عبد العزيز لاحظ ان كل يوم بعد",
            "start": 463.139,
            "duration": 6.481
        },
        {
            "text": "صلاه العشاء الشيخ عبد الله فاضل يختفي",
            "start": 466.68,
            "duration": 6.419
        },
        {
            "text": "تماما فكلف واحد من الضباط انه يراقب",
            "start": 469.62,
            "duration": 6.359
        },
        {
            "text": "الشيخ كل يوم بالليل وبالفعل هذا اللي حصل",
            "start": 473.099,
            "duration": 6.061
        },
        {
            "text": "كل يوم بعد صلاه العشاء الضابط يلحق الشيخ",
            "start": 475.979,
            "duration": 5.641
        },
        {
            "text": "عبد الله فاضل الى بيته وبعد ما يدخل",
            "start": 479.16,
            "duration": 4.819
        },
        {
            "text": "الشيخ عبد الله فاضل بيته ويقفل الباب",
            "start": 481.62,
            "duration": 5.94
        },
        {
            "text": "ياتي الضابط ويترك الباب عليه فتفاجاوا",
            "start": 483.979,
            "duration": 6.28
        },
        {
            "text": "الضباط ان لم يضربون الباب عليه ما يكون",
            "start": 487.56,
            "duration": 5.94
        },
        {
            "text": "في رد فاستمر الضابط يراقبه كل يوم وعلى",
            "start": 490.259,
            "duration": 6.481
        },
        {
            "text": "هذا الحال الموقف يتكرر فيطرق الباب عليه",
            "start": 493.5,
            "duration": 5.819
        },
        {
            "text": "بعد انتهاء من صلاه العشاء وما يكون في رد",
            "start": 496.74,
            "duration": 5.459
        },
        {
            "text": "فابلغ القائد احمد عبد العزيز فيما حصل",
            "start": 499.319,
            "duration": 5.88
        },
        {
            "text": "فقال لها الضابط ابيك تحاول للمره الاخيره",
            "start": 502.199,
            "duration": 6.9
        },
        {
            "text": "بان تطرق الباب بعد نصف ساعه الضابط نفذ",
            "start": 505.199,
            "duration": 7.741
        },
        {
            "text": "الامر وطرق الباب بعد نصف ساعه لكن لا",
            "start": 509.099,
            "duration": 7.201
        },
        {
            "text": "يوجد رد من الشيخ الصدمه ان الضابط كان",
            "start": 512.94,
            "duration": 6.719
        },
        {
            "text": "يراقب البيت طوال الليل والشيء الغريب ان",
            "start": 516.3,
            "duration": 5.94
        },
        {
            "text": "الشيخ ما يظهر الا وقت صلاه الفجر",
            "start": 519.659,
            "duration": 6.421
        },
        {
            "text": "فالموضوع اصبح غامض جدا فشلون الشيخ يدخل",
            "start": 522.24,
            "duration": 7.44
        },
        {
            "text": "بيته مباشره بعد صلاه العشاء وينام بهذه",
            "start": 526.08,
            "duration": 6.379
        },
        {
            "text": "السرعه والاغرب ان كيف ما يسمع صوت الباب",
            "start": 529.68,
            "duration": 6.599
        },
        {
            "text": "ويختفي طوال الوقت ويفاجئ الضباط بتواجده",
            "start": 532.459,
            "duration": 6.581
        },
        {
            "text": "اثناء صلاه الفجر حاولوا الضباط انه",
            "start": 536.279,
            "duration": 6.24
        },
        {
            "text": "يشوفون اي مخارج ثانيه حول البيت لكن ما",
            "start": 539.04,
            "duration": 6.359
        },
        {
            "text": "لقوا اي مخرج فازداد الموضوع غرابه بشكل",
            "start": 542.519,
            "duration": 6.241
        },
        {
            "text": "اكثر فامر الظابط احمد عبد العزيز جميع",
            "start": 545.399,
            "duration": 6.961
        },
        {
            "text": "الضباط بانه ما حد فيهم يتحدث مع الشيخ",
            "start": 548.76,
            "duration": 7.98
        },
        {
            "text": "عبد الله فاضل الى ان نكتشف حقيقته وفي 22",
            "start": 552.36,
            "duration": 9.419
        },
        {
            "text": "مايو عام 1948 اتخذ مجلس الامن قرار يدعو",
            "start": 556.74,
            "duration": 8.34
        },
        {
            "text": "فيه اطراف الحرب في فلسطين بوقف القتال",
            "start": 561.779,
            "duration": 7.021
        },
        {
            "text": "خلال 36 ساعه طبعا العرب رفضوا هذه الهدنه",
            "start": 565.08,
            "duration": 7.56
        },
        {
            "text": "خوف الجيوش النظاميه وخوفا من استغلال",
            "start": 568.8,
            "duration": 7.02
        },
        {
            "text": "اليهود للفرصه لكن مجلس الامن ضغط بشكل",
            "start": 572.64,
            "duration": 6.54
        },
        {
            "text": "كبير واقر بقرار وقف الحرب لمده اربع",
            "start": 575.82,
            "duration": 5.88
        },
        {
            "text": "اسابيع في هذه الفتره طلبت القوات",
            "start": 579.18,
            "duration": 5.46
        },
        {
            "text": "اليهوديه بعض الادوات الطبيه والاسعافات",
            "start": 581.7,
            "duration": 6.48
        },
        {
            "text": "الاوليه لعلاج الضابط اليهودي اللي كان",
            "start": 584.64,
            "duration": 6.84
        },
        {
            "text": "مصاب باصابه خطيره في الرقبه وافق قائد",
            "start": 588.18,
            "duration": 6.599
        },
        {
            "text": "الكتيبه المصريه انه يرسل طبيب مصري لعلاج",
            "start": 591.48,
            "duration": 6.479
        },
        {
            "text": "الضابط اليهودي وهذا الامر كان متعارف في",
            "start": 594.779,
            "duration": 6.721
        },
        {
            "text": "هدنه الحروب لكن للعلم هذه الموافقه اتت",
            "start": 597.959,
            "duration": 6.841
        },
        {
            "text": "من منبع المصلحه لان قائد الكتيبه طلب من",
            "start": 601.5,
            "duration": 6.54
        },
        {
            "text": "الطبيب المصري انه يجمع اكبر عدد من",
            "start": 604.8,
            "duration": 6.78
        },
        {
            "text": "المعلومات اثناء دخوله وخروجه من المعسكر",
            "start": 608.04,
            "duration": 7.02
        },
        {
            "text": "اليهودي وبالفعل وصل الطبيب المصري معسكر",
            "start": 611.58,
            "duration": 6.78
        },
        {
            "text": "اليهود في الليل واول ما دخل المعسكر وهو",
            "start": 615.06,
            "duration": 6.06
        },
        {
            "text": "ماشي باتجاه الخيمه اللي فيها الضابط",
            "start": 618.36,
            "duration": 5.7
        },
        {
            "text": "المصاب شافه بعينه الشيخ عبد الله فاضل",
            "start": 621.12,
            "duration": 6.24
        },
        {
            "text": "وهو يتجول ما بين المخيمات في المعسكر",
            "start": 624.06,
            "duration": 7.2
        },
        {
            "text": "اليهودي ولسوء حظ يه انه كان في نوبته",
            "start": 627.36,
            "duration": 6.9
        },
        {
            "text": "الليليه لنقل المعلومات عن القوه المصريه",
            "start": 631.26,
            "duration": 6.72
        },
        {
            "text": "الى المعسكر اليهودي الطبيب المصري تجاهل",
            "start": 634.26,
            "duration": 7.079
        },
        {
            "text": "تماما وعمل نفسه كانه مشغول خلص مهمته",
            "start": 637.98,
            "duration": 6.96
        },
        {
            "text": "بشكل كامل ورجع للمعسكر المصري وعلى طول",
            "start": 641.339,
            "duration": 6.961
        },
        {
            "text": "ابلغ القائد احمد عبد العزيز بكل ما حصل",
            "start": 644.94,
            "duration": 6.48
        },
        {
            "text": "فقال الضابط احمد عبد العزيز خلاص ننتظره",
            "start": 648.3,
            "duration": 6.24
        },
        {
            "text": "في الفجر وبعد ما يصلي فينا نحقق معاه",
            "start": 651.42,
            "duration": 6.24
        },
        {
            "text": "ونقبض عليه طبعا مثل ما قلت لكم يهوذا",
            "start": 654.54,
            "duration": 6.84
        },
        {
            "text": "فاضل او الشيخ عبد الله جدا ذكي فعرف انه",
            "start": 657.66,
            "duration": 7.679
        },
        {
            "text": "تم اكتشاف امره فقرر انه يختفي وما يرجع",
            "start": 661.38,
            "duration": 7.139
        },
        {
            "text": "للمعسكر المصري فاستقر في معسكر الصهاينه",
            "start": 665.339,
            "duration": 7.081
        },
        {
            "text": "ولم يتحرك وانقطع خبره على المصريين طبعا",
            "start": 668.519,
            "duration": 7.201
        },
        {
            "text": "بهاللحظه تاكدوا الضباط المصريين ان يهوذا",
            "start": 672.42,
            "duration": 7.26
        },
        {
            "text": "فاضل هو عباره عن عميل للصهاينه فقرروا",
            "start": 675.72,
            "duration": 6.54
        },
        {
            "text": "انهم يروحون الى بيت الشيخ عبد الله فاضل",
            "start": 679.68,
            "duration": 6.24
        },
        {
            "text": "متسللين عشان يعرفون طريقه يهوذا في ذهابه",
            "start": 682.26,
            "duration": 7.019
        },
        {
            "text": "الى معسكر اليهود وطبعا متسللين لانه ممكن",
            "start": 685.92,
            "duration": 6.32
        },
        {
            "text": "البيت يكون مراقب من اليهود فادخلوه البيت",
            "start": 689.279,
            "duration": 6.961
        },
        {
            "text": "والصدمه انك تشفوا انه هناك نفق داخل",
            "start": 692.24,
            "duration": 7.719
        },
        {
            "text": "البيت يؤدي الى معسكر اليهود وتبين فيما",
            "start": 696.24,
            "duration": 6.96
        },
        {
            "text": "بعد ان اليهود اشتروا هذا البيت ليهوذا",
            "start": 699.959,
            "duration": 8.161
        },
        {
            "text": "لغرض الجاسوسيه بكل جراه تطوع احد الضباط",
            "start": 703.2,
            "duration": 8.639
        },
        {
            "text": "المصريين وقال للقائد احمد عبد العزيز انا",
            "start": 708.12,
            "duration": 6.779
        },
        {
            "text": "راح اجيب يهوذا من معسكر اليهود ابيكم",
            "start": 711.839,
            "duration": 6.12
        },
        {
            "text": "تتخيلون معاي هذه المهمه المستحيله بان",
            "start": 714.899,
            "duration": 6.781
        },
        {
            "text": "شخص يتسلل الى معسكر العدو ويخطف منهم",
            "start": 717.959,
            "duration": 7.741
        },
        {
            "text": "عميلي وفعلا في نفس اليوم ليلا تسلل هذا",
            "start": 721.68,
            "duration": 7.2
        },
        {
            "text": "الضابط من نفس النفق الى معسكر اليهود في",
            "start": 725.7,
            "duration": 6.72
        },
        {
            "text": "عمليه سريه بمساعده احد الجنود فهل يراقب",
            "start": 728.88,
            "duration": 7.38
        },
        {
            "text": "المعسكر حتى شاف يهوذا فاضل موجود بروحه",
            "start": 732.42,
            "duration": 6.9
        },
        {
            "text": "في المخيم فاخطفوه وكمموا وفمه عشان ما",
            "start": 736.26,
            "duration": 6.24
        },
        {
            "text": "يصيح وينبه اليهود وعلى طول بسرعه رجعوه",
            "start": 739.32,
            "duration": 5.759
        },
        {
            "text": "الى المعسكر المصري اول ما وصلوا تم",
            "start": 742.5,
            "duration": 5.1
        },
        {
            "text": "التحقيق معاه وبعدها ارسلوا للمحاكمه",
            "start": 745.079,
            "duration": 6.721
        },
        {
            "text": "العسكريه امام الضباط تصدر الحكم باعدامه",
            "start": 747.6,
            "duration": 7.2
        },
        {
            "text": "رميا بالرصاص ونفذوا الحكم بعد اصدار",
            "start": 751.8,
            "duration": 6.479
        },
        {
            "text": "القرار فورا بعدها رموا جثته امام معسكر",
            "start": 754.8,
            "duration": 7.08
        },
        {
            "text": "الصهاينه ليكون عبره لبقيه العملاء هنا",
            "start": 758.279,
            "duration": 7.981
        },
        {
            "text": "رحل يهوذا الذي تستر بعمامه الدين وكذلك",
            "start": 761.88,
            "duration": 8.579
        },
        {
            "text": "كوهين ببدلات السياسيه والاخطر منهم هو",
            "start": 766.26,
            "duration": 7.46
        },
        {
            "text": "الخائن لوطنه الذي يتستر",
            "start": 770.459,
            "duration": 7.141
        },
        {
            "text": "بعباءه الوطنيه فما اجمل ان يكون للانسان",
            "start": 773.72,
            "duration": 8.559
        },
        {
            "text": "وطن يستقر فيه ويعتز بالانتساب اليه ولذلك",
            "start": 777.6,
            "duration": 9.14
        },
        {
            "text": "حب الوطن من الايمان",
            "start": 782.279,
            "duration": 4.461
        },
        {
            "text": "[موسيقى]",
            "start": 788.74,
            "duration": 5.5
        },
        {
            "text": "[موسيقى]",
            "start": 797.49,
            "duration": 9.83
        }
    ]
}