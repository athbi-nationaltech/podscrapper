        },
        {
            "text": "للقدس فهذا الرجل لعب دور كبير في هذه",
            "start": 412.8,
            "duration": 6.079
        },
        {
            "text": "الفتره بتوجيه الفلسطينيين فكان يدعوهم",
            "start": 415.919,
            "duration": 6.72
        },
        {
            "text": "بالت صدي لليهود ورفض وعد بالفور ويدعوهم",
            "start": 418.879,
            "duration": 6.521
        },
        {
            "text": "للمطالبه بالاستقلال التام عن الانتداب",
            "start": 422.639,
            "duration": 5.68
        },
        {
            "text": "البريطاني هذا الموقف من امين الحسيني",
            "start": 425.4,
            "duration": 6.56
        },
        {
            "text": "كانت تكلفته كبيره جدا فقررت الحكومه",
            "start": 428.319,
            "duration": 7.961
        },
        {
            "text": "البريطانيه بسجنه لمده عشر سنوات فاتجه",
            "start": 431.96,
            "duration": 7.56
        },
        {
            "text": "الى الاردن لكي لا يضطر لتنفيذ الحكم كانت",
            "start": 436.28,
            "duration": 6.479
        },
        {
            "text": "نتيجه هذا الصراع الممتد لايام بوجود",
            "start": 439.52,
            "duration": 7.76
        },
        {
            "text": "شهداء وجرحى فلسطينيين وكذلك قتلى وجرحى",
            "start": 442.759,
            "duration": 7.28
        },
        {
            "text": "يهود هذا الصراع اللي صار في موسم النبي",
            "start": 447.28,
            "duration": 6.44
        },
        {
            "text": "موسى يعتبر اول صراع مباشر ما بين",
            "start": 450.039,
            "duration": 6.641
        },
        {
            "text": "الفلسطينيين والبريطانيين وافتتح المجال",
            "start": 453.72,
            "duration": 6.44
        },
        {
            "text": "لجميع الانتفاضات بعده مثل الثوره العربيه",
            "start": 456.68,
            "duration": 5.6
        },
        {
            "text": "الكبرى اللي حصلت في عام",
            "start": 460.16,
            "duration": 5.8
        },
        {
            "text": "1936 فالسؤال شنو قصه هذه الثوره وما",
            "start": 462.28,
            "duration": 6.759
        },
        {
            "text": "علاقته بالاحداث اللي حصلت في موسم النبي",
            "start": 465.96,
            "duration": 6.799
        },
        {
            "text": "موسى بعد انتفاضه النبي موسى عام 1920",
            "start": 469.039,
            "duration": 7.28
        },
        {
            "text": "استمر الفلسطينيين بمواجهه عمليات التهجير",
            "start": 472.759,
            "duration": 6.521
        },
        {
            "text": "والاستيطان والتاريخ سجل انتفاضات واح",
            "start": 476.319,
            "duration": 6.121
        },
        {
            "text": "احداث كثيره حصلت بعد انتفاضه موسم النبي",
            "start": 479.28,
            "duration": 7.12
        },
        {
            "text": "موسى عام 1920 مثل ثوره يافا عام",
            "start": 482.44,
            "duration": 6.8
        },
        {
            "text": "1921 ثوره البراق عام",
            "start": 486.4,
            "duration": 5.639
        },
        {
            "text": "1929 وانتفاضه عام",
            "start": 489.24,
            "duration": 7.799
        },
        {
            "text": "1933 وفي عام 1936 احدثت انتفاضه كذلك في",
            "start": 492.039,
            "duration": 7.681
        },
        {
            "text": "موسم النبي موسى ولاهميتها الكبيره",
            "start": 497.039,
            "duration": 6.6
        },
        {
            "text": "وتاثيرها الضخم سجلها التاريخ باسم الثوره",
            "start": 499.72,
            "duration": 7.36
        },
        {
            "text": "الكبرى كانت الناس تردد الاهازيج الثوريه",
            "start": 503.639,
            "duration": 6.921
        },
        {
            "text": "والقصائد الدينيه كعاده خلال موسم النبي",
            "start": 507.08,
            "duration": 6.36
        },
        {
            "text": "موسى هذه الاجواء كانت تحفز الفلسطينيين",
            "start": 510.56,
            "duration": 4.959
        },
        {
            "text": "على المقاومه بشكل اكبر فقرروا",
            "start": 513.44,
            "duration": 4.8
        },
        {
            "text": "الفلسطينيين بالاضراب التام عن العمل",
            "start": 515.519,
            "duration": 5.841
        },
        {
            "text": "والتزموا بهذا القرار لمده سته اشهر",
            "start": 518.24,
            "duration": 6.4
        },
        {
            "text": "احتجاجا على سياسه بريطانيا بهجره اليهود",
            "start": 521.36,
            "duration": 5.919
        },
        {
            "text": "ولم يكتفي الناس بالاضراب ل تعبيرهم عن",
            "start": 524.64,
            "duration": 6.36
        },
        {
            "text": "الغضب فثاروا في 7 مايو عام",
            "start": 527.279,
            "duration": 7.761
        },
        {
            "text": "1936 ضد بريطانيا العظمى وقرر عبد القادر",
            "start": 531.0,
            "duration": 7.64
        },
        {
            "text": "الحسيني ان يحمل السلاح ويبادر بالهجوم",
            "start": 535.04,
            "duration": 6.32
        },
        {
            "text": "هذا الا اسم مهم في تاريخ فلسطين خلونا",
            "start": 538.64,
            "duration": 5.879
        },
        {
            "text": "نعرف من هو عبد القادر الحسيني عبد القادر",
            "start": 541.36,
            "duration": 6.52
        },
        {
            "text": "الحسيني هو ابن الشيخ موسى كاظم الحسيني",
            "start": 544.519,
            "duration": 6.721
        },
        {
            "text": "اللي كان يسمى بشيخ المجاهدين في فلسطين",
            "start": 547.88,
            "duration": 6.199
        },
        {
            "text": "لانه كان من اوائل الناس اللي اعترضوا على",
            "start": 551.24,
            "duration": 5.48
        },
        {
            "text": "الانتداب البريطاني فكانت له مكانه خاصه",
            "start": 554.079,
            "duration": 5.0
        },
        {
            "text": "بين الفلسطينيين استخدمها في دعوتهم",
            "start": 556.72,
            "duration": 5.76
        },
        {
            "text": "للاحتجاج والتظاهر والمقاومه فسار ابنه",
            "start": 559.079,
            "duration": 7.121
        },
        {
            "text": "عبد القادر الحسيني على خطاه وحمل السلاح",
            "start": 562.48,
            "duration": 7.28
        },
        {
            "text": "واعلن بدايه الثوره وكان يوجد تنسيق بين",
            "start": 566.2,
            "duration": 6.72
        },
        {
            "text": "القياديين فالحسين كان مسؤول عن تنسيق",
            "start": 569.76,
            "duration": 6.24
        },
        {
            "text": "الثوره في القدس وغيره كانوا مسؤولين عن",
            "start": 572.92,
            "duration": 5.96
        },
        {
            "text": "تنسيق الثوره في بقيه المناطق تحولت",
            "start": 576.0,
            "duration": 6.24
        },
        {
            "text": "فلسطين الى ساحه حرب فالفلسطينيون كانوا",
            "start": 578.88,
            "duration": 7.079
        },
        {
            "text": "يواجهون اكبر قوه عظمى في العالم وتميز",
            "start": 582.24,
            "duration": 7.12
        },
        {
            "text": "عبد القادر الحسيني بالخطط المحكمه وبرز",
            "start": 585.959,
            "duration": 7.161
        },
        {
            "text": "اسمه عند الصديق والعدو ومن خططه تعطيل",
            "start": 589.36,
            "duration": 7.08
        },
        {
            "text": "المواصلات بين القرى بحيث تتعطل الحركه",
            "start": 593.12,
            "duration": 6.8
        },
        {
            "text": "مؤقتا ومع استمرار الثوره عبر السنه نوات",
            "start": 596.44,
            "duration": 6.959
        },
        {
            "text": "حققت فلسطين مكاسب كثيره ف الفلسطينيين",
            "start": 599.92,
            "duration": 6.4
        },
        {
            "text": "استعادوا مساحات كبيره في المناطق الجبليه",
            "start": 603.399,
            "duration": 5.481
        },
        {
            "text": "واستبدلوا مؤسسات الانتداب البريطاني",
            "start": 606.32,
            "duration": 6.28
        },
        {
            "text": "بمؤسسات وطنيه مثل المحكمه والبريد في هذا",
            "start": 608.88,
            "duration": 6.6
        },
        {
            "text": "الوقت صرح بن جوريون القائد الصهيوني",
            "start": 612.6,
            "duration": 6.64
        },
        {
            "text": "الشهير ان هذه العمليه تسعى لتدمير",
            "start": 615.48,
            "duration": 6.72
        },
        {
            "text": "الاقتصاد اليهودي بريطانيا بطبيعه الحال",
            "start": 619.24,
            "duration": 6.08
        },
        {
            "text": "لم تقف متفرجه على هذا المشهد بل كانت",
            "start": 622.2,
            "duration": 6.92
        },
        {
            "text": "تعدم كل شخص تبدو عليه علامات المقاومه",
            "start": 625.32,
            "duration": 7.0
        },
        {
            "text": "وكان عبد القادر الحسيني على راس قائمه",
            "start": 629.12,
            "duration": 6.279
        },
        {
            "text": "المطلوبين حاولت بريطانيا قمع المتظاهرين",
            "start": 632.32,
            "duration": 6.84
        },
        {
            "text": "واستخدموا كل السبل الوحشيه لتخويفهم وحسب",
            "start": 635.399,
            "duration": 7.521
        },
        {
            "text": "الارقام الرسميه البريطانيه تم قتل اكثر",
            "start": 639.16,
            "duration": 7.479
        },
        {
            "text": "من 2000 عربي وحسب الاحصاءات الاخرى فعدد",
            "start": 642.92,
            "duration": 7.96
        },
        {
            "text": "القتلى كان اكثر من 5000 قتيل",
            "start": 646.639,
            "duration": 7.841
        },
        {
            "text": "و1700 جريح الثوره العربيه الكبرى لم تكن",
            "start": 650.88,
            "duration": 5.56
        },
        {
            "text": "الاولى او الاخيره في تاريخ النضال",
            "start": 654.48,
            "duration": 5.0
        },
        {
            "text": "الفلسطيني اللي احدثت في موسم النبي موسى",
            "start": 656.44,
            "duration": 5.88
        },
        {
            "text": "استرجعوا فيها الفلسطينيين بعض المناطق عن",
            "start": 659.48,
            "duration": 6.0
        },
        {
            "text": "طريق الاضراب عن العمل او مواجهه من يسعى",
            "start": 662.32,
            "duration": 5.8
        },
        {
            "text": "للاستيطان والاحتلال البريطاني مر",
            "start": 665.48,
            "duration": 6.28
        },
        {
            "text": "بالاحياء الفلسطينيه واغلق المدارس وسجن",
            "start": 668.12,
            "duration": 7.04
        },
        {
            "text": "الالاف من البشر وكذلك عززت بدعمها",
            "start": 671.76,
            "duration": 7.16
        },
        {
            "text": "المنظمات الصهيونيه مثل منظمه الهاجانا",
            "start": 675.16,
            "duration": 7.52
        },
        {
            "text": "فمدت بالسلاح واللباس والاموال وهذه",
            "start": 678.92,
            "duration": 7.359
        },
        {
            "text": "المنظمات هي من ارتكبت المجازر والابادات",
            "start": 682.68,
            "duration": 7.56
        },
        {
            "text": "بحق القرى الفلسطينيه هذه هي قصه انتفاضه",
            "start": 686.279,
            "duration": 7.841
        },
        {
            "text": "موسم النبي موسى الموسم اللي احدثت فيه",
            "start": 690.24,
            "duration": 6.76
        },
        {
            "text": "اول انتفاضه ضد الاحتلال البريطاني",
            "start": 694.12,
            "duration": 6.24
        },
        {
            "text": "والموسم اللي تكررت فيه عده انتفاضات",
            "start": 697.0,
            "duration": 7.519
        },
        {
            "text": "ليكون شاهد على مقاومه اهل فلسطين للدفاع",
            "start": 700.36,
            "duration": 8.4
        },
        {
            "text": "عن اراضيهم ومقدساتهم لم تنتهي قصه الصمود",
            "start": 704.519,
            "duration": 9.401
        },
        {
            "text": "الفلسطيني وما زالت مستمره الى",
            "start": 708.76,
            "duration": 5.16
        },
        {
            "text": "اليوم",
            "start": 717.959,
            "duration": 3.0
        },
        {
            "text": "[موسيقى]",
            "start": 721.05,
            "duration": 24.84
        },
        {
            "text": "ا",
            "start": 747.88,
            "duration": 3.0
        },
        {
            "text": "[موسيقى]",
            "start": 751.18,
            "duration": 11.449
        },
        {
            "text": "[موسيقى]",
            "start": 772.69,
            "duration": 3.239
        },
        {
            "text": "ا",
            "start": 777.56,
            "duration": 3.0
        }
    ]
