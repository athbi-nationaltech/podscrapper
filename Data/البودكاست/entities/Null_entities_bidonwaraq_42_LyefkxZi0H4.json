"\"Entities\": [  {    \"name\": \"مجلس التعاون الخليجي\",    \"original_text\": \"Gulf Cooperation Council\",    \"type\": \"ORGANIZATION\",    \"description\": \"منظمة إقليمية سياسية واقتصادية تضم ست دول عربية مطلة على الخليج العربي هي: الإمارات العربية المتحدة، البحرين، السعودية، عمان، قطر، الكويت. تأسس المجلس في عام 1981 بهدف تعزيز التعاون والتكامل بين الدول الأعضاء في مختلف المجالات.\"  },  {    \"name\": \"الفالودة\",    \"original_text\": \"falooda\",    \"type\": \"PRODUCT\",    \"description\": \"حلوى باردة ومشهورة في جنوب آسيا والشرق الأوسط، تتكون عادة من مزيج من الحليب، الشعيرية، بذور الريحان، الورد، ومكسرات مثل الفستق.\"  },  {    \"name\": \"مجلس الأمة الكويتي\",    \"original_text\": \"National Assembly\",    \"type\": \"ORGANIZATION\",    \"description\": \"الهيئة التشريعية في الكويت، ويتكون من 50 عضواً ينتخبون بالاقتراع السري المباشر، ومدة العضوية فيه أربع سنوات ميلادية.\"  },  {    \"name\": \"منيرة\",    \"original_text\": \"Muneera\",    \"type\": \"PERSON\",    \"description\": \"يُرجح انها منيرة العجل، مقدمة برنامج بدون ورق\"  },  {    \"name\": \"فيصل العقل\",    \"original_text\": \"Faisal Al-Agel\",    \"type\": \"PERSON\",    \"description\": \"كاتب ومقدم برنامج بدون ورق\"  },  {    \"name\": \"بدون ورق\",    \"original_text\": \"Bidon Waraq\",    \"type\": \"MEDIA\",    \"description\": \"بودكاست سياسي يُناقش قضايا الساعة في الكويت والخليج العربي.\"  },  {    \"name\": \"عبدالله النفيسي\",    \"original_text\": \"Abdullah Al-Nafisi\",    \"type\": \"PERSON\",    \"description\": \"سياسي ومفكر كويتي، شغل منصب أستاذ العلوم السياسية في جامعة الكويت، وله العديد من المؤلفات في الفكر السياسي والعلاقات الدولية.\"  },  {    \"name\": \"إيران\",    \"original_text\": \"Iran\",    \"type\": \"LOCATION\",    \"description\": \"دولة تقع في غرب آسيا، عاصمتها طهران، وتتمتع بثقافة وحضارة غنية تمتد لآلاف السنين.\"  },  {    \"name\": \"تشارلز غلاس\",    \"original_text\": \"Charles Glass\",    \"type\": \"PERSON\",    \"description\": \"صحفي ومؤلف أمريكي ب